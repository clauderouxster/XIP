<project>

<target name="setCommonProperties" depends="platformSetting, getXipVersionAnBuild"
                description="set common properties">
	<!-- lib to use -->
        <property name="lib.dir"        	location="${root.dir}/lib/${platform}" />
	<!-- test bin dir-->
        <property name="test.bin.dir"        	location="${basedir}/bin/${platform}" />
        <mkdir dir="${test.bin.dir}" />
</target>

</project>
