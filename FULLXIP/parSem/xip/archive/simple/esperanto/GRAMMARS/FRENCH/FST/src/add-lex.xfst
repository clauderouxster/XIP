## some definitions for units 
define PREFIX [
  {yocto} 	: y		# = 10^-24
| {zepto} 	: z		# = 10^-21
| {atto}	: a		# = 10^-18
| {femto}	: f		# = 10^-15
| {pico} 	: p		# = 10^-12
| {nano}	: n		# = 10^-9
| {micro}	: "\u00B5"	# = 10^-6	# MICRO SIGN
| {milli} 	: m		# = 10^-3
| {centi}	: c		# = 10^-2
| {deci}	: d		# = 10^-1

| {hecto}	: h		# = 10^2 	  	
| {kilo}	: k		# = 10^3
| {mega}	: M		# = 10^6 	  	
| {giga}	: G		# = 10^9 	  	
| {tera}	: T		# = 10^12 	  	
| {peta}	: P		# = 10^15 	  
| {exa}		: E		# = 10^18 	  	
| {zetta}	: Z		# = 10^21 	  	
| {yotta}	: Y		# = 10^24 	  	

# not standard but used:
| {deka} 	: {da}		# = 10^1  	
| {deka} 	: {dk}		# = 10^1  	
| {kilo}	: K		# = 10^3	## should be only k, but K is often used 
| {micro}	: u		# = 103		## if micro symbol not available ...
];

# abbreviation only:

define PRF	PREFIX.l ;

# superscripts
define S2	[ 2 | "\u00B2"	] ; 		# SUPERSCRIPT TWO
define S3	[ 3 | "\u00B3"	] ;		# SUPERSCRIPT THREE
define M1	[ {-1} | "\u207B"  "\u00B9" ];	#  SUPERSCRIPT MINUS + SUPERSCRIPT ONE

define UNITS (PRF) [
  m S2					# m²		area
| m S3					# m³		volume
] %+Measure:0 %+NOUN%_INV:0 ;

define TEST [
Num+ (PRF) m S2 %+Area:0 %+Measure:0 %+NOUN%_INV:0 |
Num+ (PRF) m S3 %+Vol:0 %+Measure:0 %+NOUN%_INV:0 
] ;


read regex LEX | ADD | PRENOMS | LIEUX | ORGANISATIONS | PERSONNAGES | UNITS | TEST ;

