pour windows:
	installer sur une machine, puis copy c:\WINDOWS\System32\python2.6.dll
	copy c:\Python26\include python/2.6/windows/include        


pour linux:
	choranche
	cd python/2.6/src/Python-2.6.4
	make clean
	./configure --enable-shared
	make
	cp libpython2.6.a libpython2.6.so ../../linux/lib
	cp pyconfig.h ../../linux/include/

pour linux64:
	utiliser mlir5 (et non pas cochette) sinon la lib generee n'est pas compatible avec mlir5
	mlir5
	cd python/2.6/src/Python-2.6.4
	make clean
	./configure --enable-shared
	make
	cp libpython2.6.a libpython2.6.so ../../linux64/lib
	cp pyconfig.h ../../linux64/include/
