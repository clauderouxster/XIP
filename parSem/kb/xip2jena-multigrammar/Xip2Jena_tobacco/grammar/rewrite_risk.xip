DependencyRules:

// What the heck are these good for?  They contain absolutely no information!
if (^COORDINATE(#1,#2[fictivenode,lemme="SOMEONE"]))
  ~.


//////////////////////////////////////
// Entities (named and otherwise)
//////////////////////////////////////

//PERSON --> Person

// Doesn't exist in POLRISK grammar; must be a recent addition to the NE grammar?
//if (ORGANISATION[comp](#1))
//  COMPANY(#1).

// Otherwise ORGANISATION --> Organisation

if (ARGUMENT(#1))
  RISKEVENT(#1).

if (CORE [SAY] (#1))
  SAY(#1).

if (CORE (#1[KNOW]))
  KNOW(#1).

|#1[PRODUCT]|
  PHYSICALARTIFACT(#1).

if (ARGUMENT (#1,#2[SUBSTANCE]))
  PHYSICALSUBSTANCE(#2).

// LOCORG: handled after relations



//////////////////////////////////////
// Relations
//////////////////////////////////////


if (ARGUMENT [SUBJ](#1,#2) & ~RISKEVENT(#2))
  ENTITY(#2),
  HASPARTICIPANT(#1,#2),
  HASACTOR(#1,#2).

if (ARGUMENT [OBJ](#1,#2) & ~RISKEVENT(#2))
  ENTITY(#2),
  HASPARTICIPANT(#1,#2),
  HASTARGET(#1,#2).

if (COORDINATE [LOC-PR] (#1,#2) & COORDINATE[target](#1,#2))
    LOCATION(#2),
    HASLOCATION(#1,#2).

if (EMBED [COMPLTHAT] (#1,#2))
  RISKEVENT(#1),
  REFERSTO (#2,#1).

if (EMBED (#1,#2) & ~EMBED [COMPLTHAT] (#1,#2))
  RISKEVENT(#1),
  ISRELATIVETO(#2,#1).



// LOCORG is ambiguous between Location and Organization.  Ideally we
// would put in a disjunction, and let the reasoner disambiguate via
// inference if possible.
if (LOCORG [CITY] (#1))
    CITY(#1).

if (LOCORG [COUNTRY] (#1))
    COUNTRY(#1).

if (LOCORG(#1) & ~LOCATION(#1))
  GOVERNMENT(#1).


if (ROLE (#1,#2,#3))
  PROFESSION (#2),
  ENTITY (#1),
  ENTITY (#3),
  WORKSFOR(##fict#4),
  HASPARTICIPANT(#4,#3),
  HASACTOR(#4,#3),
  HASPARTICIPANT(#4,#1),
  HASTARGET(#4,#1),
  HASPROFESSION (#3,#2).


