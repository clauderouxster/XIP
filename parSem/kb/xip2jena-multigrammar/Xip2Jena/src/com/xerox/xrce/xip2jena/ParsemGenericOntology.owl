<?xml version="1.0"?>
<rdf:RDF
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrlx="http://swrl.stanford.edu/ontologies/built-ins/3.3/swrlx.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
  xml:base="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="PersonNameAtom">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Entity"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Agent"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Mention"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="ProfessionalTitle"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="FirstName">
    <rdfs:subClassOf rdf:resource="#PersonNameAtom"/>
  </owl:Class>
  <owl:Class rdf:about="#Mention">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The mention in the original text</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <owl:disjointWith>
      <owl:Class rdf:ID="Name"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#PersonNameAtom"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Agent"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#ProfessionalTitle"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="MiddleName">
    <rdfs:subClassOf rdf:resource="#PersonNameAtom"/>
  </owl:Class>
  <owl:Class rdf:ID="Organisation">
    <owl:disjointWith>
      <owl:Class rdf:ID="Person"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Agent"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Government">
    <rdfs:subClassOf rdf:resource="#Organisation"/>
  </owl:Class>
  <owl:Class rdf:about="#Person">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Agent"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="hasGender"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <owl:Class rdf:ID="Gender"/>
            </owl:someValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:allValuesFrom>
              <owl:Class rdf:about="#Gender"/>
            </owl:allValuesFrom>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#hasGender"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointWith rdf:resource="#Organisation"/>
  </owl:Class>
  <owl:Class rdf:ID="Company">
    <rdfs:subClassOf rdf:resource="#Organisation"/>
  </owl:Class>
  <owl:Class rdf:about="#Name">
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Agent"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Mention"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#ProfessionalTitle"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#ProfessionalTitle">
    <owl:disjointWith rdf:resource="#Mention"/>
    <owl:disjointWith rdf:resource="#Name"/>
    <owl:disjointWith rdf:resource="#PersonNameAtom"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Agent"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#Entity"/>
  </owl:Class>
  <owl:Class rdf:ID="PersonName">
    <rdfs:subClassOf rdf:resource="#Name"/>
  </owl:Class>
  <owl:Class rdf:ID="Female">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Gender"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Male"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#Gender">
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Male"/>
          <owl:Class rdf:about="#Female"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="LastName">
    <rdfs:subClassOf rdf:resource="#PersonNameAtom"/>
  </owl:Class>
  <owl:Class rdf:about="#Agent">
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <owl:disjointWith rdf:resource="#Name"/>
    <owl:disjointWith rdf:resource="#PersonNameAtom"/>
    <owl:disjointWith rdf:resource="#Mention"/>
    <owl:disjointWith rdf:resource="#ProfessionalTitle"/>
  </owl:Class>
  <owl:Class rdf:about="#Male">
    <rdfs:subClassOf rdf:resource="#Gender"/>
    <owl:disjointWith rdf:resource="#Female"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="hasProfessionalTitle">
    <rdfs:range rdf:resource="#ProfessionalTitle"/>
    <rdfs:domain rdf:resource="#Person"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasFirstName">
    <rdfs:domain rdf:resource="#PersonName"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="hasPersonNameAtom"/>
    </rdfs:subPropertyOf>
    <rdfs:range rdf:resource="#FirstName"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasPersonNameAtom">
    <rdfs:range rdf:resource="#PersonNameAtom"/>
    <rdfs:domain rdf:resource="#PersonName"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasMiddleName">
    <rdfs:domain rdf:resource="#PersonName"/>
    <rdfs:range rdf:resource="#MiddleName"/>
    <rdfs:subPropertyOf rdf:resource="#hasPersonNameAtom"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPersonName">
    <rdfs:range rdf:resource="#PersonName"/>
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="hasName"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasMention">
    <rdfs:range rdf:resource="#Mention"/>
    <rdfs:domain rdf:resource="#Entity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasLastName">
    <rdfs:subPropertyOf rdf:resource="#hasPersonNameAtom"/>
    <rdfs:range rdf:resource="#LastName"/>
    <rdfs:domain rdf:resource="#PersonName"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasName">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Agent"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#Name"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="hasUrl">
    <rdfs:domain rdf:resource="#Mention"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasNameForm">
    <rdfs:domain rdf:resource="#Name"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasStart">
    <rdfs:domain rdf:resource="#Mention"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasPersonNameAtomForm">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#PersonNameAtom"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasEnd">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#Mention"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasProfessionForm">
    <rdfs:domain rdf:resource="#ProfessionalTitle"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasForm">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Mention"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="detectedBy">
    <rdfs:domain rdf:resource="#Mention"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasCanonicalForm">
    <rdfs:subPropertyOf rdf:resource="#hasNameForm"/>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:about="#hasGender">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Gender"/>
    <rdfs:domain rdf:resource="#Person"/>
  </owl:FunctionalProperty>
</rdf:RDF>

