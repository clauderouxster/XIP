<rdf:RDF
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
xml:base="http://www.xrce.xerox.com/mappings.owl">

  <owl:Ontology rdf:about="">
    <rdfs:comment>Matched ontologies</rdfs:comment>
    <owl:imports rdf:resource="http://www.xrce.xerox.com/parsem/RiskOntology.owl"/>
    <owl:imports rdf:resource="http://www.loa-cnr.it/ontologies/CLO/CoreLegal.owl"/>
  </owl:Ontology>

  <owl:Class rdf:about="http://www.xrce.xerox.com/parsem/RiskOntology.owl#Government">
    <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/CLO/CoreLegal.owl#LegallyConstructedInstitution"/>
  </owl:Class>

  <owl:Class rdf:about="http://www.xrce.xerox.com/parsem/RiskOntology.owl#Organisation">
    <owl:equivalentClass rdf:resource="http://www.loa-cnr.it/ontologies/CLO/CoreLegal.owl#Organization"/>
  </owl:Class>

  <owl:Class rdf:about="http://www.xrce.xerox.com/parsem/RiskOntology.owl#Person">
    <owl:equivalentClass rdf:resource="http://www.loa-cnr.it/ontologies/CLO/CoreLegal.owl#Person"/>
  </owl:Class>

</rdf:RDF>
