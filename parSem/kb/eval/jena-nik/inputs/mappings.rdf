    <prefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string">coreLegal</prefix>
  </OntologySpec>

  <OntologySpec>
    <publicURI rdf:resource="http://www.xrce.xerox.com/parsem/RiskOntology.owl"/>
    <altURL rdf:resource="RiskOntology.owl"/>
    <language rdf:resource="http://www.w3.org/2002/07/owl"/>
    <prefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string">riskOntology</prefix>
  </OntologySpec>

  <OntologySpec>
    <publicURI rdf:resource="http://www.xrce.xerox.com/mappings.owl"/>
    <altURL rdf:resource="Multiple_mappings_Risk_CoreLegal.owl"/>
    <language rdf:resource="http://www.w3.org/2002/07/owl"/>
    <prefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mappingsOntology</prefix>
  </OntologySpec>

</rdf:RDF>
