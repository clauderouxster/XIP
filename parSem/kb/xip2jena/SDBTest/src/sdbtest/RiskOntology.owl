<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:swrlx="http://swrl.stanford.edu/ontologies/built-ins/3.3/swrlx.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:parsem="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl#"
    xmlns="http://www.xrce.xerox.com/parsem/RiskOntology.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.xrce.xerox.com/parsem/RiskOntology.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="RDF-Statement">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="Factuality"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasFactuality"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasTime"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="Time"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Day">
    <owl:disjointWith>
      <owl:Class rdf:ID="Year"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Month"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Hour"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Minute"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Second"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="TimeAtom"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Organisation">
    <owl:disjointWith>
      <owl:Class rdf:ID="Person"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Agent"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#TimeAtom">
    <owl:disjointWith>
      <owl:Class rdf:about="#Time"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="source"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Location"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Function"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Event"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Agent"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl#Entity"/>
  </owl:Class>
  <owl:Class rdf:ID="City">
    <owl:disjointWith>
      <owl:Class rdf:ID="Country"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Location"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Factuality">
    <rdfs:subClassOf rdf:resource="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl#Entity"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:oneOf rdf:parseType="Collection">
          <Factuality rdf:ID="factual"/>
          <Factuality rdf:ID="counterFactual"/>
          <Factuality rdf:ID="possible"/>
        </owl:oneOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Mention">
    <rdfs:subClassOf rdf:resource="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl#Entity"/>
  </owl:Class>
  <owl:Class rdf:ID="Government">
    <rdfs:subClassOf rdf:resource="#Organisation"/>
  </owl:Class>
  <owl:Class rdf:about="#Time">
    <owl:disjointWith rdf:resource="#TimeAtom"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#source"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Location"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Function"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Event"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Agent"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl#Entity"/>
  </owl:Class>
  <owl:Class rdf:about="#source">
    <owl:disjointWith rdf:resource="#TimeAtom"/>
    <owl:disjointWith rdf:resource="#Time"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Location"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Function"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Event"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Agent"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl#Entity"/>
  </owl:Class>
  <owl:Class rdf:about="#Second">
    <owl:disjointWith>
      <owl:Class rdf:about="#Year"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Month"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Day"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Hour"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Minute"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#TimeAtom"/>
  </owl:Class>
  <owl:Class rdf:about="#Person">
    <owl:disjointWith rdf:resource="#Organisation"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:allValuesFrom>
              <owl:Class rdf:ID="Gender"/>
            </owl:allValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="hasGender"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:someValuesFrom>
              <owl:Class rdf:about="#Gender"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasGender"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Agent"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="CorpusSource">
    <rdfs:subClassOf rdf:resource="#source"/>
    <owl:disjointWith>
      <owl:Class rdf:ID="ManualSource"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="SimpleTime">
    <rdfs:subClassOf rdf:resource="#Time"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#TimeAtom"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasTimeAtom"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="TimeInterval"/>
    </owl:disjointWith>
    <rdfs:comment xml:lang="en">We use the GMT time zone.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Month">
    <rdfs:subClassOf rdf:resource="#TimeAtom"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Year"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Day"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Hour"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Minute"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Second"/>
  </owl:Class>
  <owl:Class rdf:about="#Agent">
    <owl:disjointWith rdf:resource="#source"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Event"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#TimeAtom"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Function"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Time"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#Function"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasFunction"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#Location"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasLocation"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl#Entity"/>
  </owl:Class>
  <owl:Class rdf:ID="Company">
    <rdfs:subClassOf rdf:resource="#Organisation"/>
  </owl:Class>
  <owl:Class rdf:about="#TimeInterval">
    <rdfs:subClassOf rdf:resource="#Time"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="hasEndTime"/>
            </owl:onProperty>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
          </owl:Restriction>
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="hasStartTime"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty rdf:resource="#hasStartTime"/>
            <owl:allValuesFrom rdf:resource="#SimpleTime"/>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty rdf:resource="#hasEndTime"/>
            <owl:allValuesFrom rdf:resource="#SimpleTime"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointWith rdf:resource="#SimpleTime"/>
  </owl:Class>
  <owl:Class rdf:about="#Location">
    <owl:disjointWith rdf:resource="#TimeAtom"/>
    <owl:disjointWith rdf:resource="#Time"/>
    <owl:disjointWith rdf:resource="#source"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Function"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Event"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl#Entity"/>
  </owl:Class>
  <owl:Class rdf:about="#Event">
    <owl:disjointWith rdf:resource="#Time"/>
    <owl:disjointWith rdf:resource="#Agent"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasLocation"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#Location"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#Function"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl#Entity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasTime"/>
        <owl:allValuesFrom rdf:resource="#Time"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#Location"/>
    <owl:disjointWith rdf:resource="#source"/>
    <owl:disjointWith rdf:resource="#TimeAtom"/>
  </owl:Class>
  <owl:Class rdf:about="#Year">
    <rdfs:subClassOf rdf:resource="#TimeAtom"/>
    <owl:disjointWith rdf:resource="#Month"/>
    <owl:disjointWith rdf:resource="#Day"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Hour"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Minute"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Second"/>
  </owl:Class>
  <owl:Class rdf:about="#Hour">
    <rdfs:subClassOf rdf:resource="#TimeAtom"/>
    <owl:disjointWith rdf:resource="#Year"/>
    <owl:disjointWith rdf:resource="#Month"/>
    <owl:disjointWith rdf:resource="#Day"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Minute"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Second"/>
  </owl:Class>
  <owl:Class rdf:ID="PersonName">
    <rdfs:subClassOf rdf:resource="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl#Entity"/>
  </owl:Class>
  <owl:Class rdf:about="#Function">
    <rdfs:subClassOf rdf:resource="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl#Entity"/>
    <owl:disjointWith rdf:resource="#TimeAtom"/>
    <owl:disjointWith rdf:resource="#Time"/>
    <owl:disjointWith rdf:resource="#source"/>
    <owl:disjointWith rdf:resource="#Location"/>
    <owl:disjointWith rdf:resource="#Event"/>
    <owl:disjointWith rdf:resource="#Agent"/>
  </owl:Class>
  <owl:Class rdf:about="#Minute">
    <rdfs:subClassOf rdf:resource="#TimeAtom"/>
    <owl:disjointWith rdf:resource="#Year"/>
    <owl:disjointWith rdf:resource="#Month"/>
    <owl:disjointWith rdf:resource="#Day"/>
    <owl:disjointWith rdf:resource="#Hour"/>
    <owl:disjointWith rdf:resource="#Second"/>
  </owl:Class>
  <owl:Class rdf:about="#Gender">
    <rdfs:subClassOf rdf:resource="http://www.xrce.xerox.com/parsem/ParsemGenericOntology.owl#Entity"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:oneOf rdf:parseType="Collection">
          <Gender rdf:ID="male"/>
          <Gender rdf:ID="female"/>
        </owl:oneOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#Country">
    <rdfs:subClassOf rdf:resource="#Location"/>
    <owl:disjointWith rdf:resource="#City"/>
  </owl:Class>
  <owl:Class rdf:about="#ManualSource">
    <owl:disjointWith rdf:resource="#CorpusSource"/>
    <rdfs:subClassOf rdf:resource="#source"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2003/11/swrl#argument2"/>
  <owl:ObjectProperty rdf:ID="hasHour">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hasTimeAtom"/>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="#SimpleTime"/>
    <rdfs:range rdf:resource="#Hour"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasGender">
    <rdfs:range rdf:resource="#Gender"/>
    <rdfs:domain rdf:resource="#Person"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPersonAtomName">
    <rdfs:domain rdf:resource="#PersonName"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasMinute">
    <rdfs:domain rdf:resource="#SimpleTime"/>
    <rdfs:range rdf:resource="#Minute"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hasTimeAtom"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasMention">
    <rdfs:range rdf:resource="#Mention"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasName">
    <rdfs:range rdf:resource="#PersonName"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasLastName">
    <rdfs:subPropertyOf rdf:resource="#hasPersonAtomName"/>
    <rdfs:domain rdf:resource="#PersonName"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasGovernment">
    <rdfs:domain rdf:resource="#Location"/>
    <rdfs:range rdf:resource="#Government"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasFirstName">
    <rdfs:domain rdf:resource="#PersonName"/>
    <rdfs:subPropertyOf rdf:resource="#hasPersonAtomName"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasCore">
    <rdfs:domain rdf:resource="#Event"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasFunction">
    <rdfs:domain rdf:resource="#Agent"/>
    <rdfs:range rdf:resource="#Function"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasYear">
    <rdfs:domain rdf:resource="#SimpleTime"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hasTimeAtom"/>
    </rdfs:subPropertyOf>
    <rdfs:range rdf:resource="#Year"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasDay">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hasTimeAtom"/>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="#SimpleTime"/>
    <rdfs:range rdf:resource="#Day"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasSecond">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hasTimeAtom"/>
    </rdfs:subPropertyOf>
    <rdfs:range rdf:resource="#Second"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasTitle">
    <rdfs:subPropertyOf rdf:resource="#hasPersonAtomName"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasSource">
    <rdfs:domain rdf:resource="#RDF-Statement"/>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#ManualSource"/>
          <owl:Class rdf:about="#CorpusSource"/>
          <owl:Class rdf:about="#Agent"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasMiddleName">
    <rdfs:domain rdf:resource="#PersonName"/>
    <rdfs:subPropertyOf rdf:resource="#hasPersonAtomName"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasMonth">
    <rdfs:range rdf:resource="#Month"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hasTimeAtom"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasParticipant">
    <rdfs:domain rdf:resource="#Event"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasCoref">
    <score rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></score>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasLocation">
    <rdfs:domain rdf:resource="#Event"/>
    <rdfs:range rdf:resource="#Location"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasTimeAtom">
    <rdfs:domain rdf:resource="#SimpleTime"/>
    <rdfs:range rdf:resource="#TimeAtom"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="score"/>
  <owl:DatatypeProperty rdf:ID="start">
    <rdfs:domain rdf:resource="#Mention"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="address"/>
  <owl:DatatypeProperty rdf:ID="absolute_value"/>
  <owl:DatatypeProperty rdf:ID="end">
    <rdfs:domain rdf:resource="#Mention"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="id"/>
  <owl:DatatypeProperty rdf:ID="subtype"/>
  <owl:DatatypeProperty rdf:ID="form"/>
</rdf:RDF>

