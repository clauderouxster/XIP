<?xml version="1.0" encoding="UTF-8"?>
<taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>eu.sapir.contentmanagement.text.SapirXipAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>SapirXipAnnotator</name>
    <description>Uses the XIP engine to do tokenization, morphological
analysis, and named entity recognition for English text.

Copyright 2000-2007 Xerox Research Centre Europe</description>
    <version>0.001</version>
    <vendor>Xerox Research Centre Europe</vendor>
    <configurationParameters searchStrategy="language_fallback"/>
    <configurationParameterSettings/>
    <typeSystemDescription>
      <imports>
        <import location="SapirTextTypes.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported>
          <language>en</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>false</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</taeDescription>
