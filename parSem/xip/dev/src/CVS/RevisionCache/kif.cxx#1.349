	if (idx==NULL || idx==kifNULL) {
		KifElement* kv;
		//particular case, the contextualpattern is a vector, which means that we expect a set of keys
		//as a result
		if (contextualpattern->type==kifVector) {
			ThreadLock _lock(type);
