	if (gotoline==-10) {
		cout<<"h for help"<<Endl;
		gotoline=-1;
	}

	long cline=tabledebugline[ke->Line()];

	if (breakpoint==cline)
		debug=true;
	else
		if (gotoline==cline) {
			debug=true;
			gotoline=-1;
		}


	if (debug==false)
		return true;

	infunction=false;


	string s;
	if (tabledebug.find(ke->Line())!=tabledebug.end()) {
		s=tabledebug[ke->Line()];
