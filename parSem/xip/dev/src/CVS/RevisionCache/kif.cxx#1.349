		cout<<"["<<STR((*kifIdString)[func->Name()])<<"]"<<cline<<"> "<<STR(s)<<Endl;
	else
		cout<<cline<<"> "<<STR(s)<<Endl;

	s="x";
	int compte;
	KifElement* v;
	int j;
	short id;
	while (s!="") {

		if (watches.size()) {
			for (j=0;j<watches.size();j++) {
				id=KifNewId(watches[j]);
