KifVector* KifCode::Providevector() {
	ThreadLock _lock(kifVector);
	int id=GetThreadid();
	Used(id,kifVectorOwner);
	int i=cursorvector;
	KifVector* kvect;

	for (;cursorvector<vectorlist.size();cursorvector++) {
		kvect=vectorlist[cursorvector];
