KifElement* KifString::Map(KifElement* dom) {
	ThreadLock _lock(type);
	string smap=value;
	kifcode->Push((KifDomain*)dom);
	KifElement* ke=kifcode->EvaluateMap(dom,smap);
	kifcode->Pop();
	if (ke->type!=kifMap)
		return kifNULL;
	ke=ke->Execute(kifNULL,(KifDomain*)dom);
	return ke;
}


