//Conversion from string to double...
double convertfloat(char* s) {
	long v=0;
	long point=0;
	int i=0;
	long mantissa=0;
	while (s[i]!=0) {
		if (s[i]=='.') {
			mantissa=1;
			i++;
			continue;
		}
		if (s[i]<48 || s[i]>58) {
			i++;
			continue;
		}
		if (!mantissa) {
			v*=10;
			v+=s[i]-48;
		}
		else {
			point*=10;
			point+=s[i]-48;
			mantissa*=10;
		}
		i++;
	}
	double d=v;
	d+= ((double)point)/mantissa;
	return d;
}
		
		
