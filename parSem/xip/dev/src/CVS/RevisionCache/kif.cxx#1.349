					mxj=maxline;
					break;
				default:
					ln=atoi(STR(positions[1]));
					mxj=atoi(STR(positions[2]));
				}
				if (mxj>maxline)
					mxj=maxline;
				while (ln<=mxj) {				
					if (tabledebug.find(ln)==tabledebug.end()) {
						ln++;
						continue;
					}
					s=tabledebug[ln];
					cout<<ln<<"> "<<tabledebug[ln]<<Endl;
					ln++;
				}
			}
			else
				if (s!="" && s[0]=='n') {
					cout<<Endl;
					mxj=1;
