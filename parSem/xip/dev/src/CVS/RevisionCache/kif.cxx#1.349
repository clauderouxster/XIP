			attributes[a]=false;
	}

	classes[c]=true;
	KifElement* ke=values[line][c][a];
	if (ke!=kifNULL && ke!=NULL)
		values[line][c][a]->Resetreference();
	else
		nbelements++;
	v->Setreference();
	values[line][c][a]=v;
}


