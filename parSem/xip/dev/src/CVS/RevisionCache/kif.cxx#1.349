void DisplayClean(string& s, char* buffer, int& compte) {
	string blanc;
	compte+=strlen(buffer);
	while(compte>0) {
		cout<<(char)8;
		compte--;
		blanc+=" ";
	}	
	cout<<buffer<<s<<blanc;
	for (int i=0;i<blanc.size();i++)
