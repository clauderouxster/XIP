	KifCallFunction* callfunc=(KifCallFunction*)kifcode->Top();
	if (callfunc==NULL)
		return kifcode->Returnerror(kifErrorStrings[241]);
	if (parameters.size()!=1) {
		callfunc->Setreturnvalue(kifNULL);
