					gotoline=j;
				infunction=true;
				debug=false;
				return true;
			}
		}

		int mxj,ln;
		map<long,string>::iterator mx=--tabledebug.end();
		long maxline=mx->first;
		if (s=="s") {
			cout<<Endl;
			for (j=1;j<stacking.size();j+=2)
