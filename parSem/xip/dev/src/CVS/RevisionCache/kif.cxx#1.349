string& Trim(string& chaine) {
	int d,f;
	for (d=0;d<chaine.size();d++) {
		if ((uchar)chaine[d]>32)
			break;
	}

	for (f=chaine.size()-1;f>=0;f--) {
		if ((uchar)chaine[f]>32)
			break;
	}
	int lg=f-d+1;
	if (lg>=1)
		chaine=chaine.substr(d,lg);
	else
		chaine="";
	return chaine;
}

string& Trimleft(string& chaine) {
	int d,f;
	f=chaine.size()-1;
	for (d=0;d<chaine.size();d++) {
		if ((uchar)chaine[d]>32)
			break;
	}

	int lg=f-d+1;
	if (lg>=1)
		chaine=chaine.substr(d,lg);
	else
		chaine="";
	return chaine;
}

string& Trimright(string& chaine) {
	int d=0,f;

	for (f=chaine.size()-1;f>=0;f--) {
		if ((uchar)chaine[f]>32)
			break;
	}

	int lg=f-d+1;
	if (lg>=1)
		chaine=chaine.substr(d,lg);
	else
		chaine="";
	return chaine;
}

