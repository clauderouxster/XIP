				kmap->Push(n,kv);
				//kv->Setreference();
				//kmap->values[n]=kv;
			}
			return kmap;
		}
		return this;
	}

	KifElement* key;
	if (idx->type==kifIndex) {
		KifIndex* kind=(KifIndex*)idx;		
		if (kind->interval==true)
			return kifNULL;
		key=kind->key->Execute(kifNULL,dom);
