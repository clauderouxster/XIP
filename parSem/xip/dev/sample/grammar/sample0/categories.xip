 aux=[aux=+]
 adj=[adj=+]
 nadj=[nadj=+, adj=+, noun=+]
 prep=[prep=+]
 adv=[adv=+]
 conj=[conj=+]
 num=[num=+] 
 dig=[dig=+]
 det=[det=+,!wh:+!]
 pron=[pron=+] 
 punct = [punct=+] 
 quant = [quant=+]
 part = [part=+]
 meas = [meas=+]
 interj = [interj=+]
 infto = [infto=+]
 
