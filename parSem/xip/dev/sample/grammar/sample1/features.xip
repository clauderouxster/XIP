Features:
// adds two additional features for tagging names/firstnames of persons
[
   man:{+},
   woman:{+}
]


// specifies which features will be displayed in the trees.out file
// the standard ones are displayed - keyword features
// the newly defined ones are displayed as well

Display:
[features, firstname, man, woman]

