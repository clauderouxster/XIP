PartFut	= [partfut=+].	// future participle ??? 	zemdlały::zemdleć+Verb+Perf+Ind+PartFut+M23FN+Pl
Partcl	= [pcl=+].	// particle 	     		samyż::	sam+Pron+M3+Sg+Acc+Partcl
Pass1	= [pass1=+].	// passive 1 (participles) 	bodzeni::bóść+Verb+Imperf+Part+Past+Pass1+M1+Pl+NomVoc
Pass2	= [pass2=+].	// passive 2 (participles) 	przetlałe::przetleć+Verb+Perf+Part+Past+Pass2+Neut+Sg+NomAccVoc
Past	= [past=+].	// past tense 			łyknąłem::łyknąć+Verb+Perf+Ind+Past+1P+Sg+Masc
Perf	= [perf=+].	// perfective verb 		abraduje::abradować+Verb+Perf+Ind+Fut+3P+Sg
Pers	= [pers=+].	// personal (pronoun) 		ja ::ja+Pron+Pers+1P+Sg+MFN+Nom
Phras	= itj.		// phraseology 			górsku::górsku+Phras
Pl	= [pl=+].	// plural 			one::one+Pron+Pers+3P+Pl+M23+Nom
Poss	= [poss=+].	// possessive (pronoun) 	mym::mój+Pron+Poss+MN+Sg+InsLoc
Prep	= prep.		// preposition 			od::od+Prep+Gen
Pres	= [pres=+].	// present tense 		dyszymy::dyszeć+Verb+Imperf+Ind+Pres+1P+Pl
Pron	= pron.		// pronoun 			czyiś::czyj+Pron+M3+Sg+Acc+Mod
Prop	= noun[proper=+].	// proper name 		Polska::Polska+Prop+Fem+Sg+Nom
Punct	= punct.	// punctuation 	  		?::?+Punct+Final
QVerb	= verb[qverb=+].	// quasi verb 		braknie::braknąć+QVerb+Perf+Ind+Pres
Refl	= [refl=+].	// reflexive (pronoun) 		się::się+Pron+Refl+Acc
Rel	= [rel=+].	// relative (pronoun) 		one::ony+Pron+Rel+M23+Pl+Nom
Sg	= [sg=+].	// singular 			pan::pan+Noun+M1+Sg+Nom
Spec	= symb[spec=+].	// special symbol 		@::@+Spec
Unit	= [unit=+].	// measure unit 		kW::kW+Noun+Unit
Verb	= verb.		// verb 			moknie::moknąć+Verb+Imperf+Ind+Pres+3P+Sg
Voc	= [voc=+].	// vocative case 		runie::ruń+Noun+Fem+Pl+Voc
1P	= [p1=+].	// first person 		my::my+Pron+Pers+1P+Pl+MFN+Nom
2P	= [p2=+].	// second person 		mokniesz::moknąć+Verb+Imperf+Ind+Pres+2P+Sg
3P	= [p3=+].	// third person 		on::on+Pron+Pers+3P+Sg+Masc+Nom 

GenAcc	  	= [gen=+, acc=+]. 
