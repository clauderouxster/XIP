!# masc = fem
LEXICON XSG	+MF+Sg		#;
LEXICON XPL	+MF+Pl		#;
LEXICON XSGPL	XSG; XPL;

!# masc = fem; plural = singular
LEXICON XSP	+MF+SP		#;

