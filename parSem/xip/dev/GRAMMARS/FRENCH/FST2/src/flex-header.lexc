!# SPECIAL SYMBOLS (for internal use only)
[-]	!# hyphen or space
(-)	!# hyphen or empty
(.)	!# optional dot

[e] 	!# feminine -e
[2e]	!# feminine -e with doubling consonant
[ce] 	!# feminine -teur/-trice
[le]	!# feminine -eau/elle (beau/belle)
[se]	!# feminine -se (andalou/andalouse)

! for plurals
[s] 	!# plural -s
(s) 	!# optional plural -s 
[x] 	!# plural -x

[NADJ]


Definitions
  MSEP = %' ([% |%-]) | %  | %- ;

!# =========================================================================
!# continuation classes for new lexicon parts
!# =========================================================================

LEXICON MSG	+Masc+Sg	#;	!#
LEXICON MPL	+Masc+Pl	#;	!#
LEXICON FSG	+Fem+Sg		#;	!#
LEXICON FPL	+Fem+Pl		#;	!#
LEXICON XSG	+MF+Sg		#;	!# invariant or unknown gender
LEXICON XPL	+MF+Pl		#;	!# invariant or unknown gender

LEXICON MFSG	MSG; FSG;		!#
LEXICON MFPL	MPL; FPL;		!#
LEXICON MSGPL	MSG; MPL;		!#
LEXICON FSGPL	FSG; FPL;		!#
LEXICON MFSGPL	MSGPL; FSGPL;		!#







