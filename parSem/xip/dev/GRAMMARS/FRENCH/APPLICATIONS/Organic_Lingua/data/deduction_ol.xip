/############################## DEDUCTION   ################################################\

Déduction:

if ( ^LIEU(#1) )
   LOCATION(#1).

if ( ^VILLE(#1) )
   CITY(#1).

if ( ^$1[ville](#1) )
   $1[ville=~,city=+](#1).

if ( ^$1[pays](#1) )
   $1[pays=~,country=+](#1).

if ( ^PERSONNE(#1) )
   PERSON(#1).

if ( ^ORG(#1) )
   ORGANISATION(#1).

if ( ^BATIMENTORG(#1) )
   ORGANISATION(#1).