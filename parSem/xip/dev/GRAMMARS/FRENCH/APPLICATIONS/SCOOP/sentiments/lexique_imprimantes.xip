économique : adj += [sents=+, positif=+].
époustouflant : adj += [sents=+, positif=+].
esthétique : adj += [sents=+, positif=+].
exccellent : adj += [sents=+, positif=+].
exccellente : adj += [sents=+, positif=+].
exceptionnel : adj += [sents=+, positif=+].
exquis : adj += [sents=+, positif=+].
facile : adj += [sents=+, positif=+].
fantastique : adj += [sents=+, positif=+].
favori : adj += [sents=+, positif=+].
fin : adj += [sents=+, positif=+].
fonctionnel : adj += [sents=+, positif=+].
imbattable : adj += [sents=+, positif=+].
indispensable : adj += [sents=+, positif=+].
inégalable : adj += [sents=+, positif=+].
intégré : adj += [sents=+, positif=+].
intuitif : adj += [sents=+, positif=+].
irréprochable : adj += [sents=+, positif=+].
net : adj += [sents=+, positif=+].
// nickel -> localgrams_imprimantes.xip
plaisant : adj += [sents=+, positif=+].
précis : adj += [sents=+, positif=+].
propre : adj += [sents=+, positif=+].
rapide : adj += [sents=+, positif=+].
raisonnable : adj += [sents=+, positif=+].
satisfaisant : adj += [sents=+, positif=+].
silencieux : adj += [sents=+, positif=+].
