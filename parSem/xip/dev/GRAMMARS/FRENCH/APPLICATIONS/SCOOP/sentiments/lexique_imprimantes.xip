valoir : verb += [positif=+, val=+].

// noms
//qualité : noun += [sents=+, positif=+].		// garder? bcp de bruit!
réussite : noun += [sents=+, positif=+].

// adverbes
agréablement : adv += [sents=+, positif=+].
//bien : adv += [sents=+, positif=+].
parfaitement : adv += [sents=+, positif=+].
proprement : adv += [sents=+, positif=+].
rapidement : adv += [sents=+, positif=+].
super : adv += [sents=+, positif=+].



