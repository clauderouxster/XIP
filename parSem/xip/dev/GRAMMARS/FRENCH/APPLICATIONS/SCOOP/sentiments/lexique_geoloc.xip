// lexique_geoloc.xip
// grammaire de géolocalisation
// créé: 27.3.2009 (Sigrid Maurel, CELI France)
// modifié: 7.4.2009 (Sigrid Maurel, CELI France)


Vocabulaires:

//----------------------------------------------------------------------------------------------------
// les directions de géolocalisation
"à côté de" : prep += [geoloc=+].
"à droite" : adv += [geoloc=+].
"à gauche" : adv += [geoloc=+].
"au-dessus de" : prep += [geoloc=+].
"au nord de" : prep += [geoloc=+].
centre : noun += [geoloc=+].
derrière : prep += [geoloc=+].
devant : prep += [geoloc=+].
droite : adj += [geoloc=+].
droite : noun += [geoloc=+].
"en bas de" : prep += [geoloc=+].
"en-dessous de" : prep += [geoloc=+].
"en face de" : prep += [geoloc=+].
"en haut de" : prep += [geoloc=+].
fond : noun += [geoloc=+].
gauche : adj += [geoloc=+].
gauche : noun += [geoloc=+].
"loin de" : prep += [geoloc=+].
milieu : noun += [geoloc=+].
près : prep += [geoloc=+].
"près de" : prep += [geoloc=+].
proche : adj += [geoloc=+].
sous : prep += [geoloc=+].
sur : prep += [geoloc=+].


//----------------------------------------------------------------------------------------------------
// les entités de géolocalisation
banque : noun += [geoent=+].
port : noun += [geoent=+].


//----------------------------------------------------------------------------------------------------
// ajouts divers
cours : noun += [quartier1=+, previllepl=+].


