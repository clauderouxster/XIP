glorifier += [comemor=+].
fêter += [comemor=+].
inaugurer += [comemor=+].
honorer += [comemor=+].
solenniser += [comemor=+].

// noms
anniversaire += [comemor=+].
cérémonie += [comemor=+].
célébration += [comemor=+].
fête += [comemor=+].
hommage += [comemor=+].
