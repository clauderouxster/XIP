Déduction:


//intervalle
|NP#1[tempexpr,interval]|
	if(~INTERVAL(#1))
	INTERVAL(#1)


//pour les durees de type "trois ans, deux heures" ,etc.
|NP#1[timeduree:+]|
	if(~DUREE(#1))
	DUREE(#1)

//TEMPEXPR
//tempexpr exprimant l'approximation
|PP#1[tempexpr,approx]|
	if(~TEMPEXPR[approx](#1))
	TEMPEXPR[approx=+](#1)

//tempexpr exprimant l'origine
|PP#1[tempexpr,orig]|
	if(~TEMPEXPR[orig](#1))
	TEMPEXPR[orig=+](#1)

//tempexpr exprimant la duree (avec une date)
|PP#1[tempexpr,duree]|
	if(~TEMPEXPR[duree](#1))
	TEMPEXPR[duree=+](#1)

//tempexpr exprimant un point final
|PP#1[tempexpr,tfin]|
	if(~TEMPEXPR[tfin](#1))
	TEMPEXPR[tfin=+](#1)




