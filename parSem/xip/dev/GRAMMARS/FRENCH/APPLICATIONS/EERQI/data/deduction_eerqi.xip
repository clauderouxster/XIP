/############################## DEDUCTION   ################################################\

Déduction:


//mots composés :
//////SC{NP{La recherche} FV{a}} FV{VERB_MENTAL:+{mis au jour}}
//SUBJ(a,recherche)

| sc{np{?*,#1[last]}, fv{#2[aux]}}, fv#3{verb{?}} |
if ( SUBJ(#2,#1) )
	SUBJ(#3,#1).



///////////////

//créer la dépendance SENT(ence)

|GROUPE#1{ ?+ } |
  SENT(#1).

///////////////

//potential key sentence dépendance


if ( ^$1(#1[traits_ksw],#2[traits_ksw]))
   $1[potksdep=+](#1,#2).

//Ces difficultés

if ( ^$1[potksdep](#1[mentscopubl:~],#2[lemme:ce]) )
   $1[potksdep=~](#1,#2)

if ( $1[potksdep](#1,#2) & ^$2[potksdep](#1,#2) )
   ~.

/
//transparent

if ( ^$1(#1[traits_ksw],#2[transparent]) & $2(#2[transparent],#3[traits_ksw]) )
   $1[potksdep=+](#1,#2).
		
if ( $1[potksdep](#1[traits_ksw],#2[transparent]) & ^$2(#2[transparent],#3[traits_ksw]) )
   $2[potksdep=+](#2,#3).
		
if ( ^$1(#1[traits_ksw],#2[transparent]) & $2(#3[traits_ksw],,#2[transparent]) )
   $1[potksdep=+](#1,#2).			

if ( $1[potksdep](#1[traits_ksw],#2[transparent]) & ^$2(#3[traits_ksw],#2[transparent]) )
   $2[potksdep=+](#3,#2).

if ( ^$1(#1[transparent],#2[traits_ksw]) & $2(#1[transparent],#3[traits_ksw]) )	
   $1[potksdep=+](#1,#2).

if ( $1(#1[transparent],#2[traits_ksw]) & ^$2(#1[transparent],#3[traits_ksw]) )	
   $2[potksdep=+](#1,#3).

if ( ^$1(#1[transparent],#2[traits_ksw]) & $2(#3[traits_ksw],#1[transparent]) )
   $1[potksdep=+](#1,#2).			

if ( $1[potksdep](#1[transparent],#2[traits_ksw]) & $2(#3[traits_ksw],#1[transparent]) )
   $2[potksdep=+](#3,#1).			
\

if ( SUBJ(?[traits_ksw],#1[humain]) & ^$1[potksdep](#2,#3))
$1[potksdep=~](#2,#3).


///////////////

// marquer les dépendances

if ( ^$1[potksdep](#1[contr],#2) | ^$1[potksdep](#1,#2[contr]) )
   $1[contr=+](#1,#2).

if ( ^$1[potksdep](#1[mental],#2) | ^$1[potksdep](#1,#2[mental]) )
   $1[mental=+](#1,#2).

if ( ^$1[potksdep](#1[scope],#2) | ^$1[potksdep](#1,#2[scope]) )
   $1[scope=+](#1,#2).

if ( ^$1[potksdep](#1[importance],#2) | ^$1[potksdep](#1,#2[importance]) )
   $1[importance=+](#1,#2).

if ( ^$1[potksdep](#1[publ],#2) | ^$1[potksdep](#1,#2[publ]) )
   $1[publ=+](#1,#2).

if ( ^$1[potksdep](#1[possibl],#2) | ^$1[potksdep](#1,#2[possibl]) )
   $1[possibl=+](#1,#2).

if ( ^$1[potksdep](#1[mentsco],#2) | ^$1[potksdep](#1,#2[mentsco]) )
   $1[mentsco=+](#1,#2).

if ( ^$1[potksdep](#1[mentscopubl],#2) | ^$1[potksdep](#1,#2[mentscopubl]) )
   $1[mentscopubl=+](#1,#2).

if ( ^$1[potksdep](#1[mentcontr],#2) | ^$1[potksdep](#1,#2[mentcontr]) )
   $1[mentcontr=+](#1,#2).

if ( ^$1[potksdep](#1[deict],#2) | ^$1[potksdep](#1,#2[deict]) )
   $1[deict=+](#1,#2).



//////////////
//CONTRIBUTION
//////////////

// %%%%%% Stce:L' auteur tient à remercier les étudiants de Licence STAPS de Nantes ( promotion 2002 - 2003 ) qui ont participé à cette recherche

/
if ( ( $1(#1,#2[non_ksw]) |  $1(#1[non_ksw],#2)) & ^SENT(#3))
    SENT[contribution=~](#1,#2).
\
// ****** CONTRIBUTION = publ+deic,possession ****** 
// ****** CONTRIBUTION Agnes = mentscopubl+p1 ****** 

if ( ^$1(#1[mentscopubl],#2[deict,p1] ) )
	$1[contribution=+](#1,#2).

if ( ^$1(#1[deict,p1],#2[mentscopubl] ) )
	$1[contribution=+](#1,#2).

if ( ^$1[contribution](#1[verb,p1:~],#2[p1,deict]) )
	$1[contribution=~](#1,#2).

// %%%%%% Stce:Ces investigations conceptuelles nous ont permis d' opérationnaliser notre étude .

//Agnes: "possession" n'est pas un bon trait pour "notre". On voudrait capter le fait que ce deict est la 1ère personne

 if ( ^$1(#1[mentscopubl,contr:~],#2[deict,p1]) & $2[potksdep](#3,#4) )
 $1[contribution=+](#1,#2).



// ****** CONTRIBUTION=mental+scope & scope+deict ****** 
// ****** CONTRIBUTION Agnes= cette phrase est trop ambiguë

// %%%%%% Stce: Ce travail apportera des éléments de compréhension à un aspect de la construction identitaire des enseignants .

//"apporter" est mental ici car son sujet est mentscopubl. On a rajoutéle trait "mental" à tous les verbes qui ont un sujet mentscopubl


if ( $1[potksdep](#1,#3) & ^$2(#2[publ],#4[deict]) & #1~:#2 & #3~:#4 )
 $2[contribution=+](#2,#4).

if ( ^$1[potksdep](#1,#3) & $2[contribution](#2[publ],#4[deict]) & #1~:#2 & #3~:#4 )
  $1[contribution=+](#1,#3).

// Ce travail apportera


if ( $1[potksdep](#1[fut],#3) & ^$2(#2[mentsco],#4[deict]) & #1~:#2 & #3~:#4 )
 $2[contribution=+](#2,#4).

if ( ^$1[potksdep](#1[fut],#3) & $2[contribution](#2[mentsco],#4[deict]) & #1~:#2 & #3~:#4 )
  $1[contribution=+](#1,#3).

// L'article apportera

if ( $1[potksdep](#2[fut],#3) & ^DETERM[def](#3[mentscopubl],#4) & #1~:#2 & #3~:#4 )
 DETERM[contribution=+](#3,#4).

if ( ^$1[potksdep](#2[fut],#3) & DETERM[def][contribution](#3[mentscopubl],#4) & #1~:#2 & #3~:#4 )
  $1[contribution=+](#2,#3).




// ****** CONTRIBUTION= occurence de la léxie "résumer" qui a le trait "publ" dans une PREPOBJ 
//  %%%%%% Stce:Pour résumer , le travail du chercheur vise à connaître , comprendre et expliquer la réalité dans une perspective d' élaboration de savoirs .)

| #1[unary,publ] |
if (^PREPOBJ(#1,#2))
  PREPOBJ[contribution=+](#1,#2)


// ****** CONTRIBUTION= passiif + (deict,mentscopubl)
//la présente étude est consacrée .
//SUBJ_PASSIVE(consacrée,étude)
//NMOD(étude,présente)

if ( ^SUBJ[passive](#1,#2[mentscopubl]) & $1(#2[mentscopubl],#3[deict]) )
   SUBJ[contribution=+](#1,#2).

if ( SUBJ[passive,contribution](#1,#2[mentscopubl]) & ^$1(#2[mentscopubl],#3[deict]) )
   $1[contribution=+](#2,#3).



//////////////
//PROBLEM///
/////////////


// ****** PROBLEM=contr+mentsco ou mentsco+contr suivi d'une dépendance "potksdep"
// %%%%%% Stce:Leur intérêt, leur caractère de scientificité ne sont guère discutés .


if ( ^$1[mentsco,contr](#1,#2) & $2[potksdep](#3,#4))
    $1[prob=+](#1,#2).

//if ( ^$1(#1[mentsco],#2[contr]) & $2[potksdep](#3,#4))
//    $1[prob=+](#1,#2).

//

if ( ^$1[potksdep,prob:~](#1,#2) & $2[prob](#3[mentcontr],#4[contr]) & #1~:#2 & #3~:#4 & ~$1[prob](#1,#2) )                              $1[prob=+](#1,#2).

if ( ^$1[potksdep,prob:~](#1,#2) & $2[prob](#3[contr],#4[mentcontr]) & #1~:#2 & #3~:#4 & ~$1[prob](#1,#2) )
  $1[prob=+](#1,#2).

//


// ****** PROBLEM=contr,noun+mental ou mental+contr,noun suivi d'une dépendance "potksdep"
// %%%%%% Stce:Charles Hadji et Jacques Baillé ( 1998 ) contribuent à répondre à ces interrogations en précisant que les ressources pour les enseignants sont de trois sortes

if ( (^$1(#1[contr,noun],#2[mental]) | ^$1(#1[mental],#2[contr,noun])) & $2[potksdep](#3,#4) )
    $1[prob=+](#1,#2).

if ( $1[potksdep](#1,#2) & (^$2[prob](#3[contr,noun],#4[mental]) | ^$2[prob](#3[mental],#4[contr,noun]))  & #1~:#3 & #2~:#4 )
  $2[prob=+](#3,#4).

//
/
// %%%%%% Stce:Par la suite , une analyse de contenu a été menée au sein des difficultés propres à chaque facette .
if(($1(#1[transparent],#2[scope]) | $1(#1[transparent],#2[contr])) & (^$2(#3[contr],#1[transparent]) |^$2(#1[transparent],#3[contr])) & #2~:#3)
  $2[prob=+](#3,#1)
\



/
| #1[unary,contr] |
if (( ^$1[potksdep](#2[scope],#3[traits_ksw]) |^$1[potksdep](#2[traits_ksw],#3[scope])) & $2[potksdep](#4,#5) & SUBJ(?,#6[pays:~]) & #2~:#4 & #3~:#5)
$1[prob=+](#3,#4)
\

// ****** PROBLEM=occurence de la léxie "hypothèse" qui a le trait "contr" et créer la dépendance "prob" dans la phrase
// %%%%%% Stce:Dans une deuxième phase , sous l' hypothèse que ces variations ne sont pas aléatoires et qu' existent des processus organisateurs sous jacents au choix et à l' agencement dynamique des modalités prises par la ( les ) composante(s) considérée(s) , des régularités sont recherchées .
/
| #1[lemme:hypothèse] |
if (^SENT(#2) )
  SENT[prob=+](#2)


| #1[argumentation,contr] |
if (^SENT(#2)  & ($1(#3[mentsco],#4[condition]) | $1(#3[condition],#4[mentsco])) & $2[potksdep](#5,#6))
 SENT[prob=+](#2)
\

// %%%%%% Stce:L'enseignant doit poser des questions à l' élève et pour chaque mauvaise réponse , il doit lui donner une punition , un choc électrique , dont l' intensité s' accentue à chaque erreur .
if ( SUBJ(#1,#2[humain])  & ^$1[potksdep](#3,#4))
    $1[prob=~](#3,#4).


/

/////////////////
//Possibility
////////////////

// ****** POSSIBILITY=possibl+mentsco & possibl+mentsco

// %%%%%% Stce:La recherche peut alors être considérée comme une pratique qui véhicule ses propres a priori , même si le chercheur - praticien observe le travail des enseignants sans préjugés supposés , en se détachant de toute attitude normative ou militante .
//
if ( ^$1(#1[possibl],#2[scope]) & $2[potksdep](#3[mentsco],#4[mentsco]) )
$1[possibility=+](#1,#2)

if ($1[possibility](#1[possibl],#2[scope]) & ^$2[potksdep](#3[mentsco],#4[mentsco]) & #1~:#3 & #2~:#4)
$2[possibility=+](#3,#4)
// 

// %%%%%% Stce:Naturellement , ces argumentations peuvent , d' un côté comme de l' autre , masquer en partie des enjeux de pouvoirs .
if ( ^$1(#1[possibl],#2[mental,noun])  & $2[potksdep](#3,#4))
$1[possibility=+](#1,#2)

if ($1[possibility](#1[possibl],#2[mental,noun]) & ^$2[potksdep](#3,#4) & #1~:#3 & #2~:#4)
$2[possibility=+](#3,#4)

/// %%%%%% Stce:L' approche longitudinale permet de relever des confrontations entre les pré conceptions et la réalité telles qu' elles prennent forme au niveau de la relation pédagogique .

if ( (^$1(#1[possibl],#2[scope]) | ^$1(#1[scope],#2[possibl])) & $2[possibl](#3,#4))
$1[possibility=+](#1,#2)

if (( $1[possibility](#1[possibl],#2[scope]) | $1(#1[scope],#2[possibl])) & ^$2[possibl](#3,#4) & #1~:#3 & #2~:#4)
$2[possibility=+](#3,#4)

// %%%%%% Stce:On pourrait avancer l' hypothèse suivante : si le philosophe destine son texte à un lecteur universel , il vise ce lecteur en s' adressant d'abord à la communauté philosophique , alors que le « philosophe de l' éducation » devrait d'abord être entendu d' une « communauté des pédagogues » .

if ( (^$1(#1[possibl],#2[contr]) | ^$1(#1[contr],#2[possibl]))  & $2[potksdep](#3,#4))
$1[possibility=+,prob=+](#1,#2)

if ( ($1[possibility,prob](#1[possibl],#2[contr]) | $1[possibility,prob](#1[contr],#2[possibl]))  & ^$2[potksdep](#3,#4))
$2[possibility=+,prob=+](#3,#4)


/////////////////
//Importance
///////////////

// ****** Importance: transparent+importance & transparent + mentsco
// %%%%%% Stce:L' importance du regard porté par cette discipline universitaire réside dans une analyse plurielle ( d'où l' utilisation du pluriel du mot « science » ) qui dépasse la simple juxtaposition de plusieurs lectures disciplinaires et qui intègre la mise en relation des regards .)

if  (^$1(#1[transparent],#2[importance]) & $2(#1[transparent],#3[mental]) )
  $1[import=+](#1,#2)

if  ($1[import](#1[transparent],#2[importance]) & ^$2(#1[transparent],#3[mental]) )
  $2[import=+](#1,#3)

//
// %%%%%% Stce:L' importance du regard porté par cette discipline universitaire réside dans une analyse plurielle ( d'où l' utilisation du pluriel du mot « science » ) qui dépasse la simple juxtaposition de plusieurs lectures disciplinaires et qui intègre la mise en relation des regards .
if ( (^$1(#1[importance],#2[scope]) | ^$1(#1[scope],#2[importance]))  & $2[potksdep](#3,#4))
$1[import=+](#1,#2)

if ( ($1[import](#1[importance],#2[scope]) | $1[import](#1[scope],#2[importance]))  & ^$2[potksdep](#3,#4))
$2[import=+](#3,#4)

// %%%%%% Stce: Il est important de souligner....
if(^ADJMOD(#1[importance],#2[mental]) )
ADJMOD[import=+](#1,#2)

// %%%%%% Stce:Nous nous intéressons notamment.....
if ( ^$1(#1[importance,verb],#2[deict,p1]) & $2[potksdep](#3,#4) )
 $1[contribution=+,import=+](#1,#2).

// %%%%%% Stce:Leur intérêt, leur caractère de scientificité ne sont guère discutés .
if ( (^$1(#1[importance],#2[contr]) | ^$1(#1[contr],#2[importance]))  & $2[potksdep](#3,#4))
$1[import=+,prob=+](#1,#2)

if ( ($1[import,prob](#1[importance],#2[contr]) | $1[import,prob](#1[contr],#2[importance]))  & ^$2[potksdep](#3,#4))
$2[import=+,prob=+](#3,#4)

\

/////////////////////

if ( ^$1[contribution](#1,#2) )
   $1[ksdep=+,potksdep=~](#1,#2)

if ( ^$1[prob](#1,#2) )
   $1[ksdep=+,potksdep=~](#1,#2)

if ( ^$1[possibility](#1,#2) )
   $1[ksdep=+,potksdep=~](#1,#2)

if ( ^$1[import](#1,#2) )
   $1[ksdep=+,potksdep=~](#1,#2)

/////////////////////



if ( ^SENT(#1) & $1[ksdep](#2,#3) & ~SENT[ks](#1) )
   SENT[ks=+](#1).

if ( ^SENT[ks](#1) & $1[contribution](#2,#3) )
   SENT[contribution=+](#1)

if ( ^SENT[ks](#1) & $1[prob](#2,#3) )
   SENT[prob=+](#1)

if ( ^SENT[ks](#1) & $1[possibility](#2,#3) )
   SENT[possibility=+](#1)

if ( ^SENT[ks](#1) & $1[import](#2,#3) )
   SENT[import=+](#1)

if ( SENT[ks](#1) & ^$1[potksdep](#2,#3) )
   $1[potksdep=~,ksdep=+](#2,#3).

if ( ^SENT[prob](#1)) 
 SENT[ks=+](#1)

////////////////////////

if ( ^SENT[prob,contribution](#1) )
   PROBLEM-SUMMARY(#1).

if ( ^SENT[prob](#1) )
   PROBLEM(#1).

if ( ^SENT[contribution](#1) )
   SUMMARY(#1).


/////////////////////////

| #2[md=+] |
if ( PROBLEM-SUMMARY(#1) & $1[ksdep](#2,#3) )
   ~.

| #3[md=+] |
if ( PROBLEM-SUMMARY(#1) & $1[ksdep](#2,#3) )
   ~.

| #2[md=+] |
if ( PROBLEM(#1) & $1[ksdep](#2,#3) )
   ~.

| #3[md=+] |
if ( PROBLEM(#1) & $1[ksdep](#2,#3) )
   ~.

| #2[md=+] |
if ( SUMMARY(#1) & $1[ksdep](#2,#3) )
   ~.

| #3[md=+] |
if ( SUMMARY(#1) & $1[ksdep](#2,#3) )
   ~.

