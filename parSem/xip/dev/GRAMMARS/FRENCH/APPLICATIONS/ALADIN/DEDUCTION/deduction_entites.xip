	DATE[t_mod=approx,absolue=+](#1).

|PP#1[date:+,absolue:+,nondate:~,orig:+]|
	if(~DATE[orig:+](#1))
	DATE[orig=+,absolue=+](#1).

|PP#1[date:+,absolue:+,nondate:~,tfin:+]|
	if(~DATE[tfin:+](#1))
	DATE[tfin=+,absolue=+](#1).

|PP#1[date:+,absolue:+,nondate:~,t_mod:~approx,orig:~,tfin:~,in-interval:~]|
	if(~DATE(#1))
	DATE[absolue=+](#1).

|PP#1[date,relative,duree:~,datecoord:~,in-interval:~]|
	if(~DATE[relative](#1))
	DATE[relative=+](#1).	


// Dates relative dans PP:
