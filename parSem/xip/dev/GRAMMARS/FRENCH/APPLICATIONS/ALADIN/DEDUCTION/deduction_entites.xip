// Dates absolues completes simples (NP):

|NP#1[date,absolue,complete,nondate:~,datecoord:~,in-interval:~,interval:~]|
	if(~DATE[complete,absolue](#1))
	DATE[complete=+,absolue=+](#1).
	

// Dates absolues incompletes simples (NP):

|NP#1[date,absolue,incomplete,nondate:~,datecoord:~,in-interval:~]|
	if(~DATE[incomplete,absolue](#1))
	DATE[incomplete=+,absolue=+](#1).


|ADV#1[date,absolue,incomplete,nondate:~,datecoord:~,in-interval:~]|
	if(~DATE[incomplete,absolue](#1))
	DATE[incomplete=+,absolue=+](#1).

// Dates absolues contenant une coord (NP):

|NP#1[date,absolue,nondate:~,datecoord:+]|
	if(~DATECOORD(#1))
	DATECOORD[absolue=+](#1).


// Dates relatives simples (NP):

|NP#1[date,relative,duree:~,datecoord:~,in-interval:~]|
	if(~DATE[relative](#1))
	DATE[relative=+](#1).

// Dates relatives contenant une coord (NP):

|NP#1[date,relative,duree:~,datecoord:+]|
	if(~DATECOORD(#1))
	DATECOORD[relative=+](#1).


// Dates absolues dans PP:
// Possibilité de ne faire qu'une seule règle
// si on ne veut pas les distinctions approx,orig, etc.

|PP#1[date:+,absolue:+,nondate:~,t_mod:approx,in-interval:~]|
