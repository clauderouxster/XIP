|PP#1[date:+,relative:+,nondate:~,t_mod:~approx,orig:~,tfin:~,in-interval:~]|
	if(~DATE(#1))
	DATE[relative=+](#1).

|PP#1[date:+,relative:+,duree:~,in-interval:~]|
	if(~DATE(#1))
	DATE[relative=+](#1).

