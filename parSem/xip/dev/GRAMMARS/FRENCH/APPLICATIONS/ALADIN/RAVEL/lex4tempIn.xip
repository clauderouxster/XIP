Vocabulaires:


// lexique correspondant a evenements T+0J

admission: 		 noun += [temp_anchor:t0] .
opération:		 noun += [temp_anchor:t0] .
chirurgie:		 noun += [temp_anchor:t0] .
entrée:			 noun += [temp_anchor:t0] .


post-opératoire:	 adj += [temp_anchor:t0,direc:+] .
pré-opératoire:		 adj += [temp_anchor:t0,direc:-] .

// ordinaux
premier:		 adj += [numJour=01] .
deuxième:		 adj += [numJour=02] .
troisième:		 adj += [numJour=03] .
quatrième:		 adj += [numJour=04] .
cinquième:		 adj += [numJour=05] .
sixième:		 adj += [numJour=06] .
septième:		 adj += [numJour=07] .
huitième:		 adj += [numJour=08] .
neuvième:		 adj += [numJour=09] .
dixième:		 adj += [numJour=10] .
onzième:		 adj += [numJour=11] .
douzième:		 adj += [numJour=12] .
