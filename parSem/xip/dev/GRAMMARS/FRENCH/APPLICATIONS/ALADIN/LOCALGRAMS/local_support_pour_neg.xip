Séquence:

// marquer avoir avec v_exist dans les expr "y (en) avoir"
1> ? =  pron[lemme:y], (pron[lemme:en]), verb[lemme:avoir,v_exist=+] .

// faire ressortir;voir
1> ? = verb[lemme:faire,v_evid=+,polar=+,support_deb=+], adv*, verb[lemme:ressortir,support_fin=+];verb[lemme:voir,support_fin=+] .

// fournir preuve de 
1> ? = verb[lemme:fournir,v_evid=+,polar=+,support_deb=+], adv*, (det), noun[lemme:preuve,support_fin=+] | prep[lemme:de] | .

// mettre en évidence;exergue
1> ? = verb[lemme:mettre,v_evid=+,polar=+,support_deb=+], adv*, ?[lemme:en], noun[lemme:évidence,support_fin=+];noun[lemme:exergue,support_fin=+] .

// disposer d'argument 
1> ? = verb[lemme:disposer,v_evid=+,polar=+,support_deb=+], adv*, ?[lemme:de], noun[lemme:argument,support_fin=+].

// trahir la presence
1> ? = verb[lemme:trahir,v_evid=+,polar=+,support_deb=+], adv*, noun[lemme:présence,support_fin=+] .


// laisser/mettre de cote
1> ? = verb[lemme:laisser,v_evid=-,polar=-,support_deb=+];verb[lemme:mettre,v_evid=-], adv*, prep[lemme:de], noun[lemme:côté,support_fin=+] .

// mettre à l'index
1> ? = verb[lemme:mettre,v_evid=-,polar=-,support_deb=+], adv*, prep[lemme:à], det, noun[lemme:index,support_fin=+] .




// tenir à l'écart
1> ? = verb[lemme:tenir,v_evid=-,polar=-,support_deb=+], adv*, prep[lemme:à], det, noun[lemme:écart,support_fin=+] .


Multimots:

2> "mise en évidence" = | det;prep | noun[n_evid=+] .
2> "mise en exergue" = | det;prep | noun[n_evid=+] .