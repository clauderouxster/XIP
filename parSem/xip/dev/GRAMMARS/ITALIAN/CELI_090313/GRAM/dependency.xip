// PREDCOMPL_OBJ
// 3.11.2008 (SM):
// Considero Giovanni un pericoloso criminale.
// Ha reso le strade scivolose.
| SC{?*, FV{?*, verb#1[last:+, atto:+]}}; NFV{?*, verb#1[last:+, atto:+]},
 NP{?*, #2[last:+]}, (punct[quote:+]), AP{?*, #3[last:+]}; NP{?*, #3[last:+]} |
	if ( OBJ(#1, #2) & #2[number]:#3[number] )
	PREDCOMPL_OBJ(#1, #3).

// 4.11.2008 (SM):
// a rendere percorribili alcune strade
| IV{?*, verb#1[last:+, atto:+]}, AP{?*, #3[last:+]}; NP{?*, #3[last:+]}, NP{?*, #2[last:+]} |
	if ( OBJ(#1, #2) & #2[number]:#3[number] )
	PREDCOMPL_OBJ(#1, #3).

// 3.11.2008 (SM):
// Lo hanno chiamato scemo
| SC{?*, FV{?*, verb#1[last:+, atto:+]}}; NFV{?*, verb#1[last:+, atto:+]}, AP{?*, #3[last:+]} |
	if ( OBJ(#1, #2[pron:+]) & #2[number]:#3[number] )
	PREDCOMPL_OBJ(#1, #3).

// 3.11.2008 (SM):
// Ho visto la nave affondare.
| SC{?*, FV{?*, verb#1[last:+, percept:+]}}; NFV{?*, verb#1[last:+, percept:+]},
 NP{?*, #2[last:+]}, AP{?*, #3[last:+]}; IV{?*, verb#3[last:+]} |
	if ( OBJ(#1, #2) & ~PREPOBJ(#3, #10[lemma:di]) )
	PREDCOMPL_OBJ(#1, #3).

// 3.11.2008 (SM):
// Ho visto affondare la nave.
| SC{?*, FV{?*, verb#1[last:+, percept:+]}; RFV{?*, verb#1[last:+, percept:+]}};
 NFV{?*, verb#1[last:+, percept:+]}, IV{?*, verb#3[last:+]} |
	if ( ^OBJ[sentential:+](#1, #3) & ~PREPOBJ(#3, #10[lemma:di]) )
	PREDCOMPL_OBJ(#1, #3).

// 4.11.2008 (SM):
// e li vedeva caricare le armi
| SC{?*, FV{?*, verb#1[last:+, percept:+]}}; NFV{?*, verb#1[last:+, percept:+]},
 IV{?*, verb#3[last:+]}, NP{?*, #2[last:+]} |
	if ( PREDCOMPL_OBJ(#1, #3) & ~OBJ(#10, #2) )
	OBJ(#1, #2).

// 3.11.2008 (SM):
// Ho visto il vicino che telefonava.
| SC{?*, FV{?*, verb#1[last:+, percept:+]}}; NFV{?*, verb#1[last:+, percept:+]},
 NP{?*, #2[last:+, lemma:~ciò]}, SC{BG{conjche}, FV{?*, verb#3[last:+]}} |
	if ( OBJ(#1, #2) )
	PREDCOMPL_OBJ(#1, #3).

// 4.11.2008 (SM):
// presidente della commissione Industria , eletto qui deputato
| SC{?*, FV{?*, verb#1[last:+, atto:+, percept:~]}}; NFV{?*, verb#1[last:+, atto:+, percept:~]}, (ADVP),
 NP{?*, #2[last:+]} |
	if ( ~PREDCOMPL_OBJ(#1, #10) & ~PREDCOMPL_SUBJ(#1, #20) & ~SUBJ(#1, #2) & ~OBJ(#1, #2) )
	PREDCOMPL_OBJ(#1, #2).


