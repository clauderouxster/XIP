DependencyRules:

/////////////////////////////////////////////////////////////////////////////////////////////////////
//HEADs
//| ?#1{?*, #2[last]} |
//if ( ~HEAD(?,#2) )
//	HEAD(#2,#1).

//| TOP#1{?*, #2[last]} |
//if ( HEAD(#2,#1) )
//	~.

//Nouns.
| NP#1{?*, #2[last]} |
	if ( ~HEAD(?,#2) )
	HEAD(#2,#1).

//Adj.
|ap#1{?*, #2[last]}| 
	if ( ~HEAD(?,#1) )
	HEAD(#2,#1).

//Adv.
| advp#1{?*, #2[last]} | 
	if ( ~HEAD(?,#1) )
	HEAD(#2,#1).

//PP
|pp#1{prep, ?*, #2[last]}| 
	if(head(#20, #2)) 
	HEAD(#20, #1).

//Prep.
//| pp#1{prep, np{?*,#2[last]} } | 
//if ( ~HEAD(?,#1) )
//	HEAD(#2,#1).

//IV.
| iv#1{?*, #2[last]} | 
	if ( ~HEAD(?,#1) )
	HEAD(#2,#1).

//FV.
| fv#1{?*, #2[last]} | 
	if ( ~HEAD(?,#1) )
	HEAD(#2,#1).

//NFV.
| nfv#1{?*, #2[last]} | 
	if ( ~HEAD(?,#1) )
	HEAD(#2,#1).

//RFV.
| rfv#1{?*, #2[last]} | 
	if ( ~HEAD(?,#1) )
	HEAD(#2,#1).


/////////////////////////////////////////////////////////////////////////////////////////////////////
// NEs

//PERSON
| noun#1[prop,featpeople,!featpeople:!, abbr:~,premod:~,postmod:~] |
if ( ~PERSON(#1) )
PERSON(#1).

| noun#1 |
if ( (lastname##1:1) & ~PERSON(#1) )
	PERSON(#1).

| noun#1{?*, noun^#2, ?*} |
	if ( PERSON(#1) & ^PERSON(#2) )
	~.

//LOC
| noun#1[prop,featgeo,!featgeo:!, demonym:~] |
	if ( ~LOC(#1) & ~ORG(#1) )	
	LOC(#1).

| noun#1 |
	if ( (place##1:1) & ~LOC(#1) )
	LOC(#1).

| noun#1{?*, noun^#2, ?*} |
	if ( LOC(#1) & ^LOC(#2) )
	~. 

//ORG
| noun#1[featorg, !featorg:!, org=~,prop,abbr:~,premod:~,postmod:~] |
	if ( ~ORG(#1) & ~PERSON(#1) & ~LOC(#1) )
	ORG(#1).

//| noun#1[prop,org,mwe] |
//	if ( ~ORG(#1) )
//	ORG(#1).

| noun#1 |
	if ( (soc##1:1) & ~ORG(#1) )
	ORG(#1).

| noun#1{?*, noun^#2, ?*} |
	if ( ORG(#1) & ^ORG(#2) )
	~. 

//DATE
| noun#1[date:+] |
	DATE(#1).

| noun#1{?*, noun^#2, ?*} |
	if ( DATE(#1) & ^DATE(#2) )
	~.

| num#1[date:+] |
	DATE(#1).

//ARTEFACT
| noun#1[featartefact, !featartefact:!, prop,abbr:~,premod:~,postmod:~] |
	ARTEFACT(#1).

| noun#1{?*, noun^#2, ?*} |
	if ( ARTEFACT(#1) & ^ARTEFACT(#2) )
	~.


/////////////////////////////////////////////////////////////////////////////////////////////////////
// STARE
// 10.10.2008 (SM):
// In Albania intanto la situazione del Paese sta precipitando verso la guerra civile.
| SC{?*[fv:~], FV{?*, #1[form:fstare, last:+]}; RFV{?*, #1[form:fstare, last:+]}},
 GV{?*, #2[gerund:+, last:+]} |
	STARE(#1, #2).

// 10.10.2008 (SM):
// Ma se sarà messo con le spalle al muro dai clan che si stanno scontrando nel suo partito...
| FV{?*, #1[form:fstare, last:+]}; RFV{?*, #1[form:fstare, last:+]}, GV{?*, #2[gerund:+, last:+]} |
	STARE(#1, #2).


/////////////////////////////////////////////////////////////////////////////////////////////////////
//DOMIN.
//La password non viene accettata / non va accettata. 16/05
|SC{ ?*,  FV{?*,#2[last,aux]}}, ADVP*, NFV{?*,#3[last,pastpart]} |	
	if((#2[number]:#3[number])	
	& ~DOMIN(#2,#3))
	DOMIN(#2,#3).

//Il bambino E' CADUTO nel pozzo.
// 2.10.2008 (SM):
// la nostra linea politica é rappresentata dal Movimento popolare del Kosovo
//|SC{ ?*,  FV{?*,#2[last,form:fessere]}}, ?*[punct:~,art:~,noun:~,nfv:~], NFV{?*,#3[last,pastpart,form:~fessere]} |
| SC{?*, FV{?*, #2[last:+, form:fessere]}}, ?*[punct:~, sc:~, np:~, nfv:~],
 NFV{?*, #3[last:+, pastpart:+]} |
	if( (#2[number]:#3[number])& ~DOMIN(#2,#3) )
	DOMIN(#2, #3).

//L'onesto HA PREVALSO sull'utile.
// 10.11.2008 (SM):
// Abbiamo notizia della presenza della criminalità italiana nello scandalo delle finanziarie albanesi ha detto Vigna
//| SC{?*, FV{?*, #2[last:+, form:favere]}}, ?*[punct:~, art:~, noun:~, nfv:~],
| SC{?*, FV{?*, #2[last:+, form:favere]}}, ?*[punct:~, art:~, np:~, nfv:~, sc:~],
 NFV{?*, #3[last:+, pastpart:+, form:~fessere]} |
// 27.8.2008 (SM):
// Hanno ucciso Tomaso alle spalle.
//	if((#2[number]:#3[number])
//	& ~DOMIN(#2,#3))
	if ( ~DOMIN(#2, #3) )
	DOMIN(#2, #3).

// 26.9.2008 (SM):
// mi sono mangiato
| SC{?*, RFV{?*, #1[last:+, form:fessere]}}, ?*[punct:~, np:~, nfv:~],
 NFV{?*, #2[last:+, pastpart:+, form:~fessere]} |
	if( (#1[number]:#2[number])& ~DOMIN(#1, #2) )
	DOMIN(#1, #2).

// 9.10.2008 (SM):
// mio fratello è stato ucciso alle spalle
| NFV{?*, #1[last:+, pastpart:+, form:fessere]}, NFV{?*, #2[last:+, pastpart:+]} |
	if ( ^DOMIN(#10, #1) )
	DOMIN(#10, #2).


/////////////////////////////////////////////////////////////////////////////////////////////////////
//PREPOBJ.
| pp#1{#3[prep], np{?*,#4[last]} } |
if ( ~PREPOBJ(#4,#3) )
	PREPOBJ(#4,#3).

| iv#1{#3[prep], #5[last]} | 
	PREPOBJ(#5,#3).

//| pp#1{#3[prep], ap{?*,#4[last]} } | 
//	PREPOBJ(#4,#3).

// 7.11.2008 (SM):
// le persone alle quali ha parlato
| prep#1, SC{?*, FV{(det), pron#2, verb[last:+]}} |
	PREPOBJ(#2, #1).


/////////////////////////////////////////////////////////////////////////////////////////////////////
// CONJOBJ
// 17.10.2008 (SM):
// mangia perché non nuota
| SC{BG{conj#1[subord:+]}, FV{?*, verb#2[last:+]}; RFV{?*, verb#2[last:+]}} |
	CONJOBJ(#2, #1).


/////////////////////////////////////////////////////////////////////////////////////////////////////
//DETERMINERS; Ex "Il ragazzo, questo ragazzo, la mia ragazza", etc...
| NP#3{?*, det#2[tokenend:~]} | 
	if ( HEAD(#1,#3) )
	DETD(#1,#2).

// 27.8.2008 (SM):
// il governo della città
| NP#3{?*, det#2[tokenend:+]} | 
	if ( HEAD(#1, #3) )
	DETD(#1, #2).



/////////////////////////////////////////////////////////////////////////////////////////////////////
//REFLEX : questa politica SI INTEGRA in una politica di repressione
// 31.10.2008 (SM), inclusion of PRONOM info
// mi lavo, mi arrabbio
//| RFV#3{?*, pron#2[refl:+]} |
| RFV#3{?*, pron#2[refl:+], verb[pronom:~, aux:~, form:~fessere, last:+]} | 
	if ( HEAD(#1, #3) )
	REFLEX(#1, #2).

// 31.10.2008 (SM):
// mi sono lavata, mi sono arrabbiata
| SC{?*, RFV#3{?*, pron#2[refl:+]}}, NFV{verb#4[pronom:~]} | 
	if ( HEAD(#1, #3) & DOMIN(#1, #4) )
	REFLEX(#4, #2).

// 26.9.2008 (SM):
// io mi mangio, mi sono lavata
if ( ^REFLEX(#1, #2) & #1[trans:+, pronom:~] )
	OBJ[refl=+](#1, #2).

// 26.9.2008 (SM):
// io ti mangio
| FV#3{?*, pron#2[acc:+, lemma:~si]} | 
	if ( HEAD(#1[trans:+], #3) )
	OBJ(#1, #2).


/////////////////////////////////////////////////////////////////////////////////////////////////////
// SUBJ_IMPERS
// 31.10.2008 (SM):
// Si riciclava denaro sporco con diversi sistemi.
| RFV{?*, pron#1[lemma:si], verb#2[pronom:~, form:~fessere]} | 
	SUBJ_IMPERS(#2, #1).

// 31.10.2008 (SM):
// Si è riciclato denaro sporco con diversi sistemi.
| SC{?*, RFV{?*, pron#1[lemma:si], verb#2[form:fessere, last:+]}}, NFV{verb#3[pronom:~]} | 
	if ( DOMIN(#2, #3) )
	SUBJ_IMPERS(#3, #1).

// 6.11.2008 (SM):
// Si riciclava denaro sporco con diversi sistemi.
if ( ^OBJ[refl:+](#1, #2) & SUBJ_IMPERS(#1, #2) )
	~.



/////////////////////////////////////////////////////////////////////////////////////////////////////
// EMPTYCOMPL
// 31.10.2008 (SM):
// Anche Dashamir Shehi attacca il Governo e si oppone a misure di emergenza . 
| RFV{?*, pron#1[lemma:si], verb#2[pronom:+, form:~fessere]} | 
	if ( ~SUBJ_IMPERS(#2, #1) )
	EMPTYCOMPL(#2, #1).

// 31.10.2008 (SM):
// Ma ieri la folla aveva campo libero e si é sfogata contro ogni simbolo del potere centrale...
| SC{?*, RFV{?*, pron#1[lemma:si], verb#2[form:fessere, last:+]}}, NFV{verb#3[pronom:+]} | 
	if ( DOMIN(#2, #3) & ~SUBJ_IMPERS(#3, #1) )
	EMPTYCOMPL(#3, #1).

// 31.10.2008 (SM):
// Pansa ha confermato quanto sostenuto dal collega dell' antimafia , limitandosi ad ammettere ...
| GV{verb#1[pronom:+, tokenstart:+]}, NP{pron#2[tokenend:+, lemma:si]} |
	EMPTYCOMPL(#1, #2).


////////////////////////////////////  SUBJ.  /////////////////////////////////////////////////////////////

//io mangio, tu mangi etc...~*io cammina etc...
//| NP{#1[pron:+,nom:+]}, ADVP*, FV{?*,#2[last]}, ~NFV |
// 20.8.2008 (SM):
// io mangio
| NP{#1[pron:+, nom:+]}, ADVP*, FV{?*, #2[last:+]} |
	if ( #1[person]:#2[person]
	   & #1[number]:#2[number] )
	SUBJ(#2, #1).

// 6.10.2008 (SM):
// tutto dipenderà dalle iniziative
| NP{#1[pron:+, indef:+]}, ADVP*, FV{?*, #2[last:+]} |
	if ( #1[number]:#2[number] )
	SUBJ(#2, #1).

//tutta F.C. Internazionale ha il piacere di ...
| NP{?*,#1[org:+,noun:+,nom:~,last]}, ?*, FV{?*,#2[last]}, NFV~ |
	if(#1[number]:#2[number]
	&~SUBJ(#2,#1))
	SUBJ(#2,#1).

// 10.3.2009 (SM):
// questa è ancora la città
| NP{#1[pron:+, dem:+]}, ADVP*, FV{?*, #2[last:+]} |
	if ( #1[number]:#2[number] )
	SUBJ(#2, #1).

//Il cane (nero/ di Michele) mangia.  Nelle grandi città il traffico è intenso. Nel centro delle grandi città il traffico è intenso. ~*Il cane di Michele MANGIANO
//16/05:ajouté ?*{?*[nfv:~]
//| NP{?*,#1[noun,nom:~,last]}, ?*, FV{?*,#2[last]}, ~NFV |
// 19.8.2008 (SM), 11.9.2008 (SM):
// C'é il sospetto che per aggiustare i guasti di questa catena di Sant'Antonio balcanica, una follia che ha coinvolto un milione di albanesi...
| NP{?*, #1[noun:+, nom:~, last:+]}; NP{?*, AP[last:+]{?*, #1[adj:+, last:+]}}, ?*[np:~, bg:~],
 FV{?*, #2[last:+]} |
	if ( #1[number]:#2[number] & ~SUBJ(#2, #1) )
	SUBJ(#2, #1).

// 11.9.2008 (SM):
// Sigrid Maurel lavora sulla grammatica italiana.
| NP{?*, #1[noun:+, nom:~, last:+, src:guess]}, ?*[np:~], FV{?*, #2[last:+]} |
	if ( ~SUBJ(#2, #1) )
	SUBJ(#2, #1).

//La più bella tra le mie amiche è Giovanna.
| NP{?*, AP#1{?*, #2[adj,last]}}, ?*, FV{?*,#3[last]}, NP[firstname]|
	if(#2[number]:#3[number])
	SUBJ(#3,#2).

//La più bella tra le mie amiche è questa.
| NP{?*, AP#1{?*, #2[adj,last]}}, ?*, FV{?*,#3[last]}, adj[dem:+]|
	if(#2[number]:#3[number])
	SUBJ(#3,#2).

//Nelle grandi città il traffico è diventato intenso.
//| SC{PP*, NP{?*,#1[last]}, ?*, FV{?*,#2[form:fessere,last]}}, NFV{?*,#3[pastpart,last]}, ?*[AP:+,NP:+,ADVP:+,NFV:~] |

// 12.9.2008 (SM):
// Valona é in mano alla folla, fuori da ogni controllo, la polizia é scomparsa, rintanata nelle caserme .
//| SC{PP*, NP{?*,#1[last]}, ?*, FV{?*,#2[form:fessere,last]}}, NFV{?*,#3[pastpart,last]} |
| SC{?*[fv:~, np:~], NP{?*, #1[last:+]}, ?*[np:~], FV{?*, #2[form:fessere, last:+]}},
 NFV{?*, #3[pastpart:+, last:+]} |
	if (
	  #1[number]:#2[number]
	& #1[number]:#3[number]
	& #1[gender]:#3[gender]
	& #2[number]:#3[number]
	& ^SUBJ(#2, #1)
	)
	SUBJ(#3, #1).

//La Confederazione Generale Italiana dei Lavoratori ha fatto sciopero.
| SC{punct*, NP{?*,#1[org:+,mwe,last]}, ?*, FV{?*,#2[form:favere,last]}}, NFV{?*,#3[pastpart,pl:~,fem:~,last]} |
	if ( #1[number]:#2[number] & ^SUBJ(#2,#1)~ )
	SUBJ(#3,#1).

//IL ragazzo ha camminato sulla tavola.
// 12.9.2008 (SM):
// Già un paio di mesi fa, quando non si parlava ancora di fallimenti, Berisha aveva fatto fuori il capo della polizia e quasi tutti gli ufficiali delle dogane .
//| SC{punct*, PP*, NP{?*, #1[last]}, ?*, FV{?*,#2[form:favere,last]}}, NFV{?*,#3[pastpart,pl:~,fem:~,last]} |
| SC{?*[fv:~, np:~], NP{?*, #1[last:+]}, ?*[coord:~], FV{?*, #2[form:favere, last:+]}},
 NFV{?*, #3[pastpart:+, pl:~, fem:~, last:+]} |
	if (
//	  #1[number]:#2[number]
	  DOMIN(#2, #3)
	& ^SUBJ(#2, #1)
	)
	SUBJ(#3, #1).

// 11.9.2008 (SM):
// Sigrid Maurel ha camminato nella città.
| SC{?*[fv:~, np:~], NP{?*, #1[last:+, src:guess]}, ?*[np:~], FV{?*, #2[form:favere, last:+]}},
 NFV{?*, #3[pastpart:+, pl:~, fem:~, last:+]} |
	if ( ^SUBJ(#2, #1) )
	SUBJ(#3, #1).

//Giovanni vuole passeggiare sulla sabbia.
| SC{ PP*, NP{?*,#1[last]}, ?*, FV{?*,#2[modal,last]}}, IV{?*[prep:~],#3[last]}|
	if(#1[number]:#2[number])	
	SUBJ(#3,#1).

//Giovanni decide di passeggiare sulla sabbia.
| SC{PP*, NP{?*,#1[nom:~,last]}, ?*, FV{?*,#3[last]}}, ADVP*, IV{prep,#5[last]}, ?*{adj,~NFV} |
	if(#1[number]:#3[number])
	SUBJ(#5,#1).

//La politica linguistica del regime SI INTEGRA in una politica di repressione.
// 11.9.2008 (SM):
// Ma ieri la folla aveva campo libero e si é sfogata contro ogni simbolo del potere centrale.
//| SC{ (PP), NP{?*,#1[pron:~,last]}, ?*, RFV{?*,#2[last]}} |
//| SC{NP{?*, #1[pron:~, last:+]}, ?*, RFV{?*, #2[last:+, form:~fessere]}} |
// 12.9.2008 (SM):
// Anche Dashamir Shehi, ex vice primo ministro, un tempo uomo del Presidente in città, attacca il Governo e si oppone a misure di emergenza.
| SC{?*[fv:~, np:~], NP{?*, #1[pron:~, last:+]}, ?*[coord:~], RFV{?*, #2[last:+, form:~fessere]}} |
	if ( #1[number]:#2[number] )	
	SUBJ(#2, #1).

// 11.9.2008 (SM):
// Paolo si è lavato le mani.
| SC{?*[fv:~, np:~], NP{?*, #1[pron:~, last:+]}, ?*[np:~], RFV{?*, #2[last:+, form:fessere]}},
 NFV{?*, #3[form:~fessere, pastpart:+, last:+]} |
	if ( #1[number]:#2[number] & #1[gender]:#3[gender] )
	SUBJ(#3, #1).

//REFL & PRONOMINAL Forms
//Io MI INTEGRO in una politica di repressione.
| SC{ (PP), NP{?*,#1[pron,last]}, ?*, RFV{#2[nom:~,acc:+,dat:+],#3[last]}} |
	if(#1[number]:#3[number]
	&#1[person]:#3[person]
	&#1[person]:#2[person]
	&#1[number]:#2[number]
	&#2[person]:#3[person]
	&#2[number]:#3[number])	
	SUBJ(#3,#1).

//Rumore proveniente dall'area di uscita delle copie.     SUBJ Rumore, proveniente   16/05
|NP{?*,#3[last]}, NFV{?*,#5[prespart,last]}|
	SUBJ(#5,#3).

//// Giovanni mangia e beve
////| NP{?*,#1[last]}, ?*, FV{#2[last]}}, ADVP*,  conj#3[coord], ADVP*,  SC{FV{#4[last]}}|
//// 20.8.2008 (SM):
//// Giovanni mangia e beve
//| SC{?*[fv:~, np:~], NP{?*, #1[last]}, ?*, FV{#2[last]}}, ADVP*, conj#3[coord], ADVP*, SC{FV{#4[last]}} |
////	SUBJ(#2,#1),	
//	if ( SUBJ(#2, #1) )
//	COORDITEMS(#2, #4),
//	SUBJ(#4, #1).

// 12.9.2008 (SM):
// In Albania intanto la situazione del Paese sta precipitando verso la guerra civile.
| SC{?*[fv:~, np:~], NP{?*, #1[last:+]}, ?*[coord:~], FV{?*, #2[form:fstare, last:+]}},
 GV{?*, #3[gerund:+, last:+]} |
	if ( #1[number]:#2[number] & ^SUBJ(#2, #1) )
	SUBJ(#3, #1).

// 1.10.2008 (SM):
// c'è una persona competente, ci sono delle persone
| SC{?*, FV{?*, pron[loc:+], verb#1[last:+, form:fessere]}}, (advp),
 NP{?*, noun#2[last:+]}; PP{?*, NP{?*, noun#2[last:+]}} |
	SUBJ(#1, #2).

// 6.10.2008 (SM):
// ce ne sarà probabilmente un secondo
| SC{?*, NP{?*, pron[loc:+]}, NP{pron[ne:+]}, FV{?*, verb#1[last:+, form:fessere]}}, (advp),
 NP{?*, noun#2[last:+]}; PP{?*, NP{?*, noun#2[last:+]}} |
	SUBJ(#1, #2).

// 2.10.2008 (SM):
// Qui c'era la base di Pasha Liman affittata ai russi che tenevano alla fonda i loro sommergibili.
| BG{conjche#1[last:+]}, (PP; ADVP; AP)*, FV{?*[refl:~], #2[last:+]}; RFV{?*, #2[last:+]} |
	SUBJ(#2, #1).

// 3.10.2008 (SM):
// Ma se sarà messo con le spalle al muro dai clan che si stanno scontrando nel suo partito...
| SC{BG{conjche#1[last:+]}, (PP; ADVP; AP)*, FV{?*, #2[form:fstare, last:+]};
 RFV{?*, #2[form:fstare, last:+]}}, GV{?*, #3[gerund:+, last:+]} |
	if ( ^SUBJ(#2, #1) )
	SUBJ(#3, #1).


// postposed subjects:

// 9.10.2008 (SM):
// Finora abbiamo evitato di proposito lo scontro - dice il ministro degli Esteri Sheu -.
| SC{?*, FV{?*, verb#1[last:+, dicendi:+]}},
 NP{?*, noun#2[last:+, people:+]; noun#2[last:+, firstname:+]; noun#2[last:+, lastname:+]} |
	SUBJ(#1, #2).

// 9.10.2008 (SM):
// Finora abbiamo evitato di proposito lo scontro - ha detto il ministro degli Esteri Sheu -.
| NFV{?*, verb#1[last:+, dicendi:+]},
 NP{?*, noun#2[last:+, people:+]; noun#2[last:+, firstname:+]; noun#2[last:+, lastname:+]} |
	SUBJ(#1, #2).

// verb: peut être erg, intrans, pronom, pas de NP avant, et NP après -> SUBJ
// 6.11.2008 (SM):
// Qui non si trova più un seguace del Partito democratico
| SC{?*, FV{?*, verb#1[last:+, trans:~, atts:~, atto:~]}; RFV{?*, verb#1[last:+, trans:~, atts:~, atto:~]}},
 (ADVP), NP{?*, noun#2[last:+]} |
	if (
	   ~SUBJ(#1, #10)
	 & #1[number]:#2[number]
	 & #1[person]:#2[person]
	 )
	SUBJ(#1, #2).

// 10.3.2009 (SM):
// Dalla rabbia dei valonesi non si salva niente e nessuno .
| SC{?*, FV{?*, verb#1[last:+, trans:~, atts:~, atto:~]}; RFV{?*, verb#1[last:+, trans:~, atts:~, atto:~]}},
 (ADVP), NP{?*, pron#2[last:+]} |
	if (
	   ~SUBJ(#1, #10)
	 & #1[number]:#2[number]
	 )
	SUBJ(#1, #2).


// 6.11.2008 (SM):
// si é esaurito il filone aureo del contrabbando
| SC{?*, FV{?*, verb#3[last:+]}; RFV{?*, verb#3[last:+]}}, NFV{verb#1[last:+, trans:~, atts:~, atto:~]},
 (ADVP), NP{?*, noun#2[last:+]} |
	if (
	   ~SUBJ(#1, #10)
	 & DOMIN(#3, #1)
	 & #1[number]:#2[number]
	 & #1[gender]:#2[gender]
	 )
	SUBJ(#1, #2).

// little corrections:

// 26.9.2008 (SM):
// io mi sono mangiato
if ( ^SUBJ(#1, #2) & DOMIN(#1, #3) )
	SUBJ(#3, #2).

// 10.10.2008 (SM):
// Sigrid sta mangiando la torta
if ( ^SUBJ(#1, #2) & STARE(#1, #3) )
	SUBJ(#3, #2).


//------------------------------------------------------------------------------------------------------
// SUBJ_PASSIVE
// 10.9.2008 (SM), reorganisation of all SUBJ_PASSIVE rules:
// 31.10.2008 (SM), inclusion of ERG info

// Io sono promosso
| SC{NP{?*, #1[pron:+, nom:+]}, ?*, FV{?*, #2[form:fessere, last:+]}},
 NFV{?*, #4[form:~fessere, pastpart:+, last:+, trans:+, erg:~]} |
	if (
	  #1[person]:#2[person]
	& #1[number]:#2[number]
	& #2[number]:#4[number]
	& #1[gender]:#4[gender]
	& #1[number]:#4[number]
	& ^SUBJ(#4, #1)
	)
	SUBJ_PASSIVE(#4, #1).

// la piazza è dedicata
| SC{punct*, NP{?*, noun#1[last:+]}, ?*, FV{?*, #2[form:fessere, last:+]}},
 NFV{?*, #3[form:~fessere, pastpart:+, last:+, trans:+, erg:~]} |
	if (
	  #1[person]:#2[person]
	& #1[number]:#2[number]
	& #2[number]:#3[number]
	& #1[gender]:#3[gender]
	& #1[number]:#3[number]
	& ^SUBJ(#3, #1)
	)
	SUBJ_PASSIVE(#3, #1).

// La relazione sarà discussa domani da un gruppo di esperti.
// ..., la nostra linea politica é rappresentata dal Movimento popolare del Kosovo ...
| SC{punct*, NP{?*, noun#1[last:+]}, ?*, FV{?*, #2[form:fessere, last:+]}},
 NFV{?*, #3[form:~fessere, pastpart:+, last:+, trans:+, erg:+]}, (ADVP), PP{prep[lemma:da]} |
	if (
	  #1[person]:#2[person]
	& #1[number]:#2[number]
	& #2[number]:#3[number]
	& #1[gender]:#3[gender]
	& #1[number]:#3[number]
	& ^SUBJ(#3, #1)
	)
	SUBJ_PASSIVE(#3, #1).

// Io sono stato promosso
| SC{NP{?*, #1[pron:+, nom:+]}, ?*, FV{?*, #2[form:fessere, ind:+, last:+]}},
 NFV{?*, #3[form:fessere, pastpart:+, last:+]}, NFV{?*, #5[form:~fessere, pastpart:+, last:+, trans:+]} |
	if(
	  #1[number]:#2[number]
	& #1[number]:#3[number]
	& #1[gender]:#3[gender]
	& #2[number]:#3[number]
	& #2[number]:#5[number]
	& #1[number]:#5[number]
	& #1[gender]:#5[gender]
	& #3[number]:#5[number]
	& #3[gender]:#5[gender]
	& ^SUBJ(#3, #1)
	)
	SUBJ_PASSIVE(#5, #1).
//	DOMIN_PASSIVE(#2, #3),
//	DOMIN_PASSIVE(#3, #5).

// Paolo è stato lodato.
| SC{NP{?*, noun#1[last:+]}, ?*, FV{?*, #2[form:fessere, ind:+, last:+]}},
 NFV{?*, #3[form:fessere, pastpart:+, last:+]}, NFV{?*, #5[form:~fessere, pastpart:+, last:+, trans:+]} |
	if(
	  #1[number]:#2[number]
	& #1[number]:#3[number]
	& #1[gender]:#3[gender]
	& #2[number]:#3[number]
	& #2[number]:#5[number]
	& #1[number]:#5[number]
	& #1[gender]:#5[gender]
	& #3[number]:#5[number]
	& #3[gender]:#5[gender]
	& ^SUBJ(#3, #1)
	)
	SUBJ_PASSIVE(#5, #1).
//	DOMIN_PASSIVE(#2, #3),
//	DOMIN_PASSIVE(#3, #5).

// Paola e Luisa sono state promosse.
//| SC{NP{?*, noun#1[last:+]}, conj[coord:+], NP{?*, noun#2[last:+]}, ?*, FV{?*, #3[form:fessere, last:+]}},
// NFV{?*, #4[form:fessere, pastpart:+, last:+]}, NFV{?*, #5[form:~fessere, pastpart:+, last:+, trans:+]} |
| SC{NP{?*, noun#1[last:+]}, ?*, conj[coord:+], NP{?*, noun#2[last:+]}, ?*, FV{?*, #3[form:fessere, last:+]}},
 NFV{?*, #4[form:fessere, pastpart:+, last:+]}, NFV{?*, #5[form:~fessere, pastpart:+, last:+, trans:+]} |
	if (
//	  #3[number]:#4[number]
	  #4[number]:#5[number]
	& #4[gender]:#5[gender]
//	& SUBJ$1(#3, #1)
	& ^SUBJ(#3, #2)
	)
	SUBJ_PASSIVE(#5, #2),
	SUBJ_PASSIVE(#5, #1).
//	DOMIN_PASSIVE(#3, #4),
//	DOMIN_PASSIVE(#4, #5).

// Io vengo promosso; 
| SC{NP{?*, #1[pron:+, nom:+]}, ?*, FV{?*, #2[lemma:venire, last:+]}},
 NFV{?*, #3[form:~fessere, pastpart:+, last:+, trans:+]} |
	if(
	  #1[number]:#2[number]
	& #1[number]:#3[number]
	& #1[gender]:#3[gender]
	& #2[number]:#3[number]
	& ^SUBJ(#2, #1)
	)
	SUBJ_PASSIVE(#3, #1).

// la macchina non viene accettata.
| SC{NP{?*, noun#1[last:+]}, ?*, FV{?*, #2[lemma:venire, last:+]}},
 NFV{?*, #3[form:~fessere, pastpart:+, last:+, trans:+]} |
	if (
	  #1[number]:#2[number]
	& #1[number]:#3[number]
	& #1[gender]:#3[gender]
	& #2[number]:#3[number]
	& ^SUBJ(#2, #1)
	)
	SUBJ_PASSIVE(#3, #1).

// 7.10.2008 (SM):
// alcune 'piramidi' non verranno travolte
| NP{?*, noun#1[last:+]}, ?*, SC{?*, FV{?*, #2[lemma:venire, last:+]}},
 NFV{?*, #3[form:~fessere, pastpart:+, last:+, trans:+]} |
	if (
	  #1[number]:#2[number]
	& #2[number]:#3[number]
	& #1[gender]:#3[gender]
	)
	SUBJ_PASSIVE(#3, #1).

// Il giornalista dichiara di essere stato riconosciuto.
| SC{NP{?*, noun#1[last:+]}, FV{?*, #2[form:~fessere, last:+]}}, IV{?*, #3[form:fessere, last:+]},
 NFV{?*, #4[form:fessere, pastpart:+, last:+]}, NFV{?*, #5[form:~fessere, pastpart:+, last:+, trans:+]} |
	if (
	  #1[number]:#2[number]
	& #1[number]:#4[number]
	& #4[number]:#5[number]
	& #1[gender]:#4[gender]
	& SUBJ(#2, #1)
	)
//	DOMIN_PASSIVE(#2, #3),
//	DOMIN_PASSIVE(#3, #4),
//	DOMIN_PASSIVE(#4, #5),
	SUBJ_PASSIVE(#5, #1).

// 7.10.2008 (SM):
// E per motivi di ordine pubblico forse saranno salvate le altre finanziarie in difficoltà.
| SC{?*[bg:~], FV{?*, #1[form:fessere, last:+]}},
 NFV{?*, #2[form:~fessere, pastpart:+, last:+, trans:+, erg:~]}, (PP; ADVP)*, NP{?*, noun#3[last:+]} |
	if (
	  #1[number]:#2[number]
	& #2[number]:#3[number]
	& #2[gender]:#3[gender]
	)
	SUBJ_PASSIVE(#2, #3).

// 7.10.2008 (SM):
// E per motivi di ordine pubblico forse sono state salvate le altre finanziarie in difficoltà.
| SC{?*[bg:~], FV{?*, #1[form:fessere, last:+]}}, NFV{?*, #4[form:fessere, pastpart:+, last:+]}, (conj; ADVP)*,
 NFV{?*, #2[form:~fessere, pastpart:+, last:+, trans:+]}, (PP; ADVP)*, NP{?*, noun#3[last:+]} |
	if (
	  #1[number]:#2[number]
	& #2[number]:#3[number]
	& #2[number]:#4[number]
	& #2[gender]:#3[gender]
// 4.11.2008 (SM):
// La ragazza è stata chiamata Maria.
	& ~SUBJ_PASSIVE(#2, #10)
	)
	SUBJ_PASSIVE(#2, #3).

// 7.10.2008 (SM):
// Tutte le operazioni militari e di polizia sono dirette da Bashkim Gazidede, comandante dei Servizi segreti.
| NP{?*, noun#3[last:+]}, (PP; ADVP; AP; conj)*, SC{?*[bg:~], FV{?*, #1[form:fessere, last:+]}},
 NFV{?*, #2[form:~fessere, pastpart:+, last:+, trans:+, erg:~]} |
	if (
	  #1[number]:#2[number]
	& #2[number]:#3[number]
	& #2[gender]:#3[gender]
	)
	SUBJ_PASSIVE(#2, #3).

// 7.10.2008 (SM):
// in 8 delle 14 città che erano conquistate dal cartello dei partiti di opposizione
| SC{BG{conjche#1[last:+]}, (PP; ADVP; AP)*, FV{?*[refl:~], #2[form:fessere, last:+]}},
 NFV{?*, #3[form:~fessere, pastpart:+, last:+, trans:+, erg:~]} |
	if ( ^SUBJ(#3, #1) & #2[number]:#3[number] )
	SUBJ_PASSIVE(#3, #1).

// 7.10.2008 (SM):
// in 8 delle 14 città che erano state conquistate dal cartello dei partiti di opposizione
| SC{BG{conjche#1[last:+]}, (PP; ADVP; AP)*, FV{?*[refl:~], #2[form:fessere, last:+]}},
 NFV{?*, #4[form:fessere, pastpart:+, last:+]}, NFV{?*, #3[form:~fessere, pastpart:+, last:+, trans:+]} |
	if ( ^SUBJ(#2, #1)
	& #2[number]:#3[number]
	& #3[number]:#4[number]
	& #3[gender]:#4[gender]
	)
	SUBJ_PASSIVE(#3, #1).

//------------------------------------------------------------------------------------------------------

// AGENT.
//past participle followed by agent complement
|?*, NFV{?*,#1[pastpart:+,last]}, PP#3{prep<lemma:da>}, NP{?, #5[last]} |
	if(head(#10, #1)
	& head(#30, #3)
	&head(#50, #5)
	& ~VMOD(#1,#3))
	AGENT(#1, #5).

|?*, NFV#1{?*,#2[pastpart:+,last]}, PP#3{prep<lemma:da>, NP#4{?, #5[last]}} |
	if(head(#2, #1)
	&head(#5, #3)
	& ^VMOD(#1,#5)~ )
	AGENT(#2, #5).

//Il giornalista è stato aggredito da due uomini.
| SC{PP*, NP{?*,#1[last]}, ?*, FV{?*,#2[form:fessere,last]}}, NFV{?*,#3[form:fessere,pastpart:+,last]},NFV{?*,#5[pastpart:+,last]}, PP#7{prep[lemma:da]}  |
	if(head(#70, #7) )
	AGENT(#5, #70).

