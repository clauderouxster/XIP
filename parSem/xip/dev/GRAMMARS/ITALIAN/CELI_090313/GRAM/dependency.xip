// 7.11.2008 (SM):
// le persone che hanno mangiato, le persone che ho visto
if ( ^SUBJ(#1, #2) & RMOD_RELCL(#3, #1) & ( DOMIN(#4, #1) | STARE(#4, #1) )
 & ~(#4[number]:#3[number]) & ~(#4[person]:#3[person]) )
	OBJ(#1, #2).

// 7.11.2008 (SM):
// le persone che mangiano, le persone che vedo
if ( ^SUBJ(#1, #2) & RMOD_RELCL(#3, #1) & ~(#1[number]:#3[number]) & ~(#1[person]:#3[person])
 & ~DOMIN(#4, #1) & ~STARE(#4, #1) )
	OBJ(#1, #2).


// punctuation
// 5.11.2008 (SM):
// ' Dietro di me , diceva , c' é una persona competente ' . 
| punct#1[quote:+], ?*[quote:~], SC{?*, FV{?*, verb#2[last:+]}}; IV{?*, verb#2[last:+]}, ?*[quote:~],
 punct#3[quote:+] |
	if ( ~OPEN_QUOTES(#10, #1) & ~CLOSE_QUOTES(#10, #1) )
	OPEN_QUOTES(#2, #1),
	CLOSE_QUOTES(#2, #3).

// 5.11.2008 (SM):
// C' é una struttura costituita da militanti ' coperti ' da attività legali e un' altra illegale...
| punct#1[quote:+], ?*[quote:~], NFV{?*, verb#2[last:+]}, ?*[quote:~, nfv:~], punct#3[quote:+] |
	if ( ~OPEN_QUOTES(#10, #1) & ~CLOSE_QUOTES(#10, #1) )
	OPEN_QUOTES(#2, #1),
	CLOSE_QUOTES(#2, #3).

// 6.11.2008 (SM):
// ' La causa degli degli albanesi é la mia ' .
| SC{punct#1[quote:+], ?*[quote:~], FV{?*, verb#2[last:+]}}, ?*[quote:~], punct#3[quote:+] |
	if ( ~OPEN_QUOTES(#10, #1) & ~CLOSE_QUOTES(#10, #1) )
	OPEN_QUOTES(#2, #1),
	CLOSE_QUOTES(#2, #3).

// 5.11.2008 (SM):
// Questa é ancora ' terra di nessuno ' , sotto , sulla discesa che schiude l' orizzonte al al mare...
// 7.11.2008 (SM):
// Il giovane continua a proporre servizi, finalmente si vede affidare una serie di articoli " di costume ".
| punct#1[quote:+], ?*[quote:~], NP{?*, noun#2[last:+]}; AP{?*, adj#2[last:+]}; PP{?*, NP{?*, noun#2[last:+]}},
 ?*[quote:~], punct#3[quote:+] |
	if ( ~OPEN_QUOTES(#10, #1) & ~CLOSE_QUOTES(#10, #1) )
	OPEN_QUOTES(#2, #1),
	CLOSE_QUOTES(#2, #3).

// 5.11.2008 (SM):
// in particolare con quella di Meksude Kademi , la ' zingara ' .
| NP{?*, punct#1[quote:+], ?*[quote:~], noun#2[last:+]}; PP{?*, NP{?*, punct#1[quote:+], ?*[quote:~],
 noun#2[last:+]}}, ?*[quote:~], punct#3[quote:+] |
	if ( ~OPEN_QUOTES(#10, #1) & ~CLOSE_QUOTES(#10, #1) )
	OPEN_QUOTES(#2, #1),
	CLOSE_QUOTES(#2, #3).

// 7.11.2008 (SM):
// ha affermato Wojtyla, "l'ho fatto come parte della mia missione universale, e così deve essere visto ".
if ( OPEN_QUOTES$1(#1, #2) & CLOSE_QUOTES$2(#1, #3) & DOMIN(#1, #4) )
	^OPEN_QUOTES$1(#4, #2),
	^CLOSE_QUOTES$2(#4, #3).


// 5.11.2008 (SM):
// ' Dietro di me , diceva , c' é una persona competente ' .
| punct#1[comma:+]; SC{?*, punct#1[comma:+], FV{?*, verb#2[last:+]}}, ?*[comma:~],
 punct#3[comma:+]; SC{?*, punct#3[comma:+], ?*} |
	if ( ~OPEN_PARENTHETICAL(#10, #1) & ~CLOSE_PARENTHETICAL(#10, #1) )
	OPEN_PARENTHETICAL(#2, #1),
	CLOSE_PARENTHETICAL(#2, #3).

// 10.11.2008 (SM):
// E improvvisamente quell' irruzione , subito definita un esodo biblico , ...
| NP{?*, noun#1[last:+]}; PP{?*, NP{?*, noun#1[last:+]}}, ?*[comma:~, np:~, pp:~], punct#2[comma:+],
 ?*[comma:~], punct#3[comma:+]; SC{?*, punct#3[comma:+], ?*} |
	if ( ~OPEN_PARENTHETICAL(#10, #2) & ~CLOSE_PARENTHETICAL(#10, #2) )
	OPEN_PARENTHETICAL(#1, #2),
	CLOSE_PARENTHETICAL(#1, #3).

// 7.11.2008 (SM):
// le ambiguità possono essere di tipo lessicale ( se differenti categorie sono associate ad una parola )
| NP{?*, noun#1[last:+]}; PP{?*, NP{?*, noun#1[last:+]}}, ?*[paren:~, np:~, pp:~],
 SC{?*, punct#2[paren:+], ?*}, ?*[paren:~], punct#3[paren:+] |
	if ( ~OPEN_PARENTHETICAL(#10, #2) & ~CLOSE_PARENTHETICAL(#10, #2) )
	OPEN_PARENTHETICAL(#1, #2),
	CLOSE_PARENTHETICAL(#1, #3).

// 7.11.2008 (SM):
// E lo studio é stato confermato dal direttore del servizio centrale operativo ( Sco ) della polizia.
// Ma tuttavia sta lì con la Banca d'Italia che sempre interviene ( e lo ha fatto anche ieri ) ...
| NP{?*, noun#1[last:+]}; PP{?*, NP{?*, noun#1[last:+]}}, ?*[paren:~, np:~, pp:~],
 (SC{?*[np:~, pp:~]}), ?*[paren:~, np:~, pp:~], punct#2[paren:+], ?*[paren:~], punct#3[paren:+] |
	if ( ~OPEN_PARENTHETICAL(#10, #2) & ~CLOSE_PARENTHETICAL(#10, #2) )
	OPEN_PARENTHETICAL(#1, #2),
	CLOSE_PARENTHETICAL(#1, #3).

// 24.11.2008 (SM):
// A Valona si é rotto un patto tra la criminalità e questo Governo che aveva consentito tutti i traffici
// illegali - dalla droga, alla prostituzione, alle armi e ai clandestini - che arricchivano le finanziarie . 
| NP{?*, noun#1[last:+]}; PP{?*, NP{?*, noun#1[last:+]}}, ?*[punct:~, np:~, pp:~], punct#2[lemma:-],
 ?*[lemma:~-], ?[num:~], punct#3[lemma:-]; SC{punct#3[lemma:-]} |
	if ( ~OPEN_PARENTHETICAL(#10, #2) & ~CLOSE_PARENTHETICAL(#10, #2) )
	OPEN_PARENTHETICAL(#1, #2),
	CLOSE_PARENTHETICAL(#1, #3).

// 10.11.2008 (SM):
// Anni fa , ha ricordato il direttore dello Sco , anche in Campania c' erano ...
if ( OPEN_PARENTHETICAL$1(#1, #2) & CLOSE_PARENTHETICAL$2(#1, #3) & DOMIN(#1, #4) )
	^OPEN_PARENTHETICAL$1(#4, #2),
	^CLOSE_PARENTHETICAL$2(#4, #3).

// 24.11.2008 (SM):
// " Nemmeno lo 007 dell' Ufficio indagini l' ha visto , però sono stato sospeso per trenta giorni " . 
| ?{?*, verb#1[last:+]}; SC{?*, FV{?*, verb#1[last:+]}}, (ADVP; AP)*, SC{punct#2[end:~, quote:~]} |
	if ( ~OPEN_PARENTHETICAL(#10, #2) & ~CLOSE_PARENTHETICAL(#11, #2) )
	SEPARATOR(#1, #2).

// 24.11.2008 (SM):
| NP{?*, #1[last:+]}; PP{?*, NP{?*, #1[last:+]}}, (ADVP; AP)*, SC{punct#2[end:~, quote:~]} |
	if ( ~OPEN_PARENTHETICAL(#10, #2) & ~CLOSE_PARENTHETICAL(#11, #2) )
	SEPARATOR(#1, #2).

// 24.11.2008 (SM):
// Il malloppo é sparito così , sulle onde dell' Adriatico .
| ?{?*, verb#1[last:+]}; SC{?*, FV{?*, verb#1[last:+]}}, (ADVP; AP)*, punct#2[end:~, quote:~] |
	if ( ~OPEN_PARENTHETICAL(#10, #2) & ~CLOSE_PARENTHETICAL(#11, #2) )
	SEPARATOR(#1, #2).

// 24.11.2008 (SM):
// e si é sfogata contro ogni simbolo del potere centrale , inviperita per i morti e i feriti
| NP{?*, #1[last:+]}; PP{?*, NP{?*, #1[last:+]}}, (ADVP; AP)*, punct#2[end:~, quote:~] |
	if ( ~OPEN_PARENTHETICAL(#10, #2) & ~CLOSE_PARENTHETICAL(#11, #2) )
	SEPARATOR(#1, #2).


// 9.10.2008 (SM):
// skip arguments of DOMIN and rename it AUX_TENSE
if ( ^DOMIN(#1, #2) )
	AUX_TENSE(#2, #1).

