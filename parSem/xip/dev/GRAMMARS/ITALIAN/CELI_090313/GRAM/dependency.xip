// PREDCOMPL_SUBJ
// 3.11.2008 (SM):
// Nelle grandi città il traffico è intenso
| SC{?*, FV{?*, verb#1[last:+, atts:+]}}; NFV{?*, verb#1[last:+, atts:+]}, AP{?*, #2[last:+]} |
	if ( #1[number]:#2[number] & ~OBJ(#1, #10) )
	PREDCOMPL_SUBJ(#1, #2).

// 4.11.2008 (SM):
// è stata chiamata Maria
// 28.11.2008 (SM):
// è solo una copertura per riciclare
| SC{?*, FV{?*, verb#1[last:+, atts:+]}; RFV{?*, verb#1[last:+, atts:+]}}; NFV{?*, verb#1[last:+, atts:+]},
 (ADVP), NP{?*, #2[last:+]} |
	if ( #1[number]:#2[number] & ~NMOD(#2, #10) & ~SUBJ(#1, #2) & ~OBJ(#1, #2) )
	PREDCOMPL_SUBJ(#1, #2).

// 5.11.2008 (SM):
// Io mi chiamo Maria
| SC{?*, FV{?*, verb#1[last:+, atts:+]}; RFV{?*, verb#1[last:+, atts:+]}}; NFV{?*, verb#1[last:+, atts:+]},
 NP{?*, #2[last:+]} |
	if ( #1[number]:#2[number] & ~NMOD(#2, #10) & ~SUBJ(#1, #2) & ^OBJ(#1, #2[prop:+]) )
	PREDCOMPL_SUBJ(#1, #2).

// 4.11.2008 (SM):
// è stata chiamata Maria
| SC{?*, FV{?*, verb#1[last:+, atts:+]}; RFV{?*, verb#1[last:+, atts:+]}}; NFV{?*, verb#1[last:+, atts:+]},
 NP{?*, #2[last:+]} |
	if ( ^SUBJ_PASSIVE(#1, #2) & PREDCOMPL_SUBJ(#1, #2) )
	~.

// 4.11.2008 (SM):
// la tesi è stata dimostrata falsa
| SC{?*, FV{?*, verb[last:+, form:fessere]}; RFV{?*, verb[last:+, form:fessere]}},
 (NFV{?*, verb[last:+, form:fessere]}), NFV{?*, verb#1[last:+]}, AP{?*, #2[last:+]} |
	if ( #1[number]:#2[number] & ~PREDCOMPL_SUBJ(#1, #2) )
	PREDCOMPL_SUBJ(#1, #2).

// 4.11.2008 (SM):
// Giovanni si è dimostrato un professore.
| SC{?*, FV{?*, verb[last:+, form:fessere]}; RFV{?*, verb[last:+, form:fessere]}},
 (NFV{?*, verb[last:+, form:fessere]}), NFV{?*, verb#1[last:+]}, NP{?*, #2[last:+]} |
	if ( #1[number]:#2[number] & ~PREDCOMPL_SUBJ(#1, #2) & ~NMOD(#2, #10) & ~SUBJ(#1, #2) )
	PREDCOMPL_SUBJ(#1, #2).


