// OBJ. PEOPLE
//Il cane (nero/ di Michele) segue Roberto Donadoni perché regala regali.
| SC{?*, FV{?*, #2[trans:+, last:+, form:~fessere]}}, ?*[punct:~, iv:~, np:~, nfv:~],
 NP{?*, #4[noun:+, people:+, last:+]} |
// 9.10.2008 (SM):
// Finora abbiamo evitato di proposito lo scontro - dice Luca Dini.
// Lo fa Dashim Shehi, presidente della commissione Industria.
	if ( ~SUBJ(#2[dicendi:+], #4) & ~OBJ(#2, #4[pron:+]) )
	OBJ(#2, #4).

// OBJ.
//Il cane (nero/ di Michele) segue il (suo padrone).
| SC{?*, FV{?*, #2[trans:+, last:+, form:~fessere]}}, ?*[punct:~, iv:~, np:~, nfv:~],
 NP{?*, #4[last:+, people:~]} |
// 9.10.2008 (SM):
// Lo fa Dashim Shehi, presidente della commissione Industria.
	if ( ~SUBJ(#2[dicendi:+], #4) & ~OBJ(#2, #10[pron:+]) )
	OBJ(#2, #4).

// OBJ. PEOPLE
//Il cane (nero/ di Michele) segue Roberto Donadoni perché regala regali.
| SC{?*, FV{?*, #2[trans:+, last:+, form:~fessere]}}, ?*[punct:~, iv:~, np:~, nfv:~],
 SC{?*, NP{?*, #4[noun:+, people:+, last:+]}, ?*} |
	OBJ(#2, #4).

// 12.9.2008 (SM):
// Qui non si trova più un seguace del Partito democratico.
| SC{?*, RFV{?*, #1[trans:+, last:+, form:~fessere]}}, ?*[punct:~, iv:~, np:~, nfv:~], NP{?*, #2[last:+]} |
//	if ( ~OBJ[refl:+](#1, #10) )
	OBJ(#1, #2).

// 12.9.2008 (SM):
// Qui non si è trovato più un seguace del Partito democratico.
| SC{?*, RFV{?*, #1[last:+, form:fessere]}}, NFV{?*, #3[trans:+, last:+, pastpart:+]},
 ?*[punct:~, iv:~, np:~, nfv:~], NP{?*, #2[last:+]} |
//	if ( ~OBJ[refl:+](#3, #10) )
	OBJ(#3, #2).

// 26.9.2008 (SM):
// abbiamo evitato di proposito lo scontro
// la nave ha quindi raggiunto la fregata
//Il Presidente HA PRESO la scelta migliore
//| SC{?*, FV{?*, #2[last:+, form:~fessere]}}, NFV{?*, #3[trans:+, last:+, pastpart:+]}, NP{?*, #4[last:+]} |
| SC{?*, FV{?*, #2[last:+, form:~fessere]}}, (conj), NFV{?*, #3[trans:+, last:+, pastpart:+]}, (advp; pp)*,
 NP{?*, #4[last:+]} |
// 9.10.2008 (SM):
	if ( ~SUBJ(#3[dicendi:+], #4) )
	OBJ(#3, #4).

//| IV{?*, #2[trans:+, last:+, form:~fessere]}, ?*[conj:~, iv:~, np:~, punct:~], NP{?*, #3[tokenend:~, last:+]} |	
| IV{?*, #2[trans:+, last:+, form:~fessere]}, ?*[conj:~, iv:~, np:~, punct:~], NP{?*, #3[last:+]} |	
	if ( ~OBJ_COORD(#2, #3) )
	OBJ(#2, #3).

//Giovanni vuole passeggiare sulla sabbia.
| SC{NP{?*, #1[last:+]}, ?*, FV{?*, #2[last:+]}}, IV{?*[prep:~], #3[trans:+, last:+]} |
	OBJ(#2, #3).

//Ringraziando Tizio Caio   09-06
| ?*, GV{?*, #1[trans:+, last:+]}, ADVP*, NP{?*, #3[noun:+, prop:+, people:+, last:+]}, ?* |
	if ( ~OBJ(#1, #3) )
	OBJ(#1, #3).

//Utilizzando la password   16/05
//| ?*, GV{?*, #1[trans:+, last:+]}, ADVP*, NP{?*, #3[last:+]}, ?* |
// 26.9.2008 (SM):
// calcolando anche i tantissimi operatori economici delle 500 imprese italiane che fanno la spola con l'Italia.
| GV{?*, #1[trans:+, last:+]}, (conj), (ADVP; PP)*, NP{?*, #2[last:+]} |
	if ( ~OBJ(#1, #2) & ~EMPTYCOMPL(#1, #2) )
	OBJ(#1, #2).

// 26.9.2008 (SM):
// io ti ho mangiato
if ( ^OBJ(#1, #2) & DOMIN(#1, #3) )
	OBJ(#3, #2).

//{ADVP{Come} IV{abilitare} Conj{o} IV{disabilitare} NP{il conto} NP{Auditron} AP{esterno} ( NP{servizi} AP{contabilitÃ} ) .}
| ?*, IV{?*, #1[trans:+, last:+]}, conj#3[coord], IV{?*, #5[trans:+, last:+]}, NP{?*, #7[last:+]}, ?* |
	if (
	  ^OBJ(#5, #7)
	& ~V_COORD_OBJ(#1, #7)
	)
	V_COORD_OBJ(#1, #7),
	V_COORD_OBJ(#5, #7).


// 8.10.2008 (SM):
// dice di andare allo stadio
| SC{?*, FV{?*, verb#1[subinf:+]}; RFV{?*, verb#1[subinf:+]}}, IV{?*, verb#2[inf:+]} |
	OBJ[sentential=+](#1, #2).

// 8.10.2008 (SM):
// ha detto di andare allo stadio
| NFV{?*, verb#1[subinf:+]}, IV{?*, verb#2[inf:+]} |
	OBJ[sentential=+](#1, #2).

// 8.10.2008 (SM):
// ricordando di andare allo stadio
| GV{?*, verb#1[subinf:+]}, IV{?*, verb#2[inf:+]} |
	OBJ[sentential=+](#1, #2).

// 8.10.2008 (SM):
// dice di andare allo stadio
| IV{prep#1[lemma:di]; prep#1[lemma:a], ?*, verb#2[inf:+]} |
	if ( OBJ[sentential:+](#10, #2) )
	CONNECT(#2, #1).

// 8-9.10.2008 (SM):
// dice che andranno allo stadio, dice quante persone sono venute
| SC{?*, FV{?*, verb#1[subfin:+]}}, SC{BG{conjche[lemma:che]; conj[lemma:come]}; NP{AP{adj[lemma:quanto]}},
 ?*[fv:~], FV{?*, verb#2[last:+]}} |
	OBJ[sentential=+](#1, #2).

// 8-9.10.2008 (SM):
// ha detto che andranno allo stadio, ha detto quante persone sono venute
| NFV{?*, verb#1[subfin:+]}, SC{BG{conjche[lemma:che]; conj[lemma:come]}; NP{AP{adj[lemma:quanto]}},
 ?*[fv:~], FV{?*, verb#2[last:+]}} |
	OBJ[sentential=+](#1, #2).

// 8-9.10.2008 (SM):
// ricordando che andranno allo stadio, ricordando quante persone sono venute
| GV{?*, verb#1[subfin:+]}, SC{BG{conjche[lemma:che]; conj[lemma:come]}; NP{AP{adj[lemma:quanto]}},
 ?*[fv:~], FV{?*, verb#2[last:+]}} |
	OBJ[sentential=+](#1, #2).

// 8.10.2008 (SM):
// dice che andranno allo stadio
| SC{BG{conjche#1; conj#1[rel:+]}, ?*[fv:~], FV{?*, verb#2[last:+]}} |
	if ( OBJ[sentential:+](#10, #2) )
	CONNECT(#2, #1).

// 9.10.2008 (SM):
// Finora abbiamo evitato di proposito lo scontro - dice il ministro degli Esteri Sheu - .
| SC{?*, FV{?*, verb#1[last:+]}}, ?*, (punct), SC{?*, FV{?*, verb#2[last:+, dicendi:+, subfin:+]}};
 NFV{verb#2[last:+, dicendi:+, subfin:+]} |
	if ( ~DOMIN(#1, #2) )
	OBJ[sentential=+](#2, #1).

// 9.10.2008 (SM):
if ( ^OBJ(#1, #2) & DOMIN(#1, #3) )
	OBJ(#3, #2).

// 9.10.2008 (SM):
if ( ^OBJ(#1, #2) & DOMIN(#2, #3) )
	OBJ(#1, #3).

