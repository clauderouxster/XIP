POS:

/*****************************************
        BASIC CATEGORIES
*****************************************\


 top = [cat=0].      /acts as root to chunking trees\
 
 adja = [adja=+, adj=+, attr=+, verbal=-].
 adjp = [adjp=+, adj=+, pred=+ ].
 adv = [adv=+,verbal=-].
 conj = [conj=+,verbal=-].
 det = [det=+,verbal=-].
 fm = [fm=+,verbal=-].
 itj = [itj=+,verbal=-].
 noun = [noun=+,verbal=-].
 num = [num=+,verbal=-].
padj = [padj=+,verbal=-].
 ptcl = [ptcl=+,verbal=-].
// pref = [pref=+].
 prep = [prep=+,verbal=-].
postp = [postp=+,verbal=-].
 pron = [pron=+,verbal=-]. 
 punct = [punct=+,verbal=-].
 trunc = [trunc=+,verbal=-].
 verb = [verb=+,verbal=+].
 symbol = [symbol=+,verbal=-].

