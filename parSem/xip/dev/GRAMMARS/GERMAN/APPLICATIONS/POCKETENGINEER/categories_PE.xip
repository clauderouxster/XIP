POS:

// Versionnumber of a printer or software
version = [noun=+].

// PE units
PEU = [peu=+].
PEUNIT = [peunit=+].
PENORM = [penorm=+].
PESURF = [pesurf=+].
// for (english) text of messages
MLD = [mld=+].
// combination of numbers
nums = [num=+].
