Functions:

ROOT, 			// tree root -- main element of the sentence, verb

ADJMOD,			// modifier of an adjectiv
ARTEFACT,		// *NAMED ENTITY*: document, product, ...
AUXIL,			// vorher: VAUX,
CONNECT,			// connector (conjunction, adverbial, ...)
COORD,			// coordinated items (heads)
COORD1,			// coordinated items (full nps, pps, ...)
COORD2,			// coordinated items (unordered, full nps, pps, ...)
DATE,			// *NAMED ENTITY*: date expression	17. Mai
DETERM,			// determiner of a noun
EXPL,			// expletive "es" (es sind viele da).
HEAD,			// head of a phrase (NP, AP or PP)
LOC,		 	// *NAMED ENTITY*: location
MODAL,			// vorher: VMOD,
NEGAT,			// negation
NMOD,			// noun modifier 	
NMOD2,			// indirect noun modifier (e.g. "schöne (Tische und) Bänke" )
NUMMOD,			// modifier of a numeral
OBJ,			// object (head only)
OBJ1,			// object (full NP)
ORG,			// *NAMED ENTITY*: organisation
PERSON, 			// *NAMED ENTITY*: person		Prof. Dr. Hugoa Maier
PLINK,			// prepositional link (between prep and head).
PRED,			// predicate
PREDET,			// pre-modifier of a determiner (solch [ein])
REFLEX,			// reflexiv verbs
SUBJ, 			// syntactic subject (head only)
SUBJ1,			// syntactic subject (full NP)
THEMA,			// "subject" of verbless sentences
TIME,			// *NAMED ENTITY*: time expression	17:20 Uhr
TRUNC,			// compound part
VFIN,			// finite verb
VMAIN,			// main verb (not auxiliary or modal)
VMOD,			// modifier of the verb
VPREF,			// verb with prefix
VSUB,			// auxiliaries, modals or other verbs that subcategorize verbs


