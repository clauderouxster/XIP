Functions:

ROOT, 			// tree root -- main element of the sentence, verb

SUBJ, 			// syntactic subject
//SUBJ_COORD, 		// syntactic subjects that are coordinated
//SUBJ_BG, 		// syntactic subject of a subordinated clause
//SUBJ_PASS,		// passive subject
THEMA,			// "subject" of verbless sentences

OBJ,			// objects
//OBJ_ACC,		// accusative (direct) object
//OBJ_DAT,		// dative (indirect) object
//OBJ_GEN,		// genitive (indirect) object
//OBJ_SENT,		// subordinate clause object

MOD-GEN,  		// genitive modifier: Peters [Freund], meines Vaters [Auto]

VMAIN,
AUXIL,			// vorher: VAUX,
MODAL,			// vorher: VMOD,
VPREF,			// verb with prefix
VMOD,			// modifier of the verb
VMOD2,			// modifier of a second verb
VPAST,			// participle
VPASSIVE,		// passive verb
NEGAT,			// for negated verbs
REFLEX,			// reflexiv verbs
PRED,			// predicates

NMOD,			// modifier of a noun
NMOD2,			// modifier of a second noun
COORDITEMS,		// coordinated nouns
COORDITEMS-A,		// coordinated adjectives
COORDITEMS-P,		// coordinated pps
COORDITEMS-SC,		// coordinated verbs
COORD,			// noun and conjunction

ADJMOD,			// modifier of an adjectiv

POSS,			// to mark possessives

DETERM,			// determiner of a noun

//CONNECT		// for subclauses
