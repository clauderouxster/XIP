//!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
//!                                                                            !
//!   Dependency file treating the extraction of lifetime relations            !
//!                                                                            !
//!   Dependencies handled in this file:  LIFETIME                             !
//!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!


DependencyRules:

////////////////////////////////////////////////////////////////////////////////////////////////////////
//                                                                                                    //
//                            LIFETIME                                                                //
//                                                                                                    //
////////////////////////////////////////////////////////////////////////////////////////////////////////


////////////////////////////////////////////////////////////////////////////////
// Birth                                                                      //
////////////////////////////////////////////////////////////////////////////////

// Example: O João nasceu em Janeiro de 1990 em Lisboa.
if( (SUBJ[ELIPS](#2[lemma:nascer],#1) || SUBJ(#2[lemma:nascer],#1[human]) || SUBJ(#2[lemma:nascer],#1[lemma:eu])) )
      EVENT[lex=+](#2,##noun#6[surface:nascimento])

// Example: O João que nasceu em Janeiro de 1990 em Lisboa.
// Example: O João que nasceu no dia 3 de Janeiro de 1990 em Lisboa.
  if( ANTECEDENT[RELAT](#2[human],#1[lemma:que]) &
      SUBJ[PRE](#3[lemma:nascer],#1) )
      EVENT[lex=+](#3,##noun#6[surface:nascimento])

// Example: O João é natural de Lisboa
  if( (SUBJ[ELIPS](#2[lemma:ser],#1) ||SUBJ(#2[lemma:ser],#1[human]) || SUBJ(#2[lemma:ser],#1[lemma:eu])) &
       PREDSUBJ(#2,#3[lemma:natural]) )
     EVENT[lex=+](#3,##noun#5[surface:nascimento])

// Example: O João que é natural de Lisboa.
  if( ANTECEDENT[RELAT](#2[human],#1[lemma:que]) &
      SUBJ[PRE](#3[lemma:ser],#1) &
      PREDSUBJ(#3,#4[lemma:natural]) )
     EVENT[lex=+](#4,##noun#6[surface:nascimento])

// Example: O João que é português
  if( ANTECEDENT[RELAT](#2[human],#1[lemma:que]) &
      SUBJ[PRE](#3[lemma:ser],#1) &
      PREDSUBJ(#3,#4[gentilic]) )
     EVENT[lex=+](#4,##noun#6[surface:nascimento])

// Example: A data de nascimento do João é 3 de Janeiro de 1977
  if ( SUBJ(#1[lemma:ser],#2[lemma:"data de nascimento"]) &
       MOD[POST](#2,#3[human]) )
      EVENT[lex=+](#2,##noun#6[surface:nascimento])

// Example: A sua data de nascimento é 3 de Janeiro de 1980.
  if ( SUBJ(#2[lemma:ser],#1[lemma:"data de nascimento"]) &
       POSS(#1,?[lemma:eu]) )
     EVENT[lex=+](#1,##noun#5[surface:nascimento])

// Example: O João é português
// Example: O João é minhoto
  if( (SUBJ[ELIPS](#3[lemma:ser],#2) || SUBJ(#3[lemma:ser],#2[human]) || SUBJ(#3[lemma:ser],#2[lemma:eu])) &
      (PREDSUBJ(#3,#4[gentcountry]) || PREDSUBJ(#3,#4[gentcity]) ||
       PREDSUBJ(#3,#4[gentregion])  || PREDSUBJ(#3,#4[location]) ||
       PREDSUBJ(#3,#4[admin_area])  || PREDSUBJ(#3,#4[country])) )
     EVENT[lex=+](#4,##noun#6[surface:nascimento])

// Example: O João tem a nacionalidade portuguesa.
  if( (SUBJ[ELIPS](#2[lemma:ter],#1)    || SUBJ(#2[lemma:ter],#1[human]) ||
       SUBJ(#2[lemma:ter],#1[lemma:eu]) || SUBJ(#2[lemma:ter],#1[lemma:que])) &
       CDIR[POST](#2,#3[lemma:nacionalidade]) &
       MOD[POST](#3,#4[gentcountry]) )
       EVENT[lex=+](#3,##noun#6[surface:nascimento])

// Example: O João deixou a aldeia natal do Mogadouro.
  if( (SUBJ[ELIPS](#3,#2) || SUBJ(#3,#2[human]) || SUBJ(#3,#2[lemma:eu])) &
       CDIR(#3,#4[lemma:"?+ natal"]) )
       EVENT[lex=+](#4,##noun#6[surface:nascimento])

// Example: O João faz anos no dia 20 de Janeiro.
if( (SUBJ[ELIPS](#2[lemma:fazer],#1) || SUBJ(#2[lemma:fazer],#1[human]) || 
     SUBJ(#2[lemma:fazer],#1[lemma:eu])) &
     MOD[TEMPORAL](#2,#3[lemma:ano]) &
	~MOD(#2,#4[lemma:casado]) &
	~MOD(#2,#4[lemma:casamento]) &
    ~CDIR(#2,#4[lemma:casado]) & 
	~CDIR(#2,#4[lemma:casamento]) )       
   EVENT[lex=+](##noun#5[surface:lexnascimento],##noun#6[surface:nascimento])

// Example: O João faz 25 anos no dia 20 de Janeiro.
if( (SUBJ[ELIPS](#2[lemma:fazer],#1) || SUBJ(#2[lemma:fazer],#1[human]) || SUBJ(#2[lemma:fazer],#1[lemma:eu])) &
     NE[quant,num](#3[lemma:"?+ ano"]) &
	~MOD(#2,#4[lemma:casado]) &
	~MOD(#2,#4[lemma:casamento]) &
    ~CDIR(#2,#4[lemma:casado]) & 
	~CDIR(#2,#4[lemma:casamento]) )
   EVENT[lex=+](##noun#5[surface:lexnascimento],##noun#6[surface:nascimento])

// Example: O aniversário do João é no dia 20 de Janeiro.
  if(  SUBJ(#2[lemma:ser],#1[lemma:aniversário]) &
      (MOD[POST](#1,#3[human]) || MOD[POST](#1,#3[lemma:eu])) )
       EVENT[lex=+](#1,##noun#6[surface:nascimento])

// Example: D. Dinis (9 de Outubro de 1261)
| NP#1{?*,noun#2[human]},
  PUNCT, (NP), (PUNCT),
  NP#4[t-date,time,quant:~],
  PUNCT
|
  if( HEAD(#2,#1) )
    EVENT[lex=+](##noun#5[surface:lexnascimento],##noun#6[surface:nascimento])

// Example: Pedro Paixão, escritor português, nascido em Campo de Ourique (Lisboa), no dia 7 de Fevereiro de 1956.
// Example: Nascido em Lisboa, a 10 de Março de 1999, o João estuda engenharia.
|?{?#3[surface:nascido]};?{?#3[surface:nascida]};?{?#3[surface:Nascido]};?{?#3[surface:Nascida]},
PP#1{?*,noun#2[location]};PP#1{?*,noun#2[admin_area]};PP#1{?*,noun#2[country]}|
   if (MOD(#3,#4) & HEAD(#2,#1) &
      ~VDOMAIN(#5[lemma:ter],#3))
      EVENT[lex=+](#3,##noun#6[surface:nascimento])

// Example: Nascido em Lisboa, a 10 de Março de 1999, o João estuda engenharia.
|?{?#3[surface:nascido]};?{?#3[surface:nascida]};?{?#3[surface:Nascido]};?{?#3[surface:Nascida]},
PP#1{?*,?#2[time]};PP#1{?*,?#2[num]}|
   if (MOD(#3,#4) & HEAD(#2,#1) &
      ~VDOMAIN(#5[lemma:ter],#3))
      EVENT[lex=+](#3,##noun#6[surface:nascimento])

////////////////////////////////////////////////////////////////////////////////
// Death                                                                      //
////////////////////////////////////////////////////////////////////////////////

// Example: O João morreu.
// Example: O João suicidou-se.
if( (SUBJ[ELIPS](#3[to-die],#2) || SUBJ[PRE](#3[to-die],#2[human]) ||
     SUBJ(#3[to-die],#2[lemma:eu])) &
    ~CDIR[POST](#3,#4[human]) &
    ~MOD[NEG](#3,?[lemma:não]) )
    EVENT[lex=+](#3,##noun#6[surface:morte])

// Example: O João que morreu.
  if( ANTECEDENT[RELAT](#2[human],#1[lemma:que]) &
      (SUBJ[PRE](#3[lemma:morrer],#1) || SUBJ[PRE](#3[lemma:falecer],#1)) )
      EVENT[lex=+](#3,##noun#6[surface:morte])

// Example: O João que foi morto em Janeiro de 1990.
// Example: O João que foi assassinado no dia 3 de Janeiro de 1990.
  if( (ANTECEDENT[RELAT](#1[human],#2[lemma:que]) || ANTECEDENT[RELAT](#1[lemma:eu],#2[lemma:que])) &
      (SUBJ[PRE](#3[lemma:matar],#2) || SUBJ[PRE](#3[lemma:assassinar],#2)) )
      EVENT[lex=+](#3,##noun#6[surface:morte])

// Example: Morreu o escritor mexicano Carlos Fuentes.
  if( SUBJ[ELIPS](#3[to-die],#2) &
      CDIR(#3,#4[human]) )
      EVENT[lex=+](#3,##noun#6[surface:morte])

// Example: A data de óbito/falecimento/morte do João é 3 de Janeiro de 1977
  if ( (SUBJ(#1[lemma:ser],#2[lemma:"data de falecimento"]) ||
        SUBJ(#1[lemma:ser],#2[lemma:"data de morte"]) ||
        SUBJ(#1[lemma:ser],#2[lemma:"data de óbito"])) &
       (MOD[POST](#2,#3[human]) || MOD[POST](#2,#3[lemma:eu])) )
       EVENT[lex=+](#2,##noun#6[surface:morte])

// Example: A sua data de óbito/falecimento/morte é 3 de Janeiro de 1977
  if ( (SUBJ(#1[lemma:ser],#2[lemma:"data de falecimento"]) ||
        SUBJ(#1[lemma:ser],#2[lemma:"data de morte"]) ||
        SUBJ(#1[lemma:ser],#2[lemma:"data de óbito"])) &
        POSS(#2,?[lemma:eu]) )
       EVENT[lex=+](#2,##noun#6[surface:morte])

// Example: O João cometeu suicídio.
  if( (SUBJ[ELIPS](#2[lemma:cometer],#1) || SUBJ(#2[lemma:cometer],#1[human]) || SUBJ(#2[lemma:cometer],#1[lemma:eu])) &
       CDIR(#2,#3[lemma:suicídio]) )
       EVENT[lex=+](#3,##noun#6[surface:morte])

// Example: O João terminou/acabou com a própria vida.
  if( (SUBJ[PRE](#2[lemma:terminar],#1[human]) || SUBJ[PRE](#2[lemma:acabar],#1[human]) ||
       SUBJ[ELIPS](#2[lemma:terminar],#1)      || SUBJ[ELIPS](#2[lemma:acabar],#1) ||
       SUBJ(#2[lemma:terminar],#1[lemma:eu])   || SUBJ(#2[lemma:acabar],#1[lemma:eu])) &
       MOD[POST](#2,#3[lemma:vida]) )
      EVENT[lex=+](##noun#5[surface:lexmorte],##noun#6[surface:morte])

// Example: O João pôs fim à sua vida.
 if( (SUBJ[ELIPS](#2[lemma:pôr],#1) || SUBJ(#2[lemma:pôr],#1[human]) || SUBJ(#2[lemma:pôr],#1[lemma:eu])) &
      CDIR(#2,#3[lemma:fim]) &
      MOD(#2,#4[lemma:vida]) &
     (DETD(#4,?[lemma:próprio]) || POSS[PRE](#4,?[lemma:eu])) )
     EVENT[lex=+](##noun#5[surface:lexmorte],##noun#6[surface:morte])

// Example: O João tirou a própria vida.
if( (SUBJ[ELIPS](#2[lemma:tirar],#1) || SUBJ(#2[lemma:tirar],#1[human]) || SUBJ(#2[lemma:tirar],#1[lemma:eu])) &
     CDIR(#2,#3[lemma:vida]) &
     (DETD(#4,?[lemma:próprio]) || POSS[PRE](#4,?[lemma:eu])) )
      EVENT[lex=+](##noun#5[surface:lexmorte],##noun#6[surface:morte])

// Example: O Pedro matou/assassinou o João.
 if( (SUBJ[PRE](#2[lemma:matar],#1)           || SUBJ[PRE](#2[lemma:assassinar],#1) ||
      SUBJ[PRE](#2[lemma:matar],#1[lemma:eu]) || SUBJ[PRE](#2[lemma:assassinar],#1[lemma:eu])) &
      CDIR[POST](#2,#3[human]) )
      EVENT[lex=+](#2,##noun#6[surface:morte])

// Example: O Pedro tirou a vida ao João em Março de 1990.
  if( (SUBJ(#2[lemma:tirar],#1) || SUBJ(#2[lemma:tirar],#1[lemma:eu])) &
       CDIR(#2,#3[lemma:vida]) &
       MOD(#2,#4[human]) )
      EVENT[lex=+](##noun#5[surface:lexmorte],##noun#6[surface:morte])

// Example: O Pedro pôs fim à vida do João em Março de 1990.
  if( (SUBJ(#2[lemma:pôr],#1) || SUBJ(#2[lemma:pôr],#1[lemma:eu])) &
       CDIR(#2,#3[lemma:fim]) &
       PREPD(#4,?[lemma:a]) & MOD(#2,#4[lemma:vida]) &
       MOD(#4,#5[human]) )
      EVENT[lex=+](##noun#6[surface:lexmorte],##noun#7[surface:morte])

// Example: O João foi ferido/baleado/esfaqueado/atropelado mortalmente.
  if( VDOMAIN(?,#4) & MOD(#4,#5[lemma:mortalmente]) &
      (SUBJ[ELIPS](#4,#2) || SUBJ[PRE](#4,#2[human]) || SUBJ[PRE](#4,#2[lemma:eu])) )
      EVENT[lex=+](#5,##noun#6[surface:morte])

// Example: O João sofreu um ferimento/acidente mortal no dia 25 de Agosto.
  if( (SUBJ[ELIPS](#2[lemma:sofrer],#1) ||SUBJ(#2[lemma:sofrer],#1[human]) ||SUBJ(#2[lemma:sofrer],#1[lemma:eu])) &
       CDIR(#2,#3[lemma:"?+ mortal"]) )
      EVENT[lex=+](#3,##noun#6[surface:morte])

// Example: O João foi a única vítima mortal do acidente.
 if( (SUBJ[ELIPS](#2[lemma:ser],#1) || SUBJ(#2[lemma:ser],#1[human]) || SUBJ(#2[lemma:ser],#1[lemma:eu])) &
      PREDSUBJ(#2,#3[lemma:"vítima mortal"]) )
      EVENT[lex=+](#3,##noun#6[surface:morte])

// Example: O João foi vítima de um ferimento/acidente mortal no dia 25 de Agosto.
 if( (SUBJ[ELIPS](#2[lemma:ser],#1) || SUBJ(#2[lemma:ser],#1[human]) || SUBJ(#2[lemma:ser],#1[lemma:eu])) &
     PREDSUBJ(#2,#3[lemma:vítima]) &
     (MOD(#3,#4[lemma:"?+ mortal"]) || MOD(#3,#4[lemma:homicídio]) || MOD(#3,#4[lemma:assassinato])) )
      EVENT[lex=+](#4,##noun#6[surface:morte])

// Example: D. Dinis (9 de Outubro de 1261 - 7 de Janeiro de 1325)
| NP#1{?*,noun#2[human]},
  PUNCT, (NP), (PUNCT), NP,
  PUNCT, (NP), (PUNCT), NP#4[t-date,time,quant:~],
  PUNCT
|
  if(HEAD(#2,#1) )
     EVENT[lex=+](##noun#5[surface:lexmorte],##noun#6[surface:morte])


////////////////////////////////////////////////////////////////////////////////
// FIXED (To Die)                                                             //
////////////////////////////////////////////////////////////////////////////////

// Example: O João bateu a bota no dia 25 de Agosto.
// Example: O João esticou o pernil no dia 25 de Agosto.
// Example: O João partiu para o outro mundo no dia 25 de Agosto.
// Example: O João desceu à sepultura no dia 25 de Agosto.
//
if ( (SUBJ[ELIPS](#3,#2) || SUBJ(#3,#2[human]) || SUBJ(#3,#2[lemma:eu])) &
      FIXED[normalized](?[lemma:morrer],?,?) )
      EVENT[lex=+](##noun#5[surface:lexmorte],##noun#6[surface:morte])

// Example: O João foi desta para melhor no dia 25 de Agosto.
// Example: O João deu o último suspiro no dia 25 de Agosto.
// Example: O João disse adeus ao mundo no dia 25 de Agosto.
// Example: O João entregou a alma ao criador no dia 25 de Agosto.

if ( ( SUBJ[ELIPS](#2,#1) || SUBJ(#2,#1[human]) || SUBJ(#2,#1[lemma:eu]) ) &
     FIXED[normalized](?[lemma:morrer],?,?,?) )
      EVENT[lex=+](##noun#5[surface:lexmorte],##noun#6[surface:morte])


////////////////////////////////////////////////////////////////////////////////
// Age                                                                        //
////////////////////////////////////////////////////////////////////////////////

// Example: O Pedro tem 25 anos.
// Example: O Pedro tem 6 meses de idade.
// Example: O Pedro tem de idade 25 primaveras.
if ( (SUBJ[ELIPS](#2[lemma:ter],#1)       || SUBJ(#2[lemma:ter],#1[human])       || SUBJ(#2[lemma:ter],#1[lemma:eu]) ||
      SUBJ[ELIPS](#2[lemma:fazer],#1)     || SUBJ(#2[lemma:fazer],#1[human])     || SUBJ(#2[lemma:fazer],#1[lemma:eu]) ||
      SUBJ[ELIPS](#2[lemma:comemorar],#1) || SUBJ(#2[lemma:comemorar],#1[human]) || SUBJ(#2[lemma:comemorar],#1[lemma:eu]) ||
      SUBJ[ELIPS](#2[lemma:atingir],#1)   || SUBJ(#2[lemma:atingir],#1[human])   || SUBJ(#2[lemma:atingir],#1[lemma:eu])) &
     (CDIR(#2,#3[t-meas]) || CDIR(#2,#3[lemma:primavera]) ||
      CDIR(#2,#3[lemma:idade])) &
      HEAD(#3,#4) )
     EVENT[lex=+](#3,##noun#6[surface:idade])

// Example: O Pedro, 25 anos, professor universitário, disse isso.
| NP#1{?*,noun#2[human]}, PUNCT, NP#3{?*,noun#4[t-meas,time]}, (PP), PUNCT |
  if(HEAD(#4,#5)
    )
      EVENT[lex=+](##noun#6[surface:lexidade],##noun#7[surface:idade])

////////////////////////////////////////////////////////////////////////////////
// Family                                                                     //
////////////////////////////////////////////////////////////////////////////////

// Example: A tia do Pedro chama-se Maria
| VF{verb#1[lemma:chamar]},NP{?[lemma:eu]},NP{noun#4[human]} |
  if( SUBJ[PRE](#1,#2[relative]) &
     (MOD[POST](#2,#3[human,SEM-Hinstitution:~]) || MOD[POST](#2,#3[ord])) )
       EVENT[lex=+](#2,##noun#7[surface:parentesco])

// Example: O João, tio do Pedro.
// Example: A tia do João, Maria.
 if( APPOSIT[REDUCED](#2[human],#3[relative]) &
     MOD[POST](#3,#1[human,SEM-Hinstitution:~]) )
      EVENT[lex=+](#3,##noun#7[surface:parentesco])

// Example: A tia do Pedro é a Maria
if( SUBJ(#3[lemma:ser],#2[relative]) &
    MOD[POST](#2,#1[human,SEM-Hinstitution:~]) &
    PREDSUBJ(#3,#6[human,SEM-Hinstitution:~,SEM-Hprofession:~])
  )
      EVENT[lex=+](#2,##noun#7[surface:parentesco])

// Example: O João é tio do Pedro.
// Example: A Maria é mãe do João.
  if( (SUBJ(#3[lemma:ser],#2[human]) || SUBJ[PRE](#3[lemma:ser],#2[ord]) ||
       SUBJ[ELIPS](#3[lemma:ser],#2) || SUBJ[PRE](#3[lemma:ser],#2[lemma:eu])) &
       PREDSUBJ(#3,#4[relative]) &
       MOD[POST](#4,#6[human,SEM-Hinstitution:~]) &
       ~COORD(?,#2) )
      EVENT[lex=+](#4,##noun#7[surface:parentesco])

// Example: O João e a Maria são irmãos-gémeos.
  if( COORD(#1[lemma:e],#2[human]) & COORD(#1[lemma:e],#4[human]) &
      SUBJ[PRE](#5[lemma:ser],#2) & SUBJ[PRE](#5,#4) &
      PREDSUBJ(#5,#6[relative]) & CLINK(#2,#4) )
      EVENT[lex=+](#6,##noun#7[surface:parentesco])

// Example: O João que é tio do Pedro.
if( ANTECEDENT[RELAT](#2[human],#1[lemma:que]) &
    SUBJ[PRE](#3[lemma:ser],#1) &
    PREDSUBJ(#3,#4[relative]) &
    MOD[POST](#4,#5[human]) )
      EVENT[lex=+](#4,##noun#7[surface:parentesco])

// Example: A Joana deu à luz a Filipa
// Example: A Joana, que deu à luz a Filipa,
 if( (SUBJ[PRE](#3[lemma:dar],#2[human]) || SUBJ[PRE](#3[lemma:dar],#2[lemma:eu])) &
      CDIR[POST](#3,#5[human]) &
      PREPD(#4[lemma:luz],?[lemma:a]) & MOD[POST](#3,#4) )
      EVENT[lex=+](##noun#6[surface:lexparentesco],##noun#7[surface:parentesco])

// Example: O filho de Pedro e Maria, João,
//Não funciona: erros nas dependências de base NJM 2010/12/23


////////////////////////////////////////////////////////////////////////////////
// Spouse                                                                     //
////////////////////////////////////////////////////////////////////////////////

// Example: O João casou com a Maria.
 if( (SUBJ[ELIPS](#3[lemma:casar],#2) || SUBJ[PRE](#3[lemma:casar],#2[human]) ||
      SUBJ[PRE](#3[lemma:casar],#2[lemma:eu])) &
     ~COORD(?,#2[human]))
      EVENT[lex=+](#3,##noun#6[surface:parentesco])

// Example: O João e a Maria estão casados há 3 anos.
 if( (SUBJ[ELIPS](#3[lemma:ser],#2) || SUBJ[PRE](#3[lemma:ser],#2[human]) ||
      SUBJ[PRE](#3[lemma:ser],#2[lemma:eu]) || SUBJ[ELIPS](#3[lemma:estar],#2) ||
      SUBJ[PRE](#3[lemma:estar],#2[human]) || SUBJ[PRE](#3[lemma:estar],#2[lemma:eu]) ) &
     (SUBJ[ELIPS](#3,#1) || SUBJ[PRE](#3,#1[human]) ||
      SUBJ[PRE](#3,#1[lemma:eu]) )  &
      PREDSUBJ(#3,#4[lemma:casar]) )
      EVENT[lex=+](#4,##noun#6[surface:parentesco]),
      EVENT[spouse=+](#4,#2),
      EVENT[spouse=+](#4,#1)

// Example: O João e a Ana fizeram três anos de casados no dia 20 de Julho.
if( (SUBJ[ELIPS](#2[lemma:fazer],#1) || SUBJ(#2[lemma:fazer],#1[human]) || SUBJ(#2[lemma:fazer],#1[lemma:eu]) ||
 SUBJ[ELIPS](#2[lemma:celebrar],#1) || SUBJ(#2[lemma:celebrar],#1[human]) || SUBJ(#2[lemma:celebrar],#1[lemma:eu]) ||
 SUBJ[ELIPS](#2[lemma:festejar],#1) || SUBJ(#2[lemma:festejar],#1[human]) || SUBJ(#2[lemma:festejar],#1[lemma:eu]) ||
 SUBJ[ELIPS](#2[lemma:comemorar],#1) || SUBJ(#2[lemma:comemorar],#1[human]) || SUBJ(#2[lemma:comemorar],#1[lemma:eu])) &
(SUBJ[ELIPS](#2,#3) || SUBJ(#2,#3) || SUBJ(#2,#3[lemma:eu])) &
(MOD(#2,#5[lemma:casado]) || MOD(#2,#5[lemma:casamento]) ||
 CDIR(#2,#5[lemma:casado]) || CDIR(#2,#5[lemma:casamento])) )         
    EVENT[lex=+](#5,##noun#6[surface:parentesco]),	  
    EVENT[spouse=+](#5,#1),
    EVENT[spouse=+](#5,#3)

// Example: O João e a Ana comemoraram o aniversário de casamento no dia 20 de Julho.
if( (SUBJ[ELIPS](#2[lemma:celebrar],#1) || SUBJ(#2[lemma:celebrar],#1[human]) || SUBJ(#2[lemma:celebrar],#1[lemma:eu]) ||
     SUBJ[ELIPS](#2[lemma:festejar],#1) || SUBJ(#2[lemma:festejar],#1[human]) || SUBJ(#2[lemma:festejar],#1[lemma:eu]) ||
     SUBJ[ELIPS](#2[lemma:comemorar],#1) || SUBJ(#2[lemma:comemorar],#1[human]) || SUBJ(#2[lemma:comemorar],#1[lemma:eu])) &
    (SUBJ[ELIPS](#2,#3) || SUBJ(#2,#3) || SUBJ(#2,#3[lemma:eu])) &
     CDIR(#2,#4[lemma:aniversário]) &
     MOD(#4,#5[lemma:casamento]))         
      EVENT[lex=+](#5,##noun#6[surface:parentesco]), 
      EVENT[spouse=+](#5,#1),
      EVENT[spouse=+](#5,#3)
  
// Example: O Paulo e a Joana deram o nó.
// Example: O Paulo e a Joana juntaram os trapos.
if( SUBJ[PRE](#1,#2[human]) & SUBJ[PRE](#1,#3[human])
    FIXED[normalized](?[lemma:casar],?,?) &
    CLINK(#2,#3) & COORD(?[lemma:e],#2) & COORD(?[lemma:e],#3) )
      EVENT[lex=+](##noun#5[surface:lexparentesco],##noun#6[surface:parentesco]),
      EVENT[spouse=+](#5,#2),
      EVENT[spouse=+](#5,#3)

// Example: O Jorge deu o nó com a Mariana
// Example: O Paulo juntou os trapos com a Carina.
  if( (SUBJ[PRE](#3,#2[human]) || SUBJ[PRE](#3,#2[lemma:eu])) &
       FIXED[normalized](?[lemma:casar],?,?) &
       MOD[POST](#3,#5[human]) )
      EVENT[lex=+](##noun#6[surface:lexparentesco],##noun#7[surface:parentesco]),
      EVENT[spouse=+](#6,#2),
      EVENT[spouse=+](#6,#5)

// Example: O Paulo e a Joana casaram-se
  if( (COORD(#6,#2[human]) || COORD(#6,#2[lemma:eu])) & COORD(#6,#4[human]) &
       SUBJ[PRE](#5[lemma:casar],#2) &
       SUBJ[PRE](#5,#4) &
