//! --- CONTENTS ---                                                           !
//!   Project    : Portuguese XIP grammar                                      !
//!   filename   : dependencyCrime.xip                                         !
//!   Date       : 2012/February                                               !
//!   Author     : Vera Cabarrão                                               !
//!                                                                            !
//!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
//!                                                                            !
//!   Dependency file treating the extraction of crime relations               !
//!                                                                            !
//!   Dependencies handled in this file:  CRIME                                !
//!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!


DependencyRules:

////////////////////////////////////////////////////////////////////////////////////////////////////////
//                                                                                                    //
//                            CRIME                                                                   //
//                                                                                                    //
////////////////////////////////////////////////////////////////////////////////////////////////////////


////////////////////////////////////////////////////////////////////////////////
// CRIME                                                                      //
////////////////////////////////////////////////////////////////////////////////

// Example: Duas agentes da polícia mortas em Manchester por antigo suspeito de homicídio.
if( (MOD[POST](#2[human],#1[lemma:morrer]) || MOD[POST](#2[human],#1[lemma:matar]) || MOD[POST](#2[human],#1[lemma:assassinar])) &
     MOD[POST](#1,#3[human]) )
      EVENT[lex=+](#1,##noun#6[surface:homicídio])

// Example: Dois homens foram mortos num tiroteio no bairro da Ribeira, na noite de terça-feira.
if( (SUBJ[PRE](#3[lemma:matar],#2[human]) || SUBJ(#3[lemma:matar],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:assassinar],#2[human]) || SUBJ(#3[lemma:assassinar],#2[lemma:eu])) &
    ~CDIR[POST](#3,#4[human]) &
    ~MOD[NEG](#3,?[lemma:não]) )
       EVENT[lex=+](#3,##noun#6[surface:homicídio])

// Example: O João assassinou o Pedro em Lisboa em 2005.
if( (SUBJ[PRE](#2[lemma:matar],#1)           || SUBJ[PRE](#2[lemma:assassinar],#1) ||
     SUBJ[PRE](#2[lemma:matar],#1[lemma:eu]) || SUBJ[PRE](#2[lemma:assassinar],#1[lemma:eu])) &
     CDIR[POST](#2,#3[human]) )
      EVENT[lex=+](#2,##noun#6[surface:homicídio])

// Example: Homem atacou ex-mulher à facada e disparou pistola junto à cabeça da vítima numa paragem de autocarro em Benfica.
if( (SUBJ[PRE](#3[lemma:atacar],#2[human])  || SUBJ(#3[lemma:atacar],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:atacar],#2[sem-hh]) ) &
     CDIR[POST](#3,#4[human]) )
       EVENT[lex=+](#3,##noun#6[surface:"tentativa de homicídio"])

if( (SUBJ[PRE](#3[lemma:disparar],#2[human]) || SUBJ(#3[lemma:disparar],#2[lemma:eu])  ||
     SUBJ[PRE](#3[lemma:disparar],#2[sem-hh]) ) &
     MOD[POST](#3,#4[human]) )
       EVENT[lex=+](#3,##noun#6[surface:"tentativa de homicídio"])

// Example: Um homem de 25 anos roubou dois fios de ouro a um casal de idosos no Sobralinho.
// Example: Duo rouba ouro a irmãos idosos.
if( (SUBJ[PRE](#3[lemma:roubar],#2[human])  || SUBJ(#3[lemma:roubar],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:roubar],#2[sem-hh]) || SUBJ[PRE](#3[lemma:furtar],#2[human]) ||
     SUBJ(#3[lemma:furtar],#2[lemma:eu])    || SUBJ[PRE](#3[lemma:furtar],#2[sem-hh]) ) &
     CDIR[POST](#3,#4) )
    EVENT[lex=+](#3,##noun#6[surface:roubo])

// Example: Adolescente condenado na Holanda a um ano de prisão por homicídio após discussão no Facebook.
if(  MOD[POST](#2[human],#3[lemma:condenar]) &
    (MOD[POST](#2,#4[SEM-act-crime]) || MOD[POST](#2,#4[lemma:morte]) || MOD[POST](#2,#4[lemma:assassinato]) ) )
       EVENT[lex=+](#4,#4)

// Example: João Vieira Pinto, Luís Duque, Rui Meireles e José Veiga foram esta manhã condenados por fraude fiscal e branqueamento de capitais no processo da transferência do jogador do Benfica para o Sporting.
// Example: O antigo primeiro-ministro egípcio do governo de Hosni Mubarak foi esta quinta-feira julgado por corrupção e condenado a três anos de prisão, assim como ao pagamento de uma coima de aproximadamente 580 mil euros.

if( (SUBJ[PRE](#3[lemma:condenar],#2[human])  || SUBJ(#3[lemma:condenar],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:condenar],#2[sem-hh]) || SUBJ[PRE](#3[lemma:absolver],#2[human])  ||
     SUBJ(#3[lemma:absolver],#2[lemma:eu])    || SUBJ[PRE](#3[lemma:absolver],#2[sem-hh]) ||
     SUBJ[PRE](#3[lemma:julgar],#2[human])    || SUBJ(#3[lemma:julgar],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:julgar],#2[sem-hh])   || SUBJ[PRE](#3[lemma:acusar],#2[human]) ||
     SUBJ(#3[lemma:acusar],#2[lemma:eu])      || SUBJ[PRE](#3[lemma:acusar],#2[sem-hh])) &
    (COMPL[POST](#3,#4[SEM-act-crime]) || COMPL[POST](#3,#4[lemma:morte]) ||
     MOD[POST](#3,#4[SEM-act-crime])   || MOD[POST](#3,#4[lemma:morte]) ||
     AGENT[POST](#3,#4[SEM-act-crime]) || AGENT[POST](#3,#4[lemma:morte])) )
       EVENT[lex=+](#4,#4)

// Example: João Lopes foi absolvido no processo em que era acusado de violar uma mulher em 2011.
if( (SUBJ[PRE](#3[lemma:violar],#2[human])  || SUBJ(#3[lemma:violar],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:violar],#2[sem-hh]) ) &
     CDIR(#3,#4[human]) &)
       EVENT[lex=+](#3,##noun#6[surface:violação])

// Example: O Tribunal Criminal de Lisboa absolveu esta sexta-feira o neurologista Machado Cândido dos crimes de burla e falsificação de documentos.
if( (SUBJ[PRE](#3,#2[lemma:"tribunal ?+"]) || SUBJ[PRE](#3,#2[human]) ) &
     CDIR(#3,#4[human]) &
     MOD[POST](#4,#5[lemma:crime]) &
     MOD[POST](#5,#6[SEM-act-crime]) )
       EVENT[lex=+](#6,#6)

// Example: O Tribunal Criminal de Lisboa absolveu esta sexta-feira o neurologista Machado Cândido dos crimes de burla qualificada e falsificação de documentos.
if( (SUBJ[PRE](#3,#2[lemma:"tribunal ?+"]) || SUBJ[PRE](#3,#2[human]) ) &
     CDIR(#3,#4[human]) &
     MOD[POST](#4,#5[SEM-act-crime]) )
       EVENT[lex=+](#5,#5)

if( (SUBJ[PRE](#3,#2[lemma:"tribunal ?+"]) || SUBJ[PRE](#3,#2[human]) ) &
     CDIR(#3,#4[human]) &
     COORD(#1[surface:e],#6[SEM-act-crime]) )
       EVENT[lex=+](#6,#6)

// Example: António Francisco Bonfim Lopes foi absolvido da acusação de lavagem de dinheiro, sob a alegação de que não havia qualquer prova hábil a confirmar a condenação.
if( (SUBJ[PRE](#3[lemma:absolver],#2[human])  || SUBJ(#3[lemma:absolver],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:absolver],#2[sem-hh]) ) &
     MOD(#3,#4[lemma:acusação]) &
     MOD(#4,#5[SEM-act-crime]) )
       EVENT[lex=+](#5,#5)

// Example: O homem foi detido às 21:20 de quarta-feira, na freguesia da Falagueira, por suspeitas da autoria de crimes de roubo e extorsão.
if( (SUBJ[PRE](#2[lemma:deter],#1[human])  || SUBJ(#2[lemma:deter],#1[lemma:eu]) ||
     SUBJ[PRE](#2[lemma:deter],#1[sem-hh]) ) &
    (MOD(#3[lemma:suspeita],#4[lemma:autoria]) || MOD(#3[lemma:suspeito],#4[lemma:autoria])) &
     MOD(#4,#5[lemma:crime]) &
     MOD(#5,#6[SEM-act-crime]) )
       EVENT[lex=+](#6,#6)

// Example: O João foi detido na passada quinta-feira em Lisboa por suspeitas de roubo e extorsão.
if( (SUBJ[PRE](#2[lemma:deter],#1[human])  || SUBJ(#2[lemma:deter],#1[lemma:eu]) ||
     SUBJ[PRE](#2[lemma:deter],#1[sem-hh]) ) &
    (MOD(#3[lemma:suspeita],#4[SEM-act-crime]) || MOD(#3[lemma:suspeito],#4[SEM-act-crime])) )
       EVENT[lex=+](#4,#4)

// Example: Quatro pessoas foram presas na tarde de segunda-feira suspeitas de cometer crimes de branqueamento de capitais em Mangualde.
if( (SUBJ[PRE](#2[lemma:ser],#1[human])  || SUBJ(#2[lemma:ser],#1[lemma:eu]) || SUBJ[PRE](#2[lemma:ser],#1[sem-hh]) ) &
    (PREDSUBJ(#2,#6[lemma:preso]) ) &
     MOD[POST](#3[lemma:suspeitar],#4[lemma:cometer]) &
     CDIR(#4,#5[SEM-act-crime]) )
       EVENT[lex=+](#5,#5)

// Example: O Tribunal de Idanha-a-Nova decidiu manter em prisão preventiva um homem e uma mulher por suspeita de crimes sexuais envolvendo menores.
if( (SUBJ[PRE](#2,#1[lemma:"tribunal ?+"])) &
    (CDIR[POST](#3,#4[human]) || MOD[POST](#3,#4[lemma:eu]) || MOD[POST](#3,#4[sem-hh])) &
    (MOD(#3,#5[lemma:suspeita]) || MOD(#3,#5[lemma:suspeito])) &
     MOD(#5,#6[SEM-act-crime]) )
     EVENT[lex=+](#6,#6)

// Example: A mulher está em prisão domiciliária desde o dia 31 de Outubro por suspeita de furto.
if( (SUBJ[PRE](#2,#1[human])  || SUBJ(#2,#1[lemma:eu]) || SUBJ[PRE](#2,#1[sem-hh]) ) &
     MOD(#2,#3[lemma:suspeita]) &
     MOD(#3,#4[SEM-act-crime]) )
       EVENT[lex=+](#4,#4)

// Example: Ricardo Palmera cumpre pena de 60 anos de prisão nos Estados Unidos pelo sequestro de três cidadãos norte-americanos.
if( (SUBJ[PRE](#2[lemma:cumprir],#1[human])  || SUBJ(#2[lemma:cumprir],#1[lemma:eu]) ||
     SUBJ[PRE](#2[lemma:cumprir],#1[sem-hh]) ) &
    (CDIR(#2,#3[lemma:"pena ?+"]) || CDIR(#2,#3[lemma:pena])) &
    (MOD(#2,#4[SEM-act-crime]) || MOD(#2,#4[lemma:morte])) )
       EVENT[lex=+](#4,#4)

// Example: O Tribunal condenou o violador de Joana a 25 anos de prisão efectiva.
if( (SUBJ[PRE](#3,#2[lemma:"tribunal ?+"]) || SUBJ[PRE](#3,#2[lemma:tribunal]) ) &
     CDIR[POST](#3,#4[lemma:violador]) )
     EVENT[lex=+](#4,##noun#6[surface:violação])

// Example: A PSP deteve, esta quinta-feira, em Bragança, um homem de 57 anos por suspeita de tráfico de droga.
if( (SUBJ[PRE](#2[lemma:deter],#1[lemma:PSP]) || SUBJ[PRE](#2[lemma:deter],#1[lemma:"Polícia Judiciária"]) ||
     SUBJ[PRE](#2[lemma:deter],#1[lemma:PJ])  || SUBJ[PRE](#2[lemma:prender],#1[lemma:PSP]) ||
     SUBJ[PRE](#2[lemma:prender],#1[lemma:"Polícia Judiciária"]) || SUBJ[PRE](#2[lemma:prender],#1[lemma:PJ]) ) &
    (CDIR[POST](#2,#3[human]) || CDIR[POST](#2,#3[sem-hh])) &
     MOD[POST](#3,#4[lemma:suspeita]) &
     MOD[POST](#4,#5[SEM-act-crime]))
     EVENT[lex=+](#5,#5)

// Example: Sessenta e oito antigos militares respondem pelo assassínio de pelo menos 4400 pessoas.
if( (SUBJ[PRE](#2[lemma:responder],#1[human])  || SUBJ(#2[lemma:responder],#1[lemma:eu]) ||
     SUBJ[PRE](#2[lemma:responder],#1[sem-hh]) || SUBJ[PRE](#2[lemma:responder],#1[sem-hprof])) &
     MOD(#2,#3[SEM-act-crime]) )
       EVENT[lex=+](#3,#3)


////////////////////////////////////////////////////////////////////////////////
// Crime, Agent, Victim, Object                                               //
////////////////////////////////////////////////////////////////////////////////

   if ((EVENT[lex](#5[lemma:morrer],#6[surface:homicídio]) || EVENT[lex](#5[lemma:matar],#6[surface:homicídio]) || EVENT[lex](#5[lemma:assassinar],#6[surface:homicídio])) )
       EVENT[crime=+](#5)

   if (EVENT[lex](#5,#6[surface:"tentativa de homicídio"]))
       EVENT[crime=+](#5)

   if (EVENT[lex](#5[lemma:roubar],#6[surface:roubo]))
       EVENT[crime=+](#5)

   if ((EVENT[lex](#5[lemma:violar],#6[surface:violação]) ||
        EVENT[lex](#5[lemma:violador],#6[surface:violação])) )
        EVENT[crime=+](#5)

   if (~(MOD(#2[lemma:suspeita],#3) || MOD(#2[lemma:suspeito],#3) || MOD(#2[lemma:suspeitar],#3)) &
        (EVENT[lex](#5,#6[SEM-act-crime]) || EVENT[lex](#5,#6[lemma:morte])) )
        EVENT[crime=+](#5)

   if ((MOD(#2[lemma:suspeita],#3) || MOD(#2[lemma:suspeito],#3) || MOD(#2[lemma:suspeitar],#3)) &
        EVENT[lex](#5,#6[SEM-act-crime]))
        EVENT[crime_susp=+](#5)


// AGENT
if( (MOD[POST](#2[human],#1[lemma:morrer]) || MOD[POST](#2[human],#1[lemma:matar]) || MOD[POST](#2[human],#1[lemma:assassinar])) &
     MOD[POST](#1,#3[human]) & PREPD(#3,#4)
     EVENT[lex](#1,#6[surface:homicídio]) )
       EVENT[agent-generic=+](#1,#3)

if( (SUBJ[PRE](#3[lemma:roubar],#2[human])  || SUBJ(#3[lemma:roubar],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:roubar],#2[sem-hh]) || SUBJ[PRE](#3[lemma:furtar],#2[human]) ||
     SUBJ(#3[lemma:furtar],#2[lemma:eu])    || SUBJ[PRE](#3[lemma:furtar],#2[sem-hh]) ) &
     CDIR[POST](#3,#4) &
     EVENT[lex](#3,#6[surface:roubo]) )
       EVENT[agent-generic=+](#3,#2)

if(  MOD[POST](#2[human],#3[lemma:condenar]) &
    (MOD[POST](#2,#4[lemma:homicídio]) || MOD[POST](#2,#4[lemma:assassinato]) || MOD[POST](#2,#4[lemma:morte])) &
     PREPD(#4,#1) &
     EVENT[lex](#4,#6[surface:homicídio]) )
       EVENT[agent-generic=+](#4,#2)

if( (SUBJ[PRE](#3[agression],#2[human])  || SUBJ(#3[agression],#2[lemma:eu]) ||
     SUBJ[PRE](#3[agression],#2[sem-hh]) ) &
     EVENT[lex](#3,#6[surface:"tentativa de homicídio"]) )
       EVENT[agent-generic=+](#3,#2)

if( (SUBJ[PRE](#3[lemma:disparar],#2[human]) || SUBJ(#3[lemma:disparar],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:disparar],#2[sem-hh])) &
     EVENT[lex](#3,#6[surface:"tentativa de homicídio"]) )
       EVENT[agent-generic=+](#3,#2)

if( (SUBJ[PRE](#3[lemma:violar],#2[human])  || SUBJ(#3[lemma:violar],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:violar],#2[sem-hh]) ) &
     HEAD(#2,#5) &
     CDIR(#3,#4[human]) &
     EVENT[lex](#3,#6[surface:violação]) )
      EVENT[agent-generic=+](#3,#5)

if( (SUBJ[PRE](#3,#2[lemma:"tribunal ?+"]) || SUBJ[PRE](#3,#2[human]) )&
     CDIR(#3,#4[human]) &
     MOD[POST](#4,#5[lemma:crime]) &
     MOD[POST](#5,#6[SEM-act-crime]) &
     EVENT[lex](#6,#6) )
      EVENT[agent-generic=+](#6,#4)

if( (SUBJ[PRE](#3,#2[lemma:"tribunal ?+"]) || SUBJ[PRE](#3,#2[human]) ) &
     CDIR(#3,#4[human]) &
     MOD[POST](#4,#5[SEM-act-crime]) &
     EVENT[lex](#5,#5) )
      EVENT[agent-generic=+](#5,#4)

if( (SUBJ[PRE](#3,#2[lemma:"tribunal ?+"]) || SUBJ[PRE](#3,#2[human]) ) &
     CDIR(#3,#4[human]) &
     COORD(#1[surface:e],#6[SEM-act-crime])&
     EVENT[lex](#6,#6) )
      EVENT[agent-generic=+](#6,#4)

if( (SUBJ[PRE](#3[lemma:absolver],#2[human])  || SUBJ(#3[lemma:absolver],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:absolver],#2[sem-hh]) ) &
     HEAD(#2,#6) &
     MOD(#3,#4[lemma:acusação]) &
     MOD(#4,#5[SEM-act-crime]) &
     EVENT[lex](#5,#5) )
      EVENT[agent-generic=+](#5,#6)

if( (SUBJ[PRE](#2[lemma:deter],#1[human])  || SUBJ(#2[lemma:deter],#1[lemma:eu]) ||
     SUBJ[PRE](#2[lemma:deter],#1[sem-hh]) ) &
     HEAD(#1,#7) &
     COMPL(#2,#3[lemma:suspeita]) &
     MOD(#3,#4[lemma:autoria]) &
     MOD(#4,#5[lemma:crime]) &
     MOD(#5,#6[SEM-act-crime]) &
     EVENT[lex](#6,#6) )
      EVENT[agent-generic=+](#6,#7)

if( (SUBJ[PRE](#2[lemma:deter],#1[human])  || SUBJ(#2[lemma:deter],#1[lemma:eu]) ||
     SUBJ[PRE](#2[lemma:deter],#1[sem-hh]) ) &
     HEAD(#1,#5) &
    (MOD(#3[lemma:suspeita],#4[SEM-act-crime]) || MOD(#3[lemma:suspeito],#4[SEM-act-crime])) &
     EVENT[lex](#4,#4) )
      EVENT[agent-generic=+](#4,#5)

if( (SUBJ[PRE](#2,#1[human])  || SUBJ(#2,#1[lemma:eu]) || SUBJ[PRE](#2,#1[sem-hh]) ) &
     HEAD(#1,#6) &
     MOD[POST](#3[lemma:suspeitar],#4[lemma:cometer]) &
     CDIR(#4,#5[SEM-act-crime]) &
     EVENT[lex](#5,#5) )
      EVENT[agent-generic=+](#5,#6)

if( (SUBJ[PRE](#2,#1[lemma:"tribunal ?+"]) || SUBJ[PRE](#2,#1[lemma:"tribunal ?+"]) ) &
    (CDIR[POST](#3,#4[human]) || MOD[POST](#3,#4[lemma:eu]) || MOD[POST](#3,#4[sem-hh])) &
     HEAD(#4,#7) &
    (MOD(#3,#5[lemma:suspeita]) || MOD(#3,#5[lemma:suspeito])) &
     MOD(#5,#6[SEM-act-crime]) &
     EVENT[lex](#6,#6) )
      EVENT[agent-generic=+](#6,#7)

if( (SUBJ[PRE](#2,#1[human])  || SUBJ(#2,#1[lemma:eu]) || SUBJ[PRE](#2,#1[sem-hh]) ) &
     HEAD(#1,#5) &
     MOD(#2,#3[lemma:suspeita]) &
     MOD(#3,#4[SEM-act-crime]) &
     EVENT[lex](#4,#4) )
       EVENT[agent-generic=+](#4,#5)

if( (SUBJ[PRE](#3[lemma:condenar],#2[human])  || SUBJ(#3[lemma:condenar],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:condenar],#2[sem-hh]) || SUBJ[PRE](#3[lemma:julgar],#2[human])  ||
     SUBJ(#3[lemma:julgar],#2[lemma:eu])      || SUBJ[PRE](#3[lemma:julgar],#2[sem-hh])) &
     HEAD(#2,#1) &
    (COMPL[POST](#3,#4[SEM-act-crime]) || COMPL[POST](#3,#4[lemma:morte])) &
     EVENT[lex](#4,#4) )
       EVENT[agent-generic=+](#4,#1)

if( (SUBJ[PRE](#3,#2[lemma:"tribunal ?+"]) || SUBJ[PRE](#3,#2[lemma:tribunal]) ) &
     CDIR[POST](#3,#4[lemma:violador]) &
     EVENT[lex](#4,#6[surface:violação]) )
       EVENT[agent-generic=+](#4,#4)

if( (SUBJ[PRE](#3,#2[human])  || SUBJ(#3,#2[lemma:eu]) || SUBJ[PRE](#3,#2[sem-hh]) ) &
     HEAD(#2,#1) &
    (COMPL[POST](#3,#4[SEM-act-crime]) || MOD[POST](#3,#4[SEM-act-crime]) ) &
     MOD[POST](#3,#5[human]) & HEAD(#5,#6) &
     EVENT[crime](#4) )
      EVENT[agent-generic=+](#4,#1)

if( (SUBJ[PRE](#2[lemma:deter],#1[lemma:PSP]) || SUBJ[PRE](#2[lemma:deter],#1[lemma:"Polícia Judiciária"]) ||
     SUBJ[PRE](#2[lemma:deter],#1[lemma:PJ])  || SUBJ[PRE](#2[lemma:prender],#1[lemma:PSP]) ||
     SUBJ[PRE](#2[lemma:prender],#1[lemma:"Polícia Judiciária"]) || SUBJ[PRE](#2[lemma:prender],#1[lemma:PJ]) ) &
    (CDIR[POST](#2,#3[human]) || CDIR[POST](#2,#3[sem-hh])) &
     HEAD(#3,#6) &
     MOD[POST](#3,#4[lemma:suspeita]) &
     MOD[POST](#4,#5[SEM-act-crime]) &
     EVENT[lex](#5,#5) )
      EVENT[agent-generic=+](#5,#6)

if( (SUBJ[PRE](#2[lemma:matar],#1)           || SUBJ[PRE](#2[lemma:assassinar],#1) ||
     SUBJ[PRE](#2[lemma:matar],#1[lemma:eu]) || SUBJ[PRE](#2[lemma:assassinar],#1[lemma:eu])) &
     HEAD(#1,#3) &
     CDIR[POST](#2,#4[human]) &
     EVENT[lex](#2,#6[surface:homicídio]) )
      EVENT[agent-generic=+](#2,#3)

if( (SUBJ[PRE](#2[lemma:cumprir],#1[human])  || SUBJ(#2[lemma:cumprir],#1[lemma:eu]) ||
     SUBJ[PRE](#2[lemma:cumprir],#1[sem-hh]) ) &
     HEAD(#1,#5) &
    (CDIR(#2,#3[lemma:"pena ?+"]) || CDIR(#2,#3[lemma:pena])) &
    (MOD(#2,#4[SEM-act-crime]) || MOD(#2,#4[lemma:morte])) &
     EVENT[lex](#4,#4) )
      EVENT[agent-generic=+](#4,#5)

if( (SUBJ[PRE](#2[lemma:responder],#1[human])  || SUBJ(#2[lemma:responder],#1[lemma:eu]) ||
     SUBJ[PRE](#2[lemma:responder],#1[sem-hh]) || SUBJ[PRE](#2[lemma:responder],#1[sem-hprof])) &
     HEAD(#1,#4) &
     MOD(#2,#3[SEM-act-crime]) &
     EVENT[lex](#3,#3) )
      EVENT[agent-generic=+](#3,#4)

// VICTIM
if( (MOD[POST](#2[human],#1[lemma:morrer]) || MOD[POST](#2[human],#1[lemma:matar]) || MOD[POST](#2[human],#1[lemma:assassinar])) &
     MOD[POST](#1,#3[human]) &
     HEAD(#2,#4) &
     EVENT[lex](#1,#6[surface:homicídio]) )
       EVENT[victim=+](#1,#4)

if( (SUBJ[PRE](#3[lemma:roubar],#2[human])  || SUBJ(#3[lemma:roubar],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:roubar],#2[sem-hh]) || SUBJ[PRE](#3[lemma:furtar],#2[human]) ||
     SUBJ(#3[lemma:furtar],#2[lemma:eu])    || SUBJ[PRE](#3[lemma:furtar],#2[sem-hh]) ) &
     CDIR[POST](#3,#4) &
    (CINDIR[POST](#3,#5[human]) || CINDIR[POST](#3,#5[org]) ||
     MOD[POST](#3,#5[human])    || MOD[POST](#3,#5[org]) ) &  HEAD(#5,#1) &
     EVENT[lex](#3,#6[surface:roubo]) )
       EVENT[victim=+](#3,#1)

if( (SUBJ[PRE](#3[agression],#2[human])  || SUBJ(#3[agression],#2[lemma:eu]) ||
     SUBJ[PRE](#3[agression],#2[sem-hh]) ) &
     CDIR(#3,#4[human]) & HEAD(#4,#1) &
     EVENT[lex](#3,#6[surface:"tentativa de homicídio"]) )
       EVENT[victim=+](#3,#1)

if( (SUBJ[PRE](#3[lemma:disparar],#2[human]) || SUBJ(#3[lemma:disparar],#2[lemma:eu]) ||
     SUBJ[PRE](#3[lemma:disparar],#2[sem-hh])) &
     MOD(#3,#4[human]) & HEAD(#4,#1) &
     EVENT[lex](#3,#6[surface:"tentativa de homicídio"]) )
       EVENT[victim=+](#3,#1)

if( (SUBJ[PRE](#3,#2[human])  || SUBJ(#3,#2[lemma:eu]) || SUBJ[PRE](#3,#2[sem-hh]) ) &
     HEAD(#2,#1) &
    (COMPL[POST](#3,#4[SEM-act-crime]) || MOD[POST](#3,#4[SEM-act-crime]) ) &
     MOD[POST](#4,#5[human]) & HEAD(#5,#6) &
     EVENT[crime](#4) &
     EVENT[agent](#4,#1) )
      EVENT[victim=+](#4,#6)

