Variables: //(used only for debuging)

string DepSubsVerb;    //in which you declare a string variable
string DepSubsDet;     //in which you declare a string variable
string DepSubsFD;      //in which you declare a string variable


DependencyRules:

////////////////////////////////////////////////////////////////////////////////
//
// VLINK DEPENDENCY (between the verbs belonging to a verbal chain)
//
////////////////////////////////////////////////////////////////////////////////

//NJM+JB, 2011/02/18 (added the possibility to have a NP between the two verbs)
//NJM+JB, 2011701/20 (added the possibility to have a PP between the two verbs)
// Example: começou a acordar
//
| VMOD#1;VTEMP#1;VASP#1;SC{?*,VMOD#1};SC{?*,VTEMP#1};SC{?*,VASP#1},
  (NP{PRON};NP{noun[surface:se]}), (NP{PRON};NP{noun[surface:se]}), (ADVP;PP;NP)*,
  #2[verb]
|
 if ( HEAD(#3,#1) & HEAD(#4,#2) )
   VLINK(#3,#4)

//NJM+JB, 2011/02/18 (added the possibility to have a NP between the two verbs)
//NJM+JB, 2011/01/20 (added the possibility to have a PP between the two verbs)
//NJM+JB, 2010/12/30
// Example: começou a, lentamente, acordar
//
| VMOD#1;VTEMP#1;VASP#1;SC{?*,VMOD#1};SC{?*,VTEMP#1};SC{?*,VASP#1},
  (NP{PRON};NP{noun[surface:se]}),(NP{PRON};NP{noun[surface:se]}),
  PUNCT[comma], (ADVP;PP;NP)*, PUNCT[comma],
  #2[verb]
|
 if ( HEAD(#3,#1) & HEAD(#4,#2) )
   VLINK(#3,#4)

//NJM+JB, 2011701/20 (added the possibility to have a PP between the two verbs)
// Example: está convencido
//
| VCOP#1;SC{?*,VCOP#1},
  (NP{PRON};NP{noun[surface:se]}),(NP{PRON};NP{noun[surface:se]}), (ADVP;PP;NP)*,
  VCPART#2
|
 if ( HEAD(#3,#1) & HEAD(#4,#2) )
   VLINK(#3,#4)

//NJM+JB, 2011701/20 (added the possibility to have a PP between the two verbs)
//NJM+JB, 2010/12/30
// Example: está, certamente, convencido
//
| VCOP#1;SC{?*,VCOP#1},
  (NP{PRON};NP{noun[surface:se]}),(NP{PRON};NP{noun[surface:se]}), (ADVP;PP;NP)*,
  PUNCT[comma], (ADVP;PP;NP)*, PUNCT[comma], (ADVP;PP;NP)*,
  VCPART#2
|
 if ( HEAD(#3,#1) & HEAD(#4,#2) )
   VLINK(#3,#4)


//NJM+JB, 2009/03/27 (they seem not to be necessary)
//
//| VPP#1, (NP{PRON};NP{noun[surface:se]}), ADVP*, VINF#2;VGER#2;VCOP#2 |
// if ( HEAD(#3,#1) & HEAD(#4,#2) )
//   VLINK(#3,#4)
//
//| VINF#1;VGER#1;SC{?*,VINF#1};SC{?*,VGER#1}, (NP{PRON};NP{noun[surface:se]}), ADVP*, VINF#2 |
// if ( HEAD(#3,#1) & HEAD(#4,#2) )
//   VLINK(#3,#4)

//NJM+JB, 2011/01/18, 2011/03/15
//Any AUXILIARY verb in gerund form can link to other verb (vinf or vcop) to form a chain
//
// Example: devendo os alunos ser oficialmente integrados  -> VLINK(devendo,ser)
// Example: O Pedro foi-se pondo a ler                     ->   XXXXX NO VLINK IN THIS CASE
//
| VGER#1[verbalfeat];SC{?*,VGER#1[verbalfeat]}, (NP{PRON};NP{noun[surface:se]}), (NP);ADVP*;PP*, VINF#2;VCOP#2 |
 if ( HEAD(#3,#1) & HEAD(#4,#2) )
   VLINK(#3,#4)


////////////////////////////////////////////////////////////////////////////////
//
// VHEAD DEPENDENCY (beteween each verb and the last verb of a verbal chain
//
////////////////////////////////////////////////////////////////////////////////

// Applies the transitive relation on VLINK, generating the VDOMAIN dependency
//   first rule : assures it is the end of the chain and introduces a VHEAD
//                (can be a singleton chain)
//   second rule: iteratively creates to the right a new VHEAD
//
// Example: poderia ter de continuar -> VHEAD(continuar,continuar)
//
| #1[verb] |
 if ( HEAD(#2,#1) & ~VLINK(#2, ?) )
   VHEAD(#2,#2)


// Example: poderia ter de continuar
//            -> VHEAD(poderia,continuar)
//            -> VHEAD(ter,continuar)
//            -> VHEAD(continuar,continuar)
//
 if ( VHEAD(#1,#2) & VLINK(#3,#1) )
   VHEAD(#3,#2)


// Rules to copy "verbalfeat" features to the main verb of a verbal chain
//
 if ( VHEAD(#1,#2) & HEAD(#1,#3[deont])      & HEAD(#2,#4) & #4[deont=+]) ~
 if ( VHEAD(#1,#2) & HEAD(#1,#3[obrig])      & HEAD(#2,#4) & #4[obrig=+]) ~
 if ( VHEAD(#1,#2) & HEAD(#1,#3[pass])       & HEAD(#2,#4) & #4[pass=+]) ~
 if ( VHEAD(#1,#2) & HEAD(#1,#3[futu])       & HEAD(#2,#4) & #4[futu=+]) ~
 if ( VHEAD(#1,#2) & HEAD(#1,#3[terminat])   & HEAD(#2,#4) & #4[terminat=+]) ~
 if ( VHEAD(#1,#2) & HEAD(#1,#3[durativo])   & HEAD(#2,#4) & #4[durativo=+]) ~
 if ( VHEAD(#1,#2) & HEAD(#1,#3[incoat])     & HEAD(#2,#4) & #4[incoat=+]) ~
 if ( VHEAD(#1,#2) & HEAD(#1,#3[frequentat]) & HEAD(#2,#4) & #4[frequentat=+]) ~
 if ( VHEAD(#1,#2) & HEAD(#1,#3[iterat])     & HEAD(#2,#4) & #4[iterat=+]) ~
 if ( VHEAD(#1,#2) & HEAD(#1,#3[imperfect])  & HEAD(#2,#4) & #4[imperfect=+]) ~
 if ( VHEAD(#1,#2) & HEAD(#1,#3[impessoal])  & HEAD(#2,#4) & #4[impessoal=+]) ~
 if ( VHEAD(#1,#2) & HEAD(#1,#3[passive])    & HEAD(#2,#4) & #4[passive=+]) ~



////////////////////////////////////////////////////////////////////////////////
//
// VDOMAIN DEPENDENCY (beteween the first and the last verb of a verbal chain
//
////////////////////////////////////////////////////////////////////////////////

// Example: poderia ter de continuar -> VDOMAIN(poderia,continuar)
//
 if ( VHEAD(#1,#2) & ~VLINK(?,#1) & ~VDOMAIN(#1,#2) )
  VDOMAIN(#1,#2)






////////////////////////////////////////////////////////////////////////////////
//
// PREPD DEPENDENCY
//
////////////////////////////////////////////////////////////////////////////////

// Example: os restantes chefes de amanhã -> PREPD(amanhã,de)
// Example: os chefes dos grupos          -> PREPD(grupos,de)
//
| PP#1{prep#2,?*,#3} |
 if ( HEAD(#3,#1) )
   PREPD(#3,#2)

// Example: para alimentar            -> PREPD(alimentar,para)
// Example: para poder ter alimentado -> PREPD(alimentar,para)
//
| prep#1, ?{verb#2}, ?*, VINF{verb#4} |
 if ( VDOMAIN(#2,#4) & ~PREPD(?,#1) )
   PREPD(#4,#1)

//NJM+JB, 2011/03/30
// PREPD between a preposition and the head of the NP that is determined by
// the relative pronoun "cujo"
//
// Example: O lugar de cuja coisa tinha ardido -> PREPD(coisa,de)
//
| prep#1, SC{?[lemma:cujo], ?*[noun:~], #2[noun]} |
 if ( HEAD(#3,#2) & ~PREPD(?,#1) )
   PREPD(#3,#1)

// PREPD between a preposition and a WH- pronoun
//
// Example: A rapariga a quem ligou                       -> PREPD(quem,a)
// Example: O Pedro queixou-se de que tinha de fazer isso -> PREPD(que,de)
// Example: A laranja da qual o Pedro fez sumo era doce   -> PREPD(qual,de)
//
| prep#1, SC{#2[first,qu];#2[first,surface:qual]} |
 if ( ~PREPD(?,#1) )
   PREPD(#2,#1)



// Distributing dependencies related with coord
//
// Examples : Entre o coração e a memória vive a saudade. -> PREPD(memoria,entre)
if ( COORD(#1,#2) & COORD(#1,#3[pp_not_np]) & PREPD(#2,#4) & #2 < #3 & ~PREPD(#3,#4) )
   PREPD(#3,#4).




////////////////////////////////////////////////////////////////////////////////
//
// Rules to compute a modification when an adverb is found near a verb
//
////////////////////////////////////////////////////////////////////////////////

//NJM+JB, 2011/01/17
// The adverb is after de SC and before another verb
//
// Example: depois de ingerir copiosamente este líquido, você fez isso -> MOD(ingerir,copiosamente)
//
| sc#3, advp#1{adv}, ?*[verb:~,scfeat:~,coord:~,sub:~], ?[verb] |
 if ( HEAD(#2,#3) & HEAD(#4,#1) & VDOMAIN(#2,#5) & ~MOD(?,#4) )
   MOD[post=+](#5,#4)

//NJM+JB, 2011/01/17
// Example: Depois de ter começado a ingerir copiosamente o líquido, o Pedro fez isso -> MOD(ingerir,copiosamente)
//
| #3[verb], advp#1{adv}, ?*[verb:~,scfeat:~,coord:~,sub:~], ?[verb] |
 if ( VDOMAIN(?,#2) & HEAD(#2,#3) & HEAD(#4,#1) & ~MOD(?,#4) )
   MOD[post=+](#2,#4)





//NJM+JB, 2010/12/22 (new restrictions on the non-verb tokens)
// The adverb is before the verb
//
// Example: não continua a compreender -> MOD(compreender,não)
//
| advp#1{adv}, ?*[verb:~,scfeat:~,coord:~,sub:~], #3[verb] |
 if ( VDOMAIN(#2,#5) & HEAD(#2,#3) & HEAD(#4,#1) & ~MOD(?,#4) )
   MOD[pre=+](#5,#4)

//NJM+JB, 2011/01/21 (simplified, since we are not building the verbal node)
// The adverb is inside the begining of verb chunk
//
// Example: pode ter  eventualmente  de comer -> MOD(comer,eventualmente)
// Example: pode ter, eventualmente, de comer -> MOD(comer,eventualmente)
// Example: temos já de resolver o problema   -> MOD(resolver,já)
//
| #1[verb]{verb#2, ?*, adv#3} |
 if ( VDOMAIN(#2,#4) & HEAD(#2,#1) & ~MOD(?,#3) )
   MOD[pre=+](#4,#3)

//NJM+JB, 2011/01/21 (simplified, since we are not building the verbal node)
// The adverb is inside the middle of verb chunk
//
// Example: teve de ir, eventualmente, a nadar -> MOD(nadar,eventualmente)
//
| #1[verb]{verb#2, ?*, adv#3}  |
 if ( VHEAD(#2,#4) & HEAD(#2,#1) & ~MOD(?,#3) )
   MOD[pre=+](#4,#3)



////////////////////////////////////////////////////////////////////////////////
//
// PREDSUBJ DEPENDENCY (between the verb that supports a predicative noun
//                      and that noun)
//
////////////////////////////////////////////////////////////////////////////////


//========================================================
//========================================================
// VSUP = SER DE
//========================================================
//========================================================

// Determinante = <E>
//
// Example: O Zé é [de alimento]PP [muito grande]PP -> PREDSUBJ(é,alimento)
//
| #1[vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}, (ADVP), PP#3{prep[lemma:de],?*,noun#4<Sserde,detE>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detE=+](#2,#4)

// Example: O Zé [que tem sido]SC [de alimento]PP [muito grande]PP -> PREDSUBJ(sido,alimento)
//
| SC{?*,#1[last,vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}}, (ADVP), PP#3{prep[lemma:de],?*,noun#4<Sserde,detE>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detE=+](#2,#4)

// Determinante = <E> with negation "falta de" and similar expressions
//
// Example: O Zé é [de falta]PP    [de categoria]PP [muito grande]PP -> PREDSUBJ(é,categoria) MOD[neg](categoria,falta)
// Example: O Zé é [de ausência]PP [de categoria]PP [muito grande]PP -> PREDSUBJ(é,categoria) MOD[neg](categoria,ausência)
// Example: O Zé é [de excesso]PP  [de categoria]PP [muito grande]PP -> PREDSUBJ(é,categoria) MOD[pos](categoria,excesso)
//
| #1[vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}, (ADVP), PP#5{prep[lemma:de],?*,noun[surface:falta];noun[surface:ausência]}, PP#3{prep[lemma:de],?*,noun#4<Sserde,detE,faltade>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & HEAD(#6,#5) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detE=+,faltade=+](#2,#4), 
   MOD[neg=+](#4,#6)

| #1[vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}, (ADVP), PP#5{prep[lemma:de],?*,noun[surface:excesso]}, PP#3{prep[lemma:de],?*,noun#4<Sserde,detE,faltade>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & HEAD(#6,#5) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detE=+,faltade=+](#2,#4), 
   MOD[pos=+](#4,#6)


// Example: O Zé [que tem sido]SC [de falta]PP    [de categoria]PP [muito grande]PP -> PREDSUBJ(sido,categoria) MOD[neg](categoria,falta)
// Example: O Zé [que tem sido]SC [de ausência]PP [de categoria]PP [muito grande]PP -> PREDSUBJ(sido,categoria) MOD[neg](categoria,ausência)
// Example: O Zé [que tem sido]SC [de excesso]PP  [de categoria]PP [muito grande]PP -> PREDSUBJ(sido,categoria) MOD[pos](categoria,excesso)
//
| SC{?*,#1[last,vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}}, (ADVP), PP#5{prep[lemma:de],?*,noun[surface:falta];noun[surface:ausência]}, PP#3{prep[lemma:de],?*,noun#4<Sserde,detE,faltade>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & HEAD(#6,#5) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detE=+,faltade=+](#2,#4), 
   MOD[neg=+](#4,#6)

| SC{?*,#1[last,vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}}, (ADVP), PP#5{prep[lemma:de],?*,noun[surface:excesso]}, PP#3{prep[lemma:de],?*,noun#4<Sserde,detE,faltade>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & HEAD(#6,#5) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detE=+,faltade=+](#2,#4), 
   MOD[pos=+](#4,#6)


// Determinante = UM + MODIF
//
// Example: O Zé é de uma coragem.
// Example: O Zé é de uma grande coragem.
// Example: O Zé é de uma coragem grande.
//
| #1[vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}, (ADVP), PP#3{prep[lemma:de],art[lemma:"um"],?*,noun#4<Sserde,detumm>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detumm=+](#2,#4)

// Example: O Zé [que tem sido]SC de uma coragem.
// Example: O Zé [que tem sido]SC de uma grande coragem.
// Example: O Zé [que tem sido]SC de uma coragem grande.
//
| SC{?*,#1[last,vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}}, (ADVP), PP#3{prep[lemma:de],art[lemma:"um"],?*,noun#4<Sserde,detumm>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detumm=+](#2,#4)


// Determinante = UM + MODIF with negation "falta de" and similar expressions
//
// Example: O Zé é [de uma falta]PP    [de coragem]PP [muito grande]PP -> PREDSUBJ[](é,coragem) MOD[neg](coragem,falta)
// Example: O Zé é [de uma ausência]PP [de coragem]PP [muito grande]PP -> PREDSUBJ[](é,coragem) MOD[neg](coragem,ausência)
// Example: O Zé é [de um excesso]PP   [de coragem]PP [muito grande]PP -> PREDSUBJ[](é,coragem) MOD[pos](coragem,excesso)
//
| #1[vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}, (ADVP), PP#5{prep[lemma:de],art[lemma:"um"],?*,noun[surface:falta];noun[surface:ausência]}, PP#3{prep[lemma:de],?*,noun#4<Sserde,detumm,faltade>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & HEAD(#6,#5) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detumm=+,faltade=+](#2,#4), 
   MOD[neg=+](#4,#6)

| #1[vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}, (ADVP), PP#5{prep[lemma:de],art[lemma:"um"],?*,noun[surface:excesso]}, PP#3{prep[lemma:de],?*,noun#4<Sserde,detumm,faltade>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & HEAD(#6,#5) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detumm=+,faltade=+](#2,#4), 
   MOD[pos=+](#4,#6)

// Example: O Zé [que tem sido]SC [de uma falta]PP    [de coragem]PP -> PREDSUBJ[](é,coragem) MOD[neg](coragem,falta)
// Example: O Zé [que tem sido]SC [de uma ausência]PP [de coragem]PP -> PREDSUBJ[](é,coragem) MOD[neg](coragem,ausência)
// Example: O Zé [que tem sido]SC [de um excesso]PP   [de coragem]PP -> PREDSUBJ[](é,coragem) MOD[pos](coragem,excesso)
//
| SC{?*,#1[last,vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}}, (ADVP), PP#5{prep[lemma:de],art[lemma:"um"],?*,noun[surface:falta];noun[surface:ausência]}, PP#3{prep[lemma:de],?*,noun#4<Sserde,detumm,faltade>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & HEAD(#6,#5) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detumm=+,faltade=+](#2,#4), MOD[neg=+](#4,#6)

| SC{?*,#1[last,vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}}, (ADVP), PP#5{prep[lemma:de],art[lemma:"um"],?*,noun[surface:excesso]}, PP#3{prep[lemma:de],?*,noun#4<Sserde,detumm,faltade>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & HEAD(#6,#5) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detumm=+,faltade=+](#2,#4), MOD[pos=+](#4,#6)


// Determinante = ARTIGO_DEFINIDO
//
// Example: O Zé é [da confiança]PP [da Ana]PP -> PREDSUBJ[](é,confiança)
//
| #1[vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}, (ADVP), PP#3{prep[lemma:do],?*,noun#4<Sserde,detdef>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detdef=+](#2,#4)

// Example: O Zé [que tem sido]SC [da confiança]PP [da Ana]PP -> PREDSUBJ[](sido,confiança)
//
| SC{?*,#1[last,vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}}, (ADVP), PP#3{prep[lemma:do],?*,noun#4<Sserde,detdef>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detdef=+](#2,#4)


// Determinante = ARTIGO_DEFINIDO with negation "falta de" and similar expressions
//
// Example: O Zé é [da falta]PP    [de confiança]PP [da Ana]PP -> PREDSUBJ[](é,confiança) MOD[neg](confiança,falta)
// Example: O Zé é [da ausência]PP [de confiança]PP [da Ana]PP -> PREDSUBJ[](é,confiança) MOD[neg](confiança,ausência)
// Example: O Zé é [do excesso]PP  [de confiança]PP [da Ana]PP -> PREDSUBJ[](é,confiança) MOD[pos](confiança,excesso)
//
| #1[vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}, (ADVP), PP#5{prep[lemma:do],?*,noun[surface:falta];noun[surface:ausência]}, PP#3{prep[lemma:de],?*,noun#4<Sserde,detdef,faltade>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & HEAD(#6,#5) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detdef=+,faltade=+](#2,#4), MOD[neg=+](#4,#6)

| #1[vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}, (ADVP), PP#5{prep[lemma:do],?*,noun[surface:excesso]}, PP#3{prep[lemma:de],?*,noun#4<Sserde,detdef,faltade>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & HEAD(#6,#5) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detdef=+,faltade=+](#2,#4), MOD[pos=+](#4,#6)


// Example: O Zé [que tem sido]SC [da falta]PP    [de confiança]PP [da Ana]PP -> PREDSUBJ[](sido,confiança) MOD[neg](confiança,falta)
// Example: O Zé [que tem sido]SC [da ausência]PP [de confiança]PP [da Ana]PP -> PREDSUBJ[](sido,confiança) MOD[neg](confiança,ausência)
// Example: O Zé [que tem sido]SC [do excesso]PP  [de confiança]PP [da Ana]PP -> PREDSUBJ[](sido,confiança) MOD[pos](confiança,excesso)
//
| SC{?*,#1[last,vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}}, (ADVP), PP#5{prep[lemma:do],?*,noun[surface:falta];noun[surface:ausência]}, PP#3{prep[lemma:de],?*,noun#4<Sserde,detdef,faltade>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & HEAD(#6,#5) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detdef=+,faltade=+](#2,#4), MOD[neg=+](#4,#6)

| SC{?*,#1[last,vsup=+]{VERB#2[lemma:"ser"];PASTPART#2[lemma:"ser"], ?-}}, (ADVP), PP#5{prep[lemma:do],?*,noun[surface:excesso]}, PP#3{prep[lemma:de],?*,noun#4<Sserde,detdef,faltade>} |
 if ( VDOMAIN(?,#2) & HEAD(#2,#1) & HEAD(#4,#3) & HEAD(#6,#5) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ[Sserde=+,detdef=+,faltade=+](#2,#4), MOD[pos=+](#4,#6)




//========================================================
//========================================================
// VSUP = TER
//========================================================
//========================================================

// Determinante = <E>
//
