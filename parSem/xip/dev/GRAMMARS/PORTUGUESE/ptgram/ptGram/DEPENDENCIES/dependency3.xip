////////////////////////////////////////////////////////////////////////////////
//
// GENERAL RULES HANDLING COPULAS and NOUNS and COPULAS and ADJECTIVES
//
////////////////////////////////////////////////////////////////////////////////

//NJM+JB, 2010/11/19
// Example: o problema é alguns dos seus alunos -> PREDSUBJ(é,alunos)
//
| #1{VERB#2[cop];PASTPART#2[cop], ?-}, (ADVP), NP#3, PP#5 |
 if ( HEAD(#2,#1) & VDOMAIN(?,#2) & HEAD(#4,#3) & HEAD(#6,#5) & QUANTD(#6,#4) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ(#2,#6)

//NJM+JB, 2011/01/19
// Example: O Pedro disse que estava tudo perdido
//
| SC#1{?*,?{VERB#2[cop];PASTPART#2[cop]}}, (ADVP), NP#5, (ADVP), NP#3;AP#3 |
 if ( HEAD(#2,#1) & VDOMAIN(#2,#7) & HEAD(#4,#3) & HEAD(#6[lemma:tudo],#5) & ~PREDSUBJ(#2,?) )
   PREDSUBJ(#7,#4),
   SUBJ[pre=+](#7,#6)

//NJM+JB, 2011/01/19
// Example: estava tudo doente
//
| #1{VERB#2[cop];PASTPART#2[cop], ?-}, (ADVP), NP#5, (ADVP), NP#3;AP#3 |
 if ( HEAD(#2,#1) & VDOMAIN(?,#2) & HEAD(#4,#3) & HEAD(#6[lemma:tudo],#5) & ~PREDSUBJ(#2,?) )
   PREDSUBJ(#2,#4),
   SUBJ[post=+](#2,#6)


//NJM+JB, 2013/01/16
// Example: estavam todos contentes -> PREDSUBJ(estavam,contentes)
//                                    
| #1{verb#2[cop];pastpart#2[cop], ?-}, (ADVP), NP#5, (ADVP), AP#3 |
 if ( HEAD(#2,#1) & VDOMAIN(?,#2) & HEAD(#4,#3) & HEAD(#6[lemma:todo],#5) & ~PREDSUBJ(#2,?) )
   PREDSUBJ(#2,#4)



// Example: O Zé é um rapaz
// Example: O Zé continua inteligente
// Example: O Zé fica cansado
// Example: O Zé está confiante
// Example: O Zé é lindo
//
// Example: O Zé que tinha sido um rapaz bonito
// Example: O Zé que podia continuar inteligente
// Example: O Zé que tinha de ficar cansado
// Example: O Zé que podia estar confiante
// Example: O Zé que já tinha sido lindo
//
| #1{?*, VERB#2[cop];PASTPART#2[cop], ?-}, (ADVP), NP#3;AP#3 |
 if ( HEAD(#2,#1) & VDOMAIN(?,#2) & HEAD(#4,#3) & ~PREDSUBJ(#2,?) )
   PREDSUBJ(#2,#4)

// Example: O Zé é dos mais compridos
//
| #1{?*, VERB#2[cop];PASTPART#2[cop], ?-}, (ADVP), PP#3 |
 if ( HEAD(#2,#1) & VDOMAIN(?,#2) & HEAD(#4,#3) & PREPD(#4,#5[lemma:de]) & ~PREDSUBJ(#2,?) )
   PREDSUBJ(#2,#4)

// Example: O Zé que é um rapaz bonito    -> PREDSUBJ(ser,bonito)
// Example: O Zé que continua inteligente -> PREDSUBJ(continua,inteligente)
// Example: O Zé que fica cansado         -> PREDSUBJ(fica,cansado)
// Example: O Zé que está confiante       -> PREDSUBJ(está,confiante)
// Example: O Zé que é lindo              -> PREDSUBJ(é,lindo)
//
| SC{?*,#1{VERB#2[cop];PASTPART#2[cop]}}, (ADVP), NP#3;AP#3 |
 if ( HEAD(#2,#1) & VDOMAIN(?,#2) & HEAD(#4,#3) & ~PREDSUBJ(#2,?) )
   PREDSUBJ(#2,#4).

// Example: O Zé que é dos mais lindos -> PREDSUBJ(é,lindo)
//
| SC{?*,#1{VERB#2[cop];PASTPART#2[cop]}}, (ADVP), PP#3 |
 if ( HEAD(#2,#1) & VDOMAIN(?,#2) & HEAD(#4,#3) & PREPD(#4,#5[lemma:de]) & ~PREDSUBJ(#2,?) )
   PREDSUBJ(#2,#4).


////////////////////////////////////////////////////////////////////////////////
//
// GENERAL RULES HANDLING COPULAS and PREPOSITIONS
//
////////////////////////////////////////////////////////////////////////////////

//NJM+JB, 2013/05/08
// Example: O Pedro está ao pé do João -> PREDSUBJ(está,ao pé de)
//                                     -> MOD(ao pé de,João)
//
| #1{VERB#2[cop];PASTPART#2[cop]}, (ADVP), PP#3{prep#5[preploc], ?-} |
 if ( HEAD(#2,#1) & VDOMAIN(?,#2) & HEAD(#4,#3) & ~PREDSUBJ(#2,#5) & ~PREPD(?[SEM-unitype],#5[lemma:a]) )
   PREDSUBJ(#2,#5),
   MOD[post=+](#5,#4).




////////////////////////////////////////////////////////////////////////////////
//
//  ADJECTIVES THAT ARE PREDICATIVE
//
////////////////////////////////////////////////////////////////////////////////


// PREPOSITION: a (prepA)
//
// Example: O Zé é (muito) aberto a falar com os pais -> COMPL(aberto,falar)
//
| AP#1{?*,adj#2[prepA]}, PP#3{prep[lemma:"a"]} |
 if ( PREDSUBJ(?,#2) & HEAD(#4,#3) & ~COMPL(#2,#4) )
   COMPL[prep1A=+](#2,#4)

// PREPOSITION: com (prepCOM)
//
// Example: O Zé é (muito) esquisito com a comida -> COMPL(esquisito,comida)
//
| AP#1{?*,adj#2[prepCOM]}, PP#3{prep[lemma:"com"]} |
 if ( PREDSUBJ(?,#2) & HEAD(#4,#3) & ~COMPL(#2,#4) )
   COMPL[prep1COM=+](#2,#4)

