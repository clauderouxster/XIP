Vocabulary:

//==============================================================================
// Adding feature advcomp
//==============================================================================
mais:                 adv += [advcomp=+].
menos:                adv += [advcomp=+].
tão:                  adv += [advcomp=+].
comparativamente:     adv += [advcomp=+].
igualmente:           adv += [advcomp=+].


//==============================================================================
// Adding feature neg
//==============================================================================
jamais:               adv += [neg=+].
nada:                 adv += [neg=+].
não:                  adv += [neg=+].
nem:                  adv += [neg=+].
nunca:                adv += [neg=+].


//==============================================================================
// Adding feature proxadv
//==============================================================================
cá:                   adv += [proxadv=+].
lá:                   adv += [proxadv=+].
