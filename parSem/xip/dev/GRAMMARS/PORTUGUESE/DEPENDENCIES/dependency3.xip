////////////////////////////////////////////////////////////////////////////////
//
// GENERAL RULES HANDLING COPULAS and NOUNS and COPULAS and ADJECTIVES
//
////////////////////////////////////////////////////////////////////////////////

// Example: O Zé é um rapaz
// Example: O Zé continua inteligente
// Example: O Zé fica cansado
// Example: O Zé está confiante
// Example: O Zé é lindo
//
// Example: O Zé que tinha sido um rapaz bonito
// Example: O Zé que podia continuar inteligente
// Example: O Zé que tinha de ficar cansado
// Example: O Zé que podia estar confiante
// Example: O Zé que já tinha sido lindo
//
| #1{VERB#2[cop];PASTPART#2[cop], ?-}, (ADVP), NP#3;AP#3 |
 if ( HEAD(#2,#1) & VDOMAIN(?,#2) & HEAD(#4,#3) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ(#2,#4)

// Example: O Zé é dos mais compridos
//
| #1{VERB#2[cop];PASTPART#2[cop], ?-}, (ADVP), PP#3 |
 if ( HEAD(#2,#1) & VDOMAIN(?,#2) & HEAD(#4,#3) & PREPD(#4,#5[lemma:de]) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ(#2,#4)

// Example: O Zé que é um rapaz bonito    -> PREDSUBJ(ser,bonito)
// Example: O Zé que continua inteligente -> PREDSUBJ(continua,inteligente)
// Example: O Zé que fica cansado         -> PREDSUBJ(fica,cansado)
// Example: O Zé que está confiante       -> PREDSUBJ(está,confiante)
// Example: O Zé que é lindo              -> PREDSUBJ(é,lindo)
//
| SC{?*,#1{VERB#2[cop];PASTPART#2[cop]}}, (ADVP), NP#3;AP#3 |
 if ( HEAD(#2,#1) & VDOMAIN(?,#2) & HEAD(#4,#3) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ(#2,#4)

// Example: O Zé que é dos mais lindos -> PREDSUBJ(é,lindo)
//
| SC{?*,#1{VERB#2[cop];PASTPART#2[cop]}}, (ADVP), PP#3 |
 if ( HEAD(#2,#1) & VDOMAIN(?,#2) & HEAD(#4,#3) & PREPD(#4,#5[lemma:de]) & ~PREDSUBJ(#2,#4) )
   PREDSUBJ(#2,#4)



////////////////////////////////////////////////////////////////////////////////
//
//  ADJECTIVES THAT ARE PREDICATIVE
//
////////////////////////////////////////////////////////////////////////////////


// PREPOSITION: a (SprepA)
//
// Example: O Zé é (muito) aberto a falar com os pais -> COMPL(aberto,falar)
//
| AP#1{?*,adj#2[SprepA]}, PP#3{prep[lemma:"a"]} |
 if ( PREDSUBJ(?,#2) & HEAD(#4,#3) & ~COMPL(#2,#4) )
   COMPL[SprepA=+](#2,#4)

// PREPOSITION: com (SprepCOM)
//
// Example: O Zé é (muito) esquisito com a comida -> COMPL(esquisito,comida)
//
| AP#1{?*,adj#2[SprepCOM]}, PP#3{prep[lemma:"com"]} |
 if ( PREDSUBJ(?,#2) & HEAD(#4,#3) & ~COMPL(#2,#4) )
   COMPL[SprepCOM=+](#2,#4)

// PREPOSITION: com (SprepDPD)
//
// Example: ???
//
| AP#1{?*,adj#2[SprepDPD]}, PP#3{prep{prep[lemma:"do"],noun[lemma:"parte"], prep},?*} |
 if ( PREDSUBJ(?,#2) & HEAD(#4,#3) & ~COMPL(#2,#4) )
   COMPL[SprepDPD=+](#2,#4)


// PREPOSITION: de (SprepDE)
//
// Example: A fruta é agradável de paladar -> COMPL(agradável,paladar)
//
| AP#1{?*,adj#2[SprepDE]}, PP#3{prep[lemma:"de"]};PP#5{prep[lemma:"do"]} |
 if ( PREDSUBJ(?,#2) & HEAD(#4,#3) & ~COMPL(#2,#4) )
   COMPL[SprepDE=+](#2,#4)

// PREPOSITION: com (SprepEM)
//
// Example: A lei é aplicável em todo o país -> COMPL(aplicável,país)
//
| AP#1{?*,adj#2[SprepEM]}, PP#3{prep[lemma:"em"]};PP#3{prep[lemma:"no"]} |
 if ( PREDSUBJ(?,#2) & HEAD(#4,#3) & ~COMPL(#2,#4) )
   COMPL[SprepEM=+](#2,#4)

// PREPOSITION: com (SprepENTRE)
//
// Example: A lei é impopular entre os estudantes  -> COMPL(impopular,estudantes)
//
| AP#1{?*,adj#2[SprepENTRE]}, PP#3{prep[lemma:"entre"]} |
 if ( PREDSUBJ(?,#2) & HEAD(#4,#3) & ~COMPL(#2,#4) )
   COMPL[SprepENTRE=+](#2,#4)

// PREPOSITION: com (SprepERA)
//
// Example: A Zé é autónoma em relação aos estudos -> COMPL(autónoma,estudos)
//
| AP#1{?*,adj#2[SprepERA]}, PP#3{prep{?[lemma:"em"],?[lemma:"relação"],prep},?*};PP#3{prep{?[lemma:"relativamente"],prep},?*} |
 if ( PREDSUBJ(?,#2) & HEAD(#4,#3) & ~COMPL(#2,#4) )
   COMPL[SprepERA=+](#2,#4)

// PREPOSITION: com (SprepJD)
//
// Example: A Zé é impopular junto dos colegas -> COMPL(impopular,colegas)
//
| AP#1{?*,adj#2[SprepJD]}, PP#3{prep{?[lemma:"junto"],prep},?*} |
 if ( PREDSUBJ(?,#2) & HEAD(#4,#3) & ~COMPL(#2,#4) )
   COMPL[SprepJD=+](#2,#4)

// PREPOSITION: com (SprepPARA)
//
// Example: A selecção é importante para os trabalhadores -> COMPL(importante,trabalhadores)
//
| AP#1{?*,adj#2[SprepPARA]}, PP#3{prep[lemma:"para"]} |
 if ( PREDSUBJ(?,#2) & HEAD(#4,#3) & ~COMPL(#2,#4) )
   COMPL[SprepPARA=+](#2,#4)

// PREPOSITION: com (SprepPC)
//
