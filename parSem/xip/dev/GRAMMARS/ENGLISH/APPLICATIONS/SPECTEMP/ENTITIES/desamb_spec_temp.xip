
Marker:

// // // // //	MARCH
// Desamb rule in desamb_spec kills month:3 for March
//   re-put it (2 rules, the first one to remove month:+, the second one to add month:3)
1> noun[month=~+,proper:+,period,time:+, lemme:march].
1> noun[month=3,proper:+,period,time:+, lemme:march].


// // // // //	JULY
// Same thing (than previous rule) for July
// XT translated month:+ into month:7
1> noun[time:+,period,lemme:July,month=~+].
1> noun[time:+,period,lemme:July,month=7].


// // // // // WED
// Same thing (than previous rule) for Wed(nesday)
1> noun[day=~+,time:+,lemme:wed].
1> noun[day=3,time:+,lemme:wed].



// // // // // AM
// Same thing (than previous rule) for AM (like in 11 AM)
1> adv[surface:am, adv:+,posthour=~+] .
1> adv[surface:am, adv:+,posthour=am] .
