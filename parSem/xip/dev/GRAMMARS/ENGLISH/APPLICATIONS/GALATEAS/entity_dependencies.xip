DependencyRules:

// >>>>>>>>>>>>>EXTRACTIONS 


|NOUN#1[share]|
        SHAREHOLDER(#1)


|NOUN#1[tutu]|
        LIXO(#1)

// GKLS
|NOUN#1[model]|
        CAR_MODEL(#1)

|NOUN#1{?*,NOUN#2}|
        if( CAR_MODEL(#1) & ^CAR_MODEL(#2))
        ~

// Basic stuff 


|NOUN#1[prize,proper]|
	if( ~PRIZE(#1))
	PRIZE(#1)

|NOUN#1[place,ambig:~,org:~,address:~]|
        LOCATION(#1)

|NOUN#1[place,ambig:~,org,country:~,city:~,usastate:~,contin:~]|
        LOCORG(#1)

//| NOUN#2[productname]| 
//      PRODUCT(#2)

 | NOUN#2[org,place:~,alsoloc:~]| 
        if(~ORGANISATION(#2))
        ORGANISATION(#2)

 | NOUN#2[org,alsoloc,country:~,city:~,usastate:~,contin:~]|  
     if(~LOCORG(#2))
        LOCORG(#2)


 | NOUN#2[bus,ambig:~,let:~,place:~]| 
        if(~ORGANISATION(#2))
        ORGANISATION(#2)

 | NOUN#2[person,ambig:~]| 
        if (~PERSON(#2))
        PERSON(#2)

 | NOUN#2[proper,celeb,toutmaj:~,ambig:~]| 
        if (~PERSON(#2))
        PERSON(#2)

 | NOUN#2[proper,celeb,fullname,ambig:~]| 
        if (~PERSON(#2))
        PERSON(#2)

 | NOUN#2{NADJ[country],NOUN[title],NOUN#3} |
        if (~PERSON(#3))
        PERSON(#3)

// |DIG#2[year,percent:~]|
     //   YEAR(#2)

// |NOUN#1[quoted]| 
//        QUOTED_ELEMENT(#1)

 |NOUN#2[city,ambig:~,org,abr:~]|    
        LOCORG[city=+](#2)

 |NOUN#2[city,ambig:~,org,abr,toutmaj:~]|    
        LOCORG[city=+](#2)

 |NOUN#2[country,ambig:~,org]|
        LOCORG[country=+](#2)
  
 |NOUN#2[address,mwe]|
        LOCATION[address=+](#2)

  |NOUN#2[place,contin,ambig:~,abr:~,org]|
        LOCORG[contin=+](#2)

  |NOUN#2[place,country:~,city:~,contin:~,address:~,ambig:~,abr:~,org:~]|
        LOCATION(#2)

 |NOUN#2[place,country:~,city:~,contin:~,address:~,ambig:~,abr:~,org]|
        if(~LOCORG(#2))
        LOCORG(#2)

| NOUN#2[bus,place]| 
        LOCORG(#2) 
 
 |NOUN#1{?*,NOUN#2}|
        if(^LOCATION(#2) & LOCATION(#1))
        ~

|NOUN#1{?*,NOUN#2}|
        if(^LOCORG(#2) & LOCORG(#1))
        ~

 |DIG#2[date,mwe]|
          TEMPEXPR[date=+](#2)

 |ADV#1[tempadv] |
        TEMPEXPR(#1) 

 |ADV#1[tempexpr] |
        TEMPEXPR(#1) 

 |NOUN#1[time,date:~,bus:~] |
        TEMPEXPR(#1) 

 |NOUN#2[date,proper:~]|
        TEMPEXPR[date=+](#2)

  |DIG#2[date]|
        TEMPEXPR[date=+](#2)

 |NOUN#1[phone]|
        PHONE(#1)

 |NOUN#1[url]|
        URL(#1)

 |NOUN#1[email]|
        EMAIL(#1)

|NOUN#1[hour]|
        HOUR(#1)

// |NP#1[tempexpr,date:~]{?[first,tempexpr:~]} |
    //    TEMPEXPR(#1)

|NOUN#1[event]|
        EVENT(#1)

|NOUN#1[law]|
        LAW(#1)
 
//|NP#1[tempexpr]|
  //      TEMPEXPR(#1)

 //|PP#1[tempexpr]{PREP,?[date:~,dig:~]}|
 //       TEMPEXPR(#1)

 |PP#1[tempexpr]{PREP,DIG#2[year:+]}|
        TEMPEXPR[date:+](#2)

//|PP#1{PREP[form:fin],NP{DIG#2[shortyear:+]}}|
//        TEMPEXPR[date:+](#2)

 |NUM#1[mwe,card,money:~]|
        COMPLEXNUM(#1)

// just a money dig
// pb with multiple extraction

 |DIG#2[money];NOUN#2[money];NUM#2[money]|
        MONEY(#2)

// percentages 

  |NUM#1{DIG,NOUN[percent:+]}|
        PERCENT(#1)

//2% to 3%
  |DIG#1{DIG[percent:+]}|
        PERCENT(#1)

  |DIG#1[percent:+]|
        PERCENT(#1)

  |DIG#1{?*,DIG#2[percent:+]}|
        if (^PERCENT(#2) & PERCENT(#1))
        ~
// units like 346 km/h

 |NOUN#1[mwe,unit]|
        UNIT(#1) 



|?[det:~], NOUN#1[firstname,pl:~,hmm_verb:~,hmm_vbi:~,hmm_vbpap:~,hmm_vbpast:~,hmm_vbprog:~,
         hmm_vpap:~,hmm_vpast:~,hmm_noun:~,hmm_aux:~,hmm_adj:~,hmm_nadj:~,misc:~,place:~,ambig:~,toutmaj:~,city:~,country:~]|
        if(ORG##1:0)
        PERSON(#1)


| NOUN#1[firstname,hmm_verb:~,hmm_vbi:~,pl:~,hmm_vbpap:~,hmm_vbpast:~,hmm_vbprog:~,
         hmm_vpap:~,hmm_vpast:~,hmm_noun:~,hmm_aux:~,hmm_adj:~,hmm_nadj:~,misc:~,place:~,ambig:~,toutmaj:~,city:~,country:~]|
        if(ORG##1:0)
        PERSON(#1)

// *** FOR GALATEAS *** 

| NOUN#1[proper,fam,title:~]|
	  PERSON(#1)

|NOUN#1[titleart] |
	TITLEART(#1)

//**** 

|NOUN#1{NOUN#2,PUNCT[form:fhyph],NOUN#3}|
        if (LOCATION(#2) & LOCORG(#3))
        LOCORG(#1)

|NOUN#1{NOUN#2,PUNCT[form:fhyph],NOUN#3}|
        if (LOCATION(#2) & LOCATION(#3))
        LOCATION(#1)

|NOUN#1{NOUN#2,PUNCT[form:fhyph],NOUN#3}|
        if (LOCORG(#2) & LOCORG(#3))
        LOCORG(#1)


// FOR EVALUATION PURPOSES 
// the name is DATE ;

if(TEMPEXPR[date](#1)) 
        DATE(#1)


if(DATE(#1) & ^TEMPEXPR[date](#1))
        ~


