function addupoffdir(dependency dep,xml n) {

   smap props=n.properties();
   props["off-dir"]=dep.parameters()[0].surface();
   string nom=dep.name();
   if (props.test(nom)) {
     props[nom]+="/"+dep.parameters()[0].surface();
   }
   else
     props[nom]=dep.parameters()[0].surface();
   n.properties(props);
}


