Vocabulary:

renew : ? += [renewal=+].
renewal : ? += [renewal=+].
autorenewal : ? += [renewal=+].
extend : ? += [renewal=+].
extension : ? += [renewal=+].
notice : ? += [notification=+].
notify : ? += [notification=+].
notification : ? += [notification=+].
cancel : ? += [termination=+].
cancellation : ? += [termination=+].
terminate : ? += [termination=+].
termination : ? += [termination=+].
terminable : ? += [termination=+].
end : ? += [termination=+].
expiration : ? += [termination=+].
expire : ? += [termination=+].


month : ? += [!renewaltime=+,!renewmonth=+].
year : ? += [!renewaltime=+].

// hour,hour are not e tempexr and a period here
hour : noun[noun=+,countable=+,sg=+] = noun[noun=+,countable=+,sg=+].
hour : noun[noun=+,countable=+,pl=+] = noun[noun=+,countable=+,pl=+].
time : noun[noun=+,countable=+,sg=+] = noun[noun=+,countable=+,sg=+].
time : noun[noun=+,countable=+,pl=+] = noun[noun=+,countable=+,pl=+].

// cancel within 15 days - is never a cancallation of renewal

within : ? += [!negprep=+].
after : ? += [!negprep=+].


// negative words

insurance : ? += [negword=+].
policy : ? += [negword=+].

// temporal expressions for continuation

automatic : ? += [timefact=+].
automatically : ? += [timefact=+].
will : ? += [timefact=+].
shall : ? += [timefact=+].
continue : ? += [timefact=+].
continued : ? += [timefact=+].
deem : ? += [timefact=+].
deemed : ? += [timefact=+].
term : ? += [timefact=+].
lease : ? += [timefact=+].

until : ? += [tempconj=+].
unless : ? += [tempconj=+].

day : ? += [!noteday=+].


// For Named Entities 

State :noun  += [wordpart=~,proper=~].
corporation : noun  += [orghead=~].
Dale += [firstname=~].
al += [particlename=~].
Lou += [firstname=~].
Coproration  : noun  += [orghead=+].
Otis  += [firstname=~].
%d%C+ += [proper=~].