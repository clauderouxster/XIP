Vocabulary:

start : noun += [focalisation:debut].
begin : noun += [focalisation:debut].
middle : noun += [focalisation:milieu].
middle : nadj += [focalisation:milieu].
end : noun += [focalisation:fin].

around : prep += [regionalisation:vers].
about : prep += [regionalisation:vers].
before : prep += [regionalisation:avant,dep_orientation:avant].
before : conj += [regionalisation:avant,dep_orientation:avant].
after : prep += [regionalisation:apres,dep_orientation:apres].
since : prep += [regionalisation:depuis].
until : prep += [regionalisation:jusque].

second : noun += [dep_grain:secondes].
minute : noun += [dep_grain:minutes].
hour : noun += [dep_grain:heures].
day : noun += [dep_grain:jours].
month : noun += [dep_grain:mois].
year : noun += [dep_grain:annees].
century : noun += [dep_grain:siecles].
millenium : noun += [dep_grain:millenaires].

Monday : ? += [dep_grain:jours].
Tuesday : ? += [dep_grain:jours].
Wednesday : ? += [dep_grain:jours].
Thursday : ? += [dep_grain:jours].
Friday : ? += [dep_grain:jours].
Saturday : ? += [dep_grain:jours].
Sunday : ? += [dep_grain:jours]. 

January : ? += [dep_grain:mois].
February : ? += [dep_grain:mois].
March : noun += [maj:+,dep_grain=mois].
April : ? += [dep_grain:mois].
May : noun += [maj:+,dep_grain=mois].
June : ? += [dep_grain:mois].
July : ? += [dep_grain:mois].
August : noun += [maj:+,dep_grain:mois].
September : ? += [dep_grain:mois].
October : ? += [dep_grain:mois].
November : ? += [dep_grain:mois].
December : ? += [dep_grain:mois].