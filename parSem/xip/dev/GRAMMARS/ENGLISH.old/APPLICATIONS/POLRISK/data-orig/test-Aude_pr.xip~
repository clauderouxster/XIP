

DependencyRules:

|#1[indspeech]|
	TEEST(#1)

|#1{?*,#2,?*}|
if (^TEEST(#1) & TEEST(#2))
	~

if (TEEST(#1[say:~]))
	TEEEST(#1)

|TOP#8 { ?+ }|
	if (TEEEST(#1))
	POTPOLEVENTSENT(#8)

/
| TOP#8 { ?+ } |
if ( $1[teest](#1,#2)|$1[teest](#1))
	POTPOLEVENTSENT(#8).


| TOP#8 { ?+ } |
if ( MODALITIES(#1,#2)|MODALITIES(#1,#2,#3)|MODALITIES(#1,#2,#3,#4)|MODALITIES(#1,#2,#3,#4,#5)|MODALITIES(#1,#2,#3,#4,#5,#6)|MODALITIES(#1,#2,#3,#4,#5,#6,#7))
	POTPOLEVENTSENT(#8).



| TOP#8 { ?+ } |
if ( EVENT(#4,#1,#2,#3,#5,#6))
	POTPOLEVENTSENT(#8).


if ( EVENT(#4,#1,#2,#3))
	TEEST(#4,#1,#2,#3)

if ( EVENT(#4,#1))
	TEEST(#4,#1)
if ( EVENT(#4,#1,#2))
	TEEST(#4,#1,#2)

if ( EVENT(#4,#1,#2,#3,#5))
	TEEST(#4,#1,#2,#3,#5)


| TOP#8 { ?+ } |
if (TEEST(#4,#1,#2,#3) | TEEST(#4,#1,#2) |TEEST(#4,#1) )
	POTPOLEVENTSENT(#8).
\
