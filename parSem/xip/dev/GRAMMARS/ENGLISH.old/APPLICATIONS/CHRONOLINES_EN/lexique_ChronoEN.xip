Vocabulary:


// Feature used especifically in Chronolines English application
since += [temp_pos=">"].
from += [temp_pos=">"].
until += [temp_pos="<"].

mid += [noun=+,time_anchor=+].

of += [notime:+].
for += [notime:+].
above += [notime:+].
beyond += [notime:+].


previous : adj += [preperiod:+].
