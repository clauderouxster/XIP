!# MARKERS 
!# !! = in both sources
!# !* = from old FSTs
!# ============================================================================

Multichar_Symbols

+Noun +Prop +Adj 

+Continent
+Country

+Sg +Pl +SP
+Masc +Fem +MF

+Acron +Abbr


! special markers
(.)	! optional dot
(-)	! optional hyphen
[-]	! hyphen or space

^]	! lemma boundary (for inflecting proper names)



!# ------------------------------------------------------------------------
!# Continuation classes

LEXICON	MSG	+Masc+Sg	#;
LEXICON	FSG	+Fem+Sg		#;
LEXICON	MPL	+Masc+Pl	#;
LEXICON	FPL	+Fem+Pl		#;

LEXICON MSGPL	MSG; MPL;
LEXICON FSGPL	FSG; FPL;

LEXICON MFSGPL	MSGPL; FSGPL;

!# plural = singular
LEXICON MSP	+Masc+SP	#;
LEXICON FSP	+Fem+SP		#;
LEXICON MFSP	MSP; FSP;

!# masc = fem
LEXICON XSG	+MF+Sg		#;
LEXICON XPL	+MF+Pl		#;
LEXICON XSGPL	XSG; XPL;

!# masc = fem; plural = singular
LEXICON XSP	+MF+SP		#;

!# =============================================================================
!# MAIN LEXICON
!# =============================================================================

LEXICON ROOT 

