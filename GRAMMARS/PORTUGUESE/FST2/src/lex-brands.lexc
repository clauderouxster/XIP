!# ============================================================================
!# sources:
!# ADD/lexBrands.xip
!#
!# ============================================================================
!# NOTE: names from XIP lexicons are  missing gender ("XSG")
!# 	 may require further classification
!# ============================================================================


Multichar_Symbols

+Noun +Prop +Adj 

+Brand


+Sg +Pl +SP
+Masc +Fem +MF

+Postmod
+Acron +Abbr


! special markers
(.)	! optional dot
(-)	! optional hyphen
[-]	! hyphen or space

^]	! lemma boundary (for inflecting proper names)



!# ------------------------------------------------------------------------
!# Continuation classes

LEXICON	MSG	+Masc+Sg	#;
LEXICON	FSG	+Fem+Sg		#;
LEXICON	MPL	+Masc+Pl	#;
LEXICON	FPL	+Fem+Pl		#;

LEXICON MSGPL	MSG; MPL;
LEXICON FSGPL	FSG; FPL;

LEXICON MFSGPL	MSGPL; FSGPL;

!# plural = singular
LEXICON MSP	+Masc+SP	#;
LEXICON FSP	+Fem+SP		#;
LEXICON MFSP	MSP; FSP;

!# masc = fem
LEXICON XSG	+MF+Sg		#;
LEXICON XPL	+MF+Pl		#;
LEXICON XSGPL	XSG; XPL;

!# masc = fem; plural = singular
LEXICON XSP	+MF+SP		#;

!# =============================================================================
!# MAIN LEXICON
!# =============================================================================

LEXICON ROOT 

