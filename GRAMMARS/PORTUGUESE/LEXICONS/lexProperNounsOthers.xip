vocabulary:


terra:       noun[maj:+,toutmaj:~] += [planeta=+].
sol:         noun[maj:+,toutmaj:~] += [planeta=+].

Mercúrio                           = noun[planeta:+].
Vénus                              = noun[planeta:+].
Marte                              = noun[planeta:+].
Júpiter                            = noun[planeta:+].
Saturno                            = noun[planeta:+].
Urano                              = noun[planeta:+].
Neptuno                            = noun[planeta:+].
Plutão                             = noun[planeta:+].
Ceres                              = noun[planeta:+].
Éris                               = noun[planeta:+].
Quaoar                             = noun[planeta:+].
Sedna                              = noun[planeta:+].
Orco                               = noun[planeta:+].
Varuna                             = noun[planeta:+].
