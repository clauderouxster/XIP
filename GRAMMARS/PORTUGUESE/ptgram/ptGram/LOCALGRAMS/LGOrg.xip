1> noun[SEM-Hcollective=+,SEM-Hinstitution=+] @= | ?[lemma:editar,pastpart];?[lemma:distribuir,pastpart];?[lemma:comercializar,pastpart], prep[lemma:por], art[fem] | ?+[maj,SEM-Hcollective=+,SEM-Hinstitution=+,start_collective=+], (prep, (art), ?+[maj,end_collective=+])*.

//DCO, 2010/09/24
// Regras corrigidas
// Atentados do IRA, atentados reinvindicados pela Al-Qaeda
1> noun[SEM-Hcollective=+,SEM-Hinstitution=+] @= | ?[lemma:atentado], ?[lemma:];?[lemma:reinvindicar, pastpart], prep[lemma:por], (art)| ?+[maj,SEM-Hcollective=+,SEM-Hinstitution=+,start_collective=+], (prep, (art), ?+[maj,end_collective=+])*.
1> noun[SEM-Hcollective=+,SEM-Hinstitution=+] @= | ?[lemma:atentado], prep[lemma:de], (art) | ?+[maj,SEM-Hcollective=+,SEM-Hinstitution=+,start_collective=+], (prep, (art), ?+[maj,end_collective=+])*.

//DCO, 2010/09/24
// "centro" não precisa de ser maj para que tudo seja uma entidade mencionada
// Centro Português de Cardiologia
1> noun[SEM-Hcollective=+,SEM-Hinstitution=+] @= ?[lemma:centro], ?+[maj], (prep, (art), ?+[maj])*.

//DCO, 2010/09/24
// Regra corrigida
// "tribunal" e "supremo" não precisam de ser maj para que tudo seja uma entidade mencionada
// Tribunal da Boa Hora
// Tribunal Criminal de 1ª instância
// Tribunal Constitucional de Portugal
1> noun[SEM-Hcollective=+,SEM-Hadministration=+] @= ?[lemma:tribunal];?[lemma:"tribunal criminal"];?[lemma:"tribunal constitucional"], (prep[lemma:de]), (art), (num), ?[lemma:instância];?+[maj], (prep, (art), ?+[maj])*.

//DCO, 2010/09/24
// e.g Supremo Tribunal de Justiça (de Portugal)
1> noun[SEM-Hcollective=+,SEM-Hadministration=+] @= ?[lemma:"Supremo Tribunal de Justiça",SEM-Hcollective=+,SEM-Hadministration=+], (prep[lemma:de], (art), ?[country]).

//DCO, 2010/09/24
// Regra corrigida - são entidades HUMANO COLECTIVO INSTITUICAO, e não HUMANO COLECTIVO ADMINISTRACAO
// Example: Polícia de Lisboa
1> noun[SEM-Hcollective=+,SEM-Hinstitution=+] @= ?[lemma:polícia], prep[lemma:de], (art), ?+[maj], (prep, (art), ?+[maj])*.

//DCO, 2010/09/24
// Example: A Polícia (Estadual)
1> ? = ?[lemma:polícia,maj,SEM-Hcollective=+,SEM-Hinstitution=+];?[lemma:"polícia estadual",maj,SEM-Hcollective=+,SEM-Hinstitution=+].

//DCO, 2010/09/24
// Regra corrigida
// Movimento de Resistência Islâmica, Movimento Popular de Libertação de Angola
1> noun[SEM-Hcollective=+,SEM-Hinstitution=+] @= ?[lemma:movimento,maj];?[lemma:"movimento popular",maj], (prep[lemma:de]), (art), (?+[maj]), ((?[lemma:e]), prep*, (art), ?+[maj])*.

//DCO, 2010/09/24
// "banco" não precisa de ser maj para que tudo seja entidade mencionada
// Example: Banco Popular, Banco Espírito Santo, Banco Bilbau e Viscaia
//DCO, 2011/05/10
//Adicionado adjectivo, e.g. "banco público"
1> noun[SEM-Hcollective=+,SEM-Hinstitution=+] @= ?[lemma:banco], (adj), (prep[lemma:de]), (art), (?+[maj]), ((?[lemma:e]), prep*, (art), ?+[maj])*.

//DCO, 2011/05/11
//Banco Central da China
1> noun[SEM-Hcollective=+,SEM-Hinstitution=+] @= ?[lemma:"banco central"], (adj[gentcountry]), (prep[lemma:de]), (art), (?+[maj]), ((?[lemma:e]), prep*, (art), ?+[maj])*.


//DCO, 2010/09/24
// Example: O Banco.
1> ? = ?[lemma:banco,maj,SEM-Hcollective=+,SEM-Hinstitution=+].

//DCO, 2010/09/24
// Regra corrigida
// Example: A PT, empresa de comunicações; A PT, a maior empresa de comunicações de Portugal; etc.

//DCO, 2010/09/24
// Regra corrigida
// Example: um empregado/funcionário/técnico/trabalhador da...
1> noun[SEM-Hcollective=+,SEM-Hinstitution=+] @= | ?[lemma:empregado];?[lemma:funcionário];?[lemma:trabalhador];?[lemma:técnico], prep[lemma:de], (art) | ?+[maj,SEM-Hpeople:~,SEM-Hcollective=+,SEM-Hinstitution=+,start_collective=+], ((?[lemma:e]), prep*, (art), ?+[maj,end_collective=+,location:~,admin_area:~])*  .

//DCO, 2010/09/24
// Regra corrigida
// Example: licenciado/graduado/doutorado pela....
1> noun[SEM-Hcollective=+,SEM-Hinstitution=+] @= | ?[lemma:licenciado];?[lemma:doutorado];?[lemma:graduado], prep[lemma:por], (art) | ?+[maj,SEM-Hcollective=+,SEM-Hinstitution=+,start_collective=+], ((?[lemma:e]), prep*, (art), ?+[maj,end_collective=+])*  .

// Example: University of Harvard, Harvard University
1> noun[SEM-Hcollective=+,SEM-Hinstitution=+,SEM-Huniversity=+] @= ?[lemma:university, maj], ?[lemma:of], ?+[maj], (prep*, (art), ?+[maj])*  .
1> noun[SEM-Hcollective=+,SEM-Hinstitution=+,SEM-Huniversity=+] @=  ?+[maj], (prep*, (art), ?+[maj])* , ?[lemma:university] .

// Escuteiros, Corpo Nacional de Escutas
1> noun[SEM-Hcollective=+,SEM-Hinstitution=+] @= ?[lemma:corpo], ?[lemma:nacional], prep[lemma:de], ?[lemma:escuta, maj].

