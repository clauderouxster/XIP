Features:


[features: [
//========================================================
// MAIN CATGEORIES
//========================================================
  cat:{0},

  adj:{+},        // adjective
  adv:{+},        // adverb
  art:{+},        // article
  det:{+},        // determiner
  conj:{+},       // conjunction
  foreign:{+},    // foreign word, when used with gentilic means that it is not a portuguese "region"
  interj:{+},     // interjection
  noun:{+},       // common or proper noun
  num:{+},        // numeral
  prep:{+},       // preposition
  pron:{+},       // pronoun
  punct:{+},      // punctuation
  symbol:{+},     // special symbol
  pastpart:{+},   // past participle
  verb:{+},       // verb
  pronrel:{+},    // relativ pronoun


//========================================================
//CHUNKS
//========================================================
  np:{+},          // noun phrase
  pp:{+},          // prepositional phrase
  ap:{+},          // adjective phrase
  advp:{+},        // adverbial phrase
  sc:{+},          // subclause

  vf:{+},          // verbal form
  vinf:{+},        // infinitive verb phrase (não flexionada)
  vger:{+},        // Gerund verb phrase
  vcpart:{+},      // Pastparticiple verbal chain
  vpp:{+},         // Pastparticiple (masc:+,sg:+) verb phrase
  vaux: [          // Auxiliary verb phrase
    vsup:{+},        // Auxiliary verb phrase (auxiliar of predicative nouns)
    vcop:{+},        // Auxiliary verb phrase (auxiliar of adjectives and past participles)
    vauxv: [         // Auxiliary verb phrase (auxiliar of verbs)
      vmod:{+},        // Auxiliary verb phrase (conveys modal values)
      vtemp:{+},       // Auxiliary verb phrase (conveys tense values)
      vasp:{+}         // Auxiliary verb phrase (conveys aspectual values)
    ]
  ],

  pp_not_np:{+},    // mark the head of a superficial NP wich in fact have the value of PP
                    // e.g.: Entre a saudade e a memória (memória bears this feature)

  notjoinadvp:{+},  // marks adverbs, like "assim", that cannot be joined to other adverbs in a single ADVP
                    // e.g.: assim daqui em diante : [[assim]adv]ADVP [[daqui em diante]adv]ADVP, and not
                    //                               [[assim]adv [daqui em diante]adv]ADVP

  notjoinnextadvp:{+},  // marks adverbs that cannot be joined with the next adverb in a single ADVP
                        // this feature is assigned to adverbs depending on a given sequence of adverbs
                        // e.g.: [aqui] [há mais de um milénio] : aqui[notjoinnextadvp]


//========================================================
// morphological features
//========================================================
  number: [ 
    pl:{+},   // plural
    sg:{+}    // singular
  ],

  person: [
    1p:{+},   // first  person
    2p:{+},   // second person
    3p:{+}    // third  person
  ],

  gender: [ 
    fem:{+},  // feminine
    masc:{+}  // masculine
  ],

  proper:{+},         // proper name, subcategory of "Noun" (may be a person, an organization, a location, ...)

  card:{+},           // cardinal number        (ex. vinte, trezentos)
  ord:{+},            // ordinal number         (ex. primeiro, vigésimo)
  mult:{+},           // multiplicative number  (ex. dobro, triplo, quádruplo)
  rom:{+},            // roman number           (ex. XXV, MCMXCIX)
  frac:{+},           // smaller then unity     (ex. meio, sexto)
  dig:{+},            // a number formed by digits (ex. 4, 1995)
  deg:{+},            // degree symbol

  maj:{+},            // The first letter of the Word is a majuscule
  toutmaj:{+},        // All the lettera of the Word are majuscules
  sub:{+},            // added on words or chunks

  reducedmorph:{+},   // A reduced form of a adv-mente

  cli:{+},            // The token is, in the original text, attached to the verb with a hyphen
  split:{+},          // to be used later on (eg. jogo Benfica-Sporting , comboio Lisboa-Porto)

  aug:{+},            // augmentative
  dim:{+},            // diminutive

  nom:{+},            // Pronoun nominative
  acc:{+},            // Pronoun accusative
  dat:{+},            // Pronoun dative
  obl:{+},            // Pronoun oblique
  Incl:{+},           // ENCLÍTICO ????????????

  ref:{+},            // ???????????????????????????????????
  refl:{+},           // na Xerox é uma das etiquetas para o "se"    ?????????????????????

  pers:{+},           // Pronoun personal  (eg., eu, me , mim)
  poss:{+},           // Pronoun possessive (eg., meu)
  rel:{+},            // Pronoun relative
  dem:{+},            // Pronoun demonstrative

  guess:{+},          // guessed by morphological analyzer (word not in the dictionary)


//========================================================
// Sentence features
//========================================================
  SentenceType: [ 
    sentDecl:{+},     // declarative
    sentInterr:{+},   // interrogative
    sentImper:{+},    // imperative
    sentExcl:{+}      // exclamative, includes interjections
  ],

//========================================================
// phrase features
//========================================================
  phstruct: [ 
    relat:{+},           // relative
    completiv:{+},       // completive
    adverbial:{+},       // adverbial
    exclamative:{+},     // exclamative
    interrog:{+}         // interrogative, also used to mark interrogative pronouns (eg, O que é que, ...)
  ],

//========================================================
// Punctuation
//========================================================
  comma:{+},       // comma
  dots:{+},        // dot
  quest:{+},       // question mark
  dash:{+},        // dash
  quote:{+},       // quote
  slash:{+},       // slash
  etc:{+},         // et cetrera

//========================================================
// SIMBOLS
//========================================================
  accent:{+},      // accent
  paren:{+},       // parentesis
  bracket:{+},     // bracket
  right:{+},       // right parentesis/bracket
  left:{+},        // left  parentesis/bracket
  inquote:{+},     // token is inside quotations

//========================================================
// Special Tokens
//========================================================
  abbr:{+},        // acronim, abbreviate
  letter:{+},      //
  item:{+},        //

  emph:{+},        // emphatic expression, produces dependency EMPH and
                    // is to be excluded from the computation of any other dependency (eg, "que RAIO disse ele?")
  destroy:{+},     // temporarily used to build verbal chunks

//=========================================================
// Tags for HAREM TEXTS
//=========================================================
  baliza:{+},      //
  fim_texto:{+},   //
  sent:{+},        //

//========================================================
// WH TOKENS
//========================================================
  qu:{+},          // added on WH tokens e.g. "que", "quando", "onde" etc. which are highly ambiguos

  what:{+},        // When "o que" or "o que é que" introduces a question
  whatIs:{+},      // When "o que" or "o que é que" introduces a question about an object
                    // (eg, o que é um samovar)
  whatAbbr:{+},    // When "o que" or "o que é que" introduces a question about an abbreviation
                    // (eg, o que é a UNICEF)
  whatEvent:{+},   // When "o que" or "o que é que" introduces a question about an event
                    // (eg, o que foi erguido em França em 13 de Agosto de 1961)
  whatRel:{+},     // When "o que" or "o que é que" introduces a question about a relation
                    // (eg, o que é D. Carlos a Dona Amélia)

//========================================================
// Verbal features
//========================================================
  dicendi:{+},    // added on dicendi verbs
  vdic:{+},       // Used to mark verbs that appear in a verba dicendi construction, that is, 
                  // verbs that were captured by the dependency rules that reanalyse and correct 
                  // the reversed subject of a dicendi construction (e.g. "Sim, disse ele").
                  
  fin:{+},        // Finit

  transf-passiva: [
    pass-estar:{+},     // used to identify a passive construction with auxiliary 'estar' (to be) and other Vcop (but not 'ser'):
                        //       A janela está/ficou partida = Algo partiu a janela.
    pass-ser:{+},       // used to identify a passive construction with auxiliary 'ser' (to be):
                        //       O livro foi lido pelo aluno = O aluno leu o livro.
    pass-pron:{+},      // used to identify a pronominal passive construction:
                        //       A janela partiu-se = Algo partiu a janela.
    pass-neutra:{+}     // used to identify a neuter passive construction:
                        //       Os canos entupiram = O lixo entupiu os canos.
  ],

  vbmodo: [
    vfin: [
      ind:{+},      // Indicativo
      subj:{+},     // Conjuntivo (subjuntivo)
      impv:{+},     // Imperativo
      cond:{+}      // Condicional
    ],
    vinfin: [
      inf:{+},      // Infinitivo simples ou não flexionado ou impessoal
      infflex:{+}   // Infinitivo flexionado ou pessoal
    ],
    gerund:{+},      // Gerúndio
    particip:{+}     // Particípio (distinct from partpart which is a category)
  ],

  pres:{+},       // Presente
  impf:{+},       // Pretérito imperfeito
  fut:{+},        // Futuro
  perf:{+},       // Pretérito perfeito
  pluperf:{+},    // Pretérito mais que perfeito

  cop:{+},        // copulative verb (lexical feature assigned to lemmas)
                  //   (see "vcop" that is assigned to chunk nodes)

  verbalfeat: [   // (ONLY) auxiliary verbs

    modal: [          // Modal auxiliary verb...
      deInfDeo:{+},     // ...followed by an infinitive verb, with preposition "de",  introducing the deont/epist feature
      deInfDeoImpf:{+}, // ...followed by an infinitive verb, with preposition "de",  introducing the deont/epist feature and the imperfective feature
      noInfDeo:{+},     // ...followed by an infinitive verb, with no preposition,    introducing the deont/epist feature
      queInfDeoImps:{+},// ...followed by an infinitive verb, with preposition "que", introducing the deont/epist feature and the impessoal feature

      deInfObr:{+},     // ...followed by an infinitive verb, with preposition "de",  introducing the obligation feature
      queInfObr:{+},    // ...followed by an infinitive verb, with preposition "que", introducing the obligation feature

      aInfIrreal:{+},   // ...followed by an infinitive verb, with preposition "a",   introducing the irreal feature
      deInfIrreal:{+},  // ...followed by an infinitive verb, with preposition "de",  introducing the irreal feature
      emInfIrreal:{+},  // ...followed by an infinitive verb, with preposition "em",  introducing the irreal feature
      noInfIrreal:{+},  // ...followed by an infinitive verb, with no preposition,    introducing the irreal feature

      aInfReal:{+},     // ...followed by an infinitive verb, with preposition "a",   introducing the real feature

      deInfImp:{+}      // ...followed by an infinitive verb, with preposition "de",  introducing the imperative feature
    ],

    temp: [         // Temp auxiliary verb...
      noPpPas:{+},    // ...followed by a  pastpart[masc:+,sg:+] verb, with no preposition, introducing the pas feature

      noInfFut:{+},   // ...followed by an infinitive verb, with no preposition,  introducing the future feature
      aInfFut:{+}     // ...followed by an infinitive verb, with preposition "a", introducing the future feature
    ],

    aspe: [         // aspectual auxiliary verb...
      aInfTer:{+},    // ...followed by an infinitive verb, with preposition "a",   introducing the terminat feature
      deInfTer:{+},   // ...followed by an infinitive verb, with preposition "de",  introducing the terminat feature
      noGerTer:{+},   // ...followed by a  gerund     verb, with no preposition,    introducing the terminat feature
      porInfTer:{+},  // ...followed by an infinitive verb, with preposition "por", introducing the terminat feature

      aInfDur:{+},    // ...followed by an infinitive verb, with preposition "a",   introducing the durativo feature
      noGerDur:{+},   // ...followed by an gerund     verb, with no preposition,    introducing the durativo feature
      noInfDur:{+},   // ...followed by an infinitive verb, with no preposition,    introducing the durativo feature

      aInfInc:{+},    // ...followed by an infinitive verb, with preposition "a",   introducing the incoat feature
      deInfInc:{+},   // ...followed by an infinitive verb, with preposition "de",  introducing the incoat feature
      emInfInc:{+},   // ...followed by an infinitive verb, with preposition "em",  introducing the incoat feature
      noGerInc:{+},   // ...followed by an gerund     verb, with no preposition,    introducing the incoat feature
      porInfInc:{+},  // ...followed by an infinitive verb, with preposition "por", introducing the incoat feature

      deInfFre:{+},   // ...followed by an infinitive verb, with preposition "de",  introducing the frequentat feature
      noInfFre:{+},   // ...followed by a  infinitive verb, with no preposition,    introducing the frequentat feature

      aInfIte:{+},    // ...followed by an infinitive verb, with preposition "a",   introducing the incoat/iterat feature

      paraInfImp:{+}, // ...followed by an infinitive verb, with preposition "para",introducing the imperfective feature
      paraInfInc:{+}, // ...followed by an infinitive verb, with preposition "para",introducing the incoat feature
      porInfImpP:{+}  // ...followed by an infinitive verb, with preposition "por", introducing the imperfective feature and the Passive feature
    ]
  ],


  modalfeat: [
    deont:{+},      // emphasises the obligation/real-occurrence of the action
    obrig:{+},      // obligation
    fact:{+},       // factive
    pass:{+},       // past
    futu:{+},       // future
    terminat:{+},   // emphasises the end of the action
    durativo:{+},   // emphasises the duration of the action
    permansivo:{+}, // emphasises the duration of the action and it is permanent
    incoat:{+},     // emphasises the start of the action
    frequentat:{+}, // emphasises the frequency of the action
    imperative:{+}, // imperative mode
    iterat:{+},     // emphasises the repetition of the action
    passive:{+},    // emphasises the passive form
    imperfect:{+},  // emphasises the object is the sintactic subject
    impessoal:{+},  // emphasises that the verb does not have a subject
    irreal:{+},     // emphasises that the verb is introducing a process that did not occur
    real:{+}        // emphasises that the verb is introducing a process that did effectively occur
  ],

  //noprep:{+},           // followed no preposition
  //wprepa:{+},           // followed by prep "a"
  //wprepde:{+},          // followed by prep "de"
  //wpreppor:{+},         // followed by prep "por"
  //wprepque:{+},         // followed by prep "que"
  //followedinf:{+},      // followed by an infinitive verb
  //followedger:{+},      // followed by an gerund verb
  //followedpp:{+},       // followed by an pastpart (VPP) masc and

  s_inf:{+},            // verbs subcategorizing infinitives with subject control CH
  s_np_inf:{+},         // verbs subcategorizing infinitive with object control CH
  s_np_ger:{+},         // verbs subcategorizing gerundive with object control
  s_pp_qufconj:{+},     // verbs subcategorizing with subject control (see lexVbControl.txt)
  s_pp_inf:{+},         // verbs subcategorizing with subject control (see lexVbControl.txt)
  s_qufconj:{+},        // verbs subcategorizing with subject control (see lexVbControl.txt)
  s_qufind :{+},        // verbs subcategorizing with subject control (see lexVbControl.txt)


  vrefl:{+},            // verb is intrinsically reflexive (e.g. suicidou-se)
                        // The verb is obligatory constructed with a reflexive pronoun (me, te, se, nhos, vos)

  markviper:{+},        // Used to remove the viper classes from auxiliary verbs and to 
                        // restrict the pairings done with the disambiguation rules (see dependencyViper.xip and dependencyViperRules.xip)

  viper: [              // verb classes from ViPEr (Baptista, 2012)
      01I:{+},             // QueF0 V (importar)
      01T:{+},             // QueF0 V QueF1 (causar)
      02:{+},              // QueF0 V Prep1 QueF1 (resultar de)
      03:{+},              // (mandar) 
      04:{+},              // (Nnr+QueF)0 V Nhum1 (most psychologic verbs type-1: irritar)
      05:{+},              // (Nnr+QueF)0 V a Nhum1 (most psycologic verbs type-2: agradar a)
      05R:{+},             // QueF0 V Prep1 N1 (abonar a favor de)
      06:{+},              // Nhum0 V QueF1 (mental perception: saber, pensar; volition: querer, ordenar)
      07:{+},              // Nhum0 V Prep1 (Vinf0)1 (aprender a)
      08:{+},              // N0 V Prep1 QueF1 (prestar-se a, depender de, resultar em, contar com)
      09:{+},              // Nhum V QueF1 a Nhum2 (dizer)
      10:{+},              // N0 V QueF1 Prep2 N2 (apostar)
      11:{+},              // N0 V N1 a QueF2 (obrigar)
      12:{+},              // N0 V N1 de (Vinf1)2 (impedir)
      13:{+},              // N0 V N1 de QueF2 (informar)
      14:{+},              // N0 V N1 Prep2 QueF2 (pagar)
      15:{+},              // N0 V (Ntmp)1 a (Vinf0)2 (passar)
      16:{+},              // N0 V (Ntmp)1 a (Vinf0)2 (passar)
      31H:{+},             // Nhum0 V (falecer)
      31I:{+},             // - V (chover, amanhecer)
      31CL:{+},            // Npc0 V (suar, feder)
      31PL:{+},            // Npl0 V (abundar, pulular)
      31R:{+},             // N0 V (morrer, florir)
      32A:{+},             // N0 V N1 [apparition] (construir)
      32C:{+},             // N0 V N1 *[apparition] (rasgar)
      32CL:{+},            // N0 V (Nbp de NHum)1 (pentear)
      32CV:{+},            // N0 V N1 = N0 converter N1 em V-n (caramelizar)
      32H:{+},             // Nhum0 V Nhum1 (assassinar)
      32NM:{+},            // N0 V Quant (pesar, medir, valer)
      32PL:{+},            // N0 V Npl1 (colecionar)
      32R:{+},             // N0 V Nc1 (estrelar (ovo))
      32TA:{+},            // N0 V N1 = N0 tornar V-a N1 (arredondar)
      33:{+},              // N0 V a N1 (telefonar)
      33MV:{+},            // N0 V Manner (portar-se, comportar-se)
      33NM:{+},            // N0 V Prep1 Quant (ascender)
      34:{+},              // N0 V de N1 (saber)
      35LD:{+},            // N0 V Loc-dyn Nloc1 (sair, entrar)
      35LS:{+},            // N0 V Loc-stat Nloc1 (viver, residir, morar)
      35R:{+},             // N0 V Prep1 N1 (confiar)
      35S:{+},             // N0 V com N1 [symmetry] (concordar, casar)
      36DT:{+},            // Nhum0 V N1 a Nhum2 (oferecer, roubar) 
      36R:{+},             // N0 V N1 Prep N2 (substituir, transformar)
      36S1:{+},            // N0 V N1 com N2 [object symmetry] (misturar)
      36S2:{+},            // N0 V N1 com N2 [subject-object symmetry] (discutir)
      36TA:{+},            // N0 V N1 Prep2 N2 = N0 tornar N1 V-a  Prep N2 (adequar)
      38L1:{+},            // N0 Vloc N1 (atravessar, ocupar)
      38L2:{+},            // N0 V N1 = N0 meter N1 em Vloc-n (enjaular)
      38L3:{+},            // Nloc0 V N1 (encerrar)
      38L4:{+},            // N0 V N1 = N0 meter V-n1 em Nloc-destiny2 (envenenar)
      38L5:{+},            // N0 V N1 = N0 tirar V-n1 de Nloc-source2 (desengordurar)
      38LD:{+},            // N0 V-dyn N1 Loc2 Nloc-destiny2 (pousar)
      38LR:{+},            // N0 V-stat N1 Loc2 Nloc2 (situar)
      38LS:{+},            // N0 V-dyn N1 Loc2 Nloc-source2 (pousar)
      38LT:{+},            // N0 V-dyn N1 Loc2 Nloc-source2 Loc3 Nloc-source2 (transferir)
      38PL:{+},            // N0 V N1 Prep Npl2 (dividir)
      38R:{+},             // N0 V N1 Loc2 Nloc-destiny2 (remeter)
      38TD:{+},            // N0 V N1 Loc2 Nloc-source2 (retirar)
      39:{+},              // N0 V N1 Pred (eleger, nomear)
      40:{+},              // N0 V N1 Prep1 N2 (bater com-em)
      41:{+},              // N0 V Prep1 N1 Prep2 QueF2 (apelar)
      42S:{+}              // N0 V com N1 Prep2 N2 [subject-object symmetry] (comungar)
    ],


  Vtd:{+},        // Direct-transitive verbs: require a NP-complement as its CDIR, 
                   //   may require other complements (PPs), which deemed irrelevant for rule-making; 
                   //   goal produce CDIR dependency
  Vti:{+},        // Indirect-transitive verbs: require a PP-complent, may require other complements (PPs), 
                   //   which are deemed irrelevant for rule-making; prevent CDIR dependency, to be used with [Vtd=:-]; 
                   //   cannot be used to define COMPL
  Vai:{+},        // Absolute intransitive verbs: do not allow any complements, except 31CL, 
                   //   i.e. those resulting from restructuring of complex subject with Npc
  Vqf:{+},        // Completive constructions: require at least a sub-clause, which may take many forms 
                   //   and can even be a complex NP with Npred; goal produce SENTENTIAL_COMPL and avoid MOD
  Vnp:{+},        // Non-completive constructions: do not allow a sub-clause, but may accept a complex NP with Npred; 
                   //   goal produce SENTENTIAL_MOD and avoid COMPL)
  Vdat:{+},       // Dative verbs: verbs presenting an essencial dative complement (CINDIR)
  Vloc:{+},       // Locative verbs: verbs presenting an essential locative complement 
                   //   can be a NP or a PP; can have more than one; goal, may help NER and EVENT 
                   //   extraction involving PLACE entities; may be sub-classified, naturally, if necessary,
                   //   but in this case i would rather go for the ViPEr class instead; 
                   //   does not include CL classes, with Nbodypart
  Vsim:{+},       // Symmetric verbs: verbs presenting the symmetry property: goal identify agent/co-agent
                   //   when a plural NP is found in the sym-arg position; produce COMPL for PP-sym position
  Vpl:{+},        // Plural verbs: verbs selecting a plural argument, or an NP with collective noun, 
                   //    ou an NP with generic interpretation; needs listing for the remaining, 
                   //    scattered by many other classes: 
                   //    validate singular-generic reference of this argument NP position
  Vhum:{+},       // Strictly human verbs, i.e. no non-human essential argument is accepted
  Varg0:{+},      // Impersonal verbs (Não vejo qq utilidade neste momento...)
  Varg1:{+},      // One-argument verbs (Não vejo qq utilidade neste momento...)
  Varg2:{+},      // Two-argument verbs (Não vejo qq utilidade neste momento...)
  Varg3:{+},      // Three-argument verbs (Não vejo qq utilidade neste momento...)


//========================================================
// Adverbial Features
//========================================================
  advcomp:{+},            // ????????????????????
  advmente:{+},           // ????????????????????

  adverbfeat: [
    advtime: [         // time adverb ...
      advtimefreq:{+},    // ... frequência (e.g., diariamente, semestralmente, ....)
      advtimeasp:{+},     // ... aspectuais (e.g., esporadicamente, pontualmente....)
      advtimedate:{+},    // ... temporais  (e.g., futuramente, proximamente, ....)
      advtimedur:{+}      // ... duração    (e.g., todo o santo dia,  ....)
    ],

    //proxadv:{+},         // This feature is defined below...

    advquant: [            // The adverb quantifies a NP with a numeral or a temporal PP ...
      advsupra:{+},         // ... the quantification is over the value (e.g., "mais de")
      advinfra:{+},         // ... the quantification is under the value (e.g., "menos de")
      advfuzzy:{+},         // ... the quantification is approximatly the value (e.g., "cerca de")
      advexact:{+},         // ... the quantification is exactly the value (e.g., "exactamente")
      advmagnus:{+},        // quantification in high degree (e.g., "a cântaros") equivalent to "muito"
      advantimagnus:{+},    // quantification in the lowest degree (e.g., "de todo") equivalent to "nada"
      advpart:{+}           // quantification partitive degree (e.g., "per capita")
    ],


      advmais:{+},        // indicates that an adverb can be preceded by mais but cannot take any other
                          // quantifying adverb; e.g. (mais/*menos/*tão concretamente); is a special feature for advfocus;
                          // in this case the "mais" is just an expresssive device.
                          //
      advnum:{+},         // determinant of a numeral
                          //
      advfocus:{+},       // modifier internal to proposition, modifies an NP or PP or major phrase/constituant;
                          // can not be cleft alone, can not be fronted to be begining of a negative sentence,
                          // can be cleft along with the constituent it modifies (e.g., "exclusivamente").
                          //
      advloc:{+},         // modifier internal to proposition, modifies an NP or PP or major phrase/constituant;
                          // can not be cleft alone, can not be fronted to be begining of a negative sentence,
                          // can be cleft along with the constituent it modifies and replaced by "(prep) onde?"
                          // (e.g., "a estibordo").
                          //
      advmanner:{+},      // modifier internal to proposition, modifies a verb or an adjective,
                          // can not be fronted to the begining of a negative sentence, and can usually be cleft,
                          // depending on the relation with the main verb (frozen, idiomatic adverbs cannot undergo
                          // clefting); it is the basic type of sentence-internal adverbs (default);
                          // Example: "correctamente"
                          //
      advmansubj:{+},     // a special case of advmod, but with a scope on the subject NP, which can be traced by
                          // an adjectival equivalente sentence N0 V Adv W = N0 ser Adv-a (e.g. "teimosamente")
                          //
      advpov:{+},         // Adv-mente specific subclass, mostly derived from relational, non-predicative adjectives,
                          // allowing the paraphrase "Adv-mente"="de um ponto de vista Adv-a" (e.g. "politicamente");
                          // usually allow both clefting and fronting to begining of negative sentence.
                          //
      advconj:{+},        // Conjunctive adverbs can not undergo clefting but are (usually) fronted; furthermore, they can
                          // not appear in the absolute begining of a discourse, while often apearing at the begining of
                          // sentences and frequently detached by coma (e.g. "efectivamente", "contudo"); note: they are
                          // often confounded with conjunctions proper, but, unlike them, they do not require a major
                          // (subordinant) clause; while some are fixed at the sentence's begining, many have high mobility
                          // and may appear almost everywhere in the sentence, between its major constituents; this is the
                          // major class of sentential adverbs.
                          // They can be semantically organized in many subclasses, mimicking the semantic classification
                          // subordinate conjunctions/clauses.
                          //
      advdisjstyle:{+},   // Style disjunctive adverbs operate on a meta-discourse operator and not on the sentence proper,
                          // This meta-operator is the argument of the adverb (e.g. "sinceramente"); this is a small class
                          // of sentential adverbs, e.g.: Sinceramente, acho que estás errado <- Eu digo sinceramente que (eu)
                          // acho que estás errado.

      advdisjattd: [      // Style disjunctive adverbs (advdisjattd) is a major class involving four subclasses:
         advhabit:{+},       // (1) advhabit (habit adverbs), e.g. habitualmente, geralmente
                             // not to be confounded with frequency adverbs); the main verb of
                             // the sentence on which they operate can only be inflected in the present or imperfective tense
                             // (with the 'habitual' aspect); are compatible with periodic time adverbs but incompatible with
                             // non-periodic time adverbs (e.g. Habitualmente, o Pedro come na cantina *hoje/ao meio-dia);
         adveval:{+},        // (2) adveval (evaluative adverbs), e.g. felizmente, curiosamente;
                             // express a (favorable/unfavorable) jugement from the speaker on the content of the sentence;
                             // sometimes they have an adjectival equivalent construction with subject-clause; only apear on
                             // assertive sentences, never on interrogative or on imperative sentences;
         advmodal:{+},       // (3) advmodal (not to be confound with manner adverbs), e.g. aparentemente, certamente, eventualmente;
                             // attribute a given modality to the sentence;
                             // sometimes they have an adjectival equivalent construction with subject-clause; only apear on
                             // assertive sentences, never on interrogative or on imperative sentences;
                             // may admit clefting along with a major sentence constituant (double scope);
         advattsubj:{+}      // (4) advattsubj (subject-oriented attitude adverbs), e.g. inteligentemente (not to be confounded
                             // with advmansubj); show all properties typical of AdvP (unacceptable clefting but acceptable fronting
                             // to the begining of negative sentence), plus
                             // sometimes they have an adjectival equivalent construction with subject-clause; only apear on
                             // assertive sentences, never on interrogative or on imperative sentences;
      ]
  ],

// Notes on ADVERBS:
//
// Much of the above is taken from Molinier and Levrier (2000).
//
// advtime, advquant, advfocus, advmod, advmansubj, advpov are all sentence-internal adverbial modifiers (AdvM);
// advconj, advdisjstyle, advdisjatt are all sentence-external modifiers, i.e. have scope over the (an) entire sentence (AdvP);
//
// Perhaps organize things better in terms of classes and subclasses? Discuss.
//
// advnum is much more a syntactic feature then a semantic one; there significant overlap with advquant;
// basically, many advquant may also be a advnum, but not all advquant are advnum as well, e.g. "assaz", "bastante";
