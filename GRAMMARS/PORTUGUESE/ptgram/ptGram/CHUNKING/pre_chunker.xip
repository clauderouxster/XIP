Sequence:


////////////////////////////////////////////////////////////////////////////////
// Single word between quotations
////////////////////////////////////////////////////////////////////////////////

1> noun[inquote=+]      = punct[bracket,left];punct[quote],      noun[!features:!], punct[bracket,right];punct[quote].
1> verb[inquote=+]      = punct[bracket,left];punct[quote],      verb[!features:!], punct[bracket,right];punct[quote].
1> adj[inquote=+]       = punct[bracket,left];punct[quote],       adj[!features:!], punct[bracket,right];punct[quote].
1> art[inquote=+]       = punct[bracket,left];punct[quote],       art[!features:!], punct[bracket,right];punct[quote].
1> conj[inquote=+]      = punct[bracket,left];punct[quote],      conj[!features:!], punct[bracket,right];punct[quote].
1> interj[inquote=+]    = punct[bracket,left];punct[quote],    interj[!features:!], punct[bracket,right];punct[quote].
1> num[inquote=+]       = punct[bracket,left];punct[quote],       num[!features:!], punct[bracket,right];punct[quote].
1> prep[inquote=+]      = punct[bracket,left];punct[quote],      prep[!features:!], punct[bracket,right];punct[quote].
1> pron[inquote=+]      = punct[bracket,left];punct[quote],      pron[!features:!], punct[bracket,right];punct[quote].
1> pastpart[inquote=+]  = punct[bracket,left];punct[quote],  pastpart[!features:!], punct[bracket,right];punct[quote].
1> symbol[inquote=+]    = punct[bracket,left];punct[quote],    symbol[!features:!], punct[bracket,right];punct[quote].

//NJM, 2011/01/27
1> noun[inquote=+]      = ?[lemma:"'"],      noun[!features:!], ?[lemma:"'"].
1> verb[inquote=+]      = ?[lemma:"'"],      verb[!features:!], ?[lemma:"'"].
1> adj[inquote=+]       = ?[lemma:"'"],       adj[!features:!], ?[lemma:"'"].
1> art[inquote=+]       = ?[lemma:"'"],       art[!features:!], ?[lemma:"'"].
1> conj[inquote=+]      = ?[lemma:"'"],      conj[!features:!], ?[lemma:"'"].
1> interj[inquote=+]    = ?[lemma:"'"],    interj[!features:!], ?[lemma:"'"].
1> num[inquote=+]       = ?[lemma:"'"],       num[!features:!], ?[lemma:"'"].
1> prep[inquote=+]      = ?[lemma:"'"],      prep[!features:!], ?[lemma:"'"].
1> pron[inquote=+]      = ?[lemma:"'"],      pron[!features:!], ?[lemma:"'"].
1> pastpart[inquote=+]  = ?[lemma:"'"],  pastpart[!features:!], ?[lemma:"'"].
1> symbol[inquote=+]    = ?[lemma:"'"],    symbol[!features:!], ?[lemma:"'"].
