Vocabulary:



//==============================================================================
// AUXILIARY MODAL VERBS (VMOD)
//==============================================================================
chegar:          verb += [deInfImp=+].                                   // 2011/02/20 (São Carlos) !! deInfImp !!
conseguir:       verb += [noInfIrreal=+].
cuidar:          verb += [deInfIrreal=+].                                // 2011/02/20 (São Carlos) !! deInfIrreal !!
dever:           verb += [noInfDeo=+, deInfDeo=+].
escapar:         verb += [aInfIrreal=+,deInfIrreal=+].                   // 2011/02/20 (São Carlos) !! aInfIrreal !!
ficar:           verb += [deInfObr=+].
haver:           verb += [deInfObr=+].
hesitar:         verb += [emInfIrreal=+,noInfIrreal=+].
limitar:         verb += [aInfReal=+].                                   // 2013/07/02
ousar:           verb += [noInfIrreal=+].                                // 2011/02/20 (São Carlos)
parecer:         verb += [noInfIrreal=+].                                // JB 2012/09/15
poder:           verb += [noInfDeo=+].
procurar:        verb += [noInfIrreal=+].
ser:             verb += [deInfDeoImpf=+].
suceder:         verb += [emInfIrreal=+].                                // 2011/02/20 (São Carlos)
tardar:          verb += [aInfIrreal=+,emInfIrreal=+].                   // 2012/10/22
tencionar:       verb += [noInfIrreal=+].                                // 2011/02/20 (São Carlos)
tender:          verb += [aInfReal=+].                                   // 2011/02/20 (São Carlos) !! aInfReal !!
tentar:          verb += [noInfIrreal=+].
ter:             verb += [deInfObr=+, queInfObr=+].
tratar:          verb += [deInfIrreal=+].                                // 2011/02/20 (São Carlos)

chegar:      pastpart += [deInfImp=+].                                   // 2011/02/20 (São Carlos)
conseguir:   pastpart += [noInfIrreal=+].
cuidar:      pastpart += [deInfIrreal=+].                                // 2011/02/20 (São Carlos)
dever:       pastpart += [noInfDeo=+, deInfDeo=+].
escapar:     pastpart += [aInfIrreal=+,deInfIrreal=+].                   // 2011/02/20 (São Carlos)
ficar:       pastpart += [deInfObr=+].
haver:       pastpart += [deInfObr=+].
hesitar:     pastpart += [emInfIrreal=+,noInfIrreal=+].
limitar:     pastpart += [aInfReal=+].                                   // 2013/07/02
ousar:       pastpart += [noInfIrreal=+].                                // 2011/02/20 (São Carlos)
poder:       pastpart += [noInfDeo=+].
procurar:    pastpart += [noInfIrreal=+].
ser:         pastpart += [deInfDeoImpf=+].
suceder:     pastpart += [emInfIrreal=+].                                // 2011/02/20 (São Carlos)
tardar:      pastpart += [aInfIrreal=+,emInfIrreal=+].                   // 2012/10/22
tencionar:   pastpart += [noInfIrreal=+].                                // 2011/02/20 (São Carlos)
tender:      pastpart += [aInfReal=+].                                   // 2011/02/20 (São Carlos)
tentar:      pastpart += [noInfIrreal=+].
ter:         pastpart += [deInfObr=+, queInfObr=+].
tratar:      pastpart += [deInfIrreal=+].                                // 2011/02/20 (São Carlos)


//Intrinsecamente pronominal (A NãO TRATR POR NÃO APARECER VEZES SUFICIENTES NOS CORPORA CONSULTADOS)
//
//apostar-se:    verb += [emInfIrreal=+].                                // 2011/02/20 (São Carlos)
//escapar-se:    verb += [aInfIrreal=+,deInfIrreal=+].                   // 2011/02/20 (São Carlos)



//==============================================================================
// AUXILIARY TENSE VERBS (VTEMP)
//==============================================================================
haver:           verb += [noPpPas=+,queInfDeoImps=+].
ir:              verb += [noInfFut=+,aInfFut=+].
ter:             verb += [noPpPas=+].

haver:       pastpart += [noPpPas=+].
ir:          pastpart += [noInfFut=+,aInfFut=+].
ter:         pastpart += [noPpPas=+].


//==============================================================================
// AUXILIARY ASPECTUAL VERBS (VASP)
//==============================================================================
acabar                += [noGerTer=+, aInfTer=+,deInfTer=+,porInfTer=+].
andar                 += [aInfDur=+,noGerDur=+].
cessar                += [deInfTer=+].
chegar                += [aInfTer=+].
começar               += [aInfInc=+,porInfInc=+,noGerInc=+].
continuar             += [aInfDur=+,noGerDur=+,porInfImpP=+].
correr                += [aInfInc=+].
costumar              += [noInfFre=+,deInfFre=+].
dar                   += [emInfInc=+].                                   // 2011/02/20 (São Carlos)  !! emInfInc !!
deitar                += [aInfInc=+].
deixar                += [deInfTer=+].
desatar               += [aInfDur=+,noGerDur=+].
estar                 += [aInfDur=+,noGerDur=+,paraInfImp=+,porInfImpP=+].
//estar a ponto       += [deInfInc=+].        coded as a chunk
//estar prestes       += [aInfInc=+].         coded as a chunk
ficar                 += [aInfDur=+,noGerDur=+,paraInfImp=+,porInfImpP=+].
ir                    += [noGerDur=+,aInfInc=+,paraInfInc=+].
largar                += [aInfInc=+,noGerInc=+].                         // 2011/02/20 (São Carlos)  !! noGerInc !!
parar                 += [deInfTer=+].
passar                += [aInfInc=+].
permanecer            += [aInfDur=+,noGerDur=+].                         // 2011/02/20 (São Carlos)
recomeçar             += [aInfIte=+].
soer                  += [noInfFre=+,deInfFre=+].                        // 2011/02/20 (São Carlos)
terminar              += [noGerTer=+,deInfTer=+,porInfTer=+,aInfTer=+].  // 2011/02/20 (São Carlos)
tornar                += [aInfIte=+].
vir                   += [noInfDur=+,noGerDur=+,aInfTer=+,deInfTer=+].
viver                 += [aInfDur=+,noGerDur=+].                         // 2011/02/20 (São Carlos)
voltar                += [aInfIte=+].

//intrinsecamente pronominal (A NãO TRATR POR NÃO APARECER VEZES SUFICIENTES NOS CORPORA CONSULTADOS)
//
//deitar-se           += [aInfInc=+].                             // 2011/02/20 (São Carlos)


//==============================================================================
//  COP VERBS (VCOP)
//==============================================================================
andar                 += [cop=+,durativo=+].
continuar             += [cop=+,permansivo=+].
estar                 += [cop=+,durativo=+].
ficar                 += [cop=+,incoat=+].
parecer               += [cop=+,irreal=+].
permanecer            += [cop=+,permansivo=+].
ser                   += [cop=+,real=+].
viver                 += [cop=+,durativo=+].


//==============================================================================
//  VERBOS OPERADORES CAUSATIVOS (não são trados como auxiliares)
//==============================================================================
