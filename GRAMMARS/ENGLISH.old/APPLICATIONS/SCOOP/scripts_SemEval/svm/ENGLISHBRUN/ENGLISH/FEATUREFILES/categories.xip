/*****************************************
        BASIC CATEGORIES
*****************************************\


 top = [cat=0]      /acts as root to chunking trees\
 

 noun=[noun=+,!time:!,!guille:!]
 verb=[verb=+,!indspeech:+!,!guille:!]  
 aux=[aux=+,!guille:!]
 adj=[adj=+,!compar:+!,!guille:!]
 nadj=[nadj=+, adj=+, noun=+,!guille:!]
 prep=[prep=+,!guille:!]
 adv=[adv=+,!guille:!]
 conj=[conj=+,!guille:!]
 num=[num=+,!guille:!] 
 dig=[dig=+,!guille:!]
 det=[det=+,!wh:+!,!guille:!]
 pron=[pron=+,!guille:!] 
 punct = [punct=+,!guille:!] 
 quant = [quant=+,!guille:!]
 sgml = [sgml=+,!guille:!]       /is this tag necessary?\
 part = [part=+,!guille:!]
 meas = [meas=+,!guille:!]
 interj = [interj=+,!guille:!]
 infto = [infto=+,!guille:!]
 
