Vocabulary:


// Special features for verb introducing a temporal relation
begin      : verb += [v_temp_rel:starts].
continue   : verb += [v_temp_rel:before_or_during].
end        : verb += [v_temp_rel:ends].
finish     : verb += [v_temp_rel:ends].
follow     : verb += [v_temp_rel:after].
precede    : verb += [v_temp_rel:before].
start      : verb += [v_temp_rel:starts].


cause      : verb += [v_cause:+].
provoke    : verb += [v_cause:+].


intend     : verb += [v_plan:+].
plan       : verb += [v_plan:+].
promise    : verb += [v_plan:+].
seek       : verb += [v_plan:+].


expect     : verb += [v_expect:+].
hope       : verb += [v_expect:+].


increase   : verb += [v_rise:+].
rise	   : verb += [v_rise:+].


decline    : verb += [v_fall:+].
descend    : verb += [v_fall:+].
diminish   : verb += [v_fall:+].
fall       : verb += [v_fall:+].


allow      : verb += [v_permit:+].
authorize  : verb += [v_permit:+].
permit     : verb += [v_permit:+].


announce   : verb += [v_announce:+].
propose    : verb += [v_announce:+].
promise    : verb += [v_announce:+].
report     : verb += [v_announce:+].
