DependencyRules:

if ( ^CONCEPT(#1) & IMPSENT(#2) )
   CONCEPT[test=+](#1).

if ( ^CONCEPT(#1) & IMPSUMMARY(#3) )
   CONCEPT[test=+](#1).


if ( ^CONCEPT(#1) & SUMMARY(#4) )
   CONCEPT[test=+](#1).

if ( ^CONCEPT(#1,#3) & IMPSENT(#2) )
   CONCEPT[test=+](#1,#3).

if ( ^CONCEPT(#1,#3) & IMPSUMMARY(#2) )
   CONCEPT[test=+](#1,#3).

if ( ^CONCEPT(#1,#3) & SUMMARY(#2) )
   CONCEPT[test=+](#1,#3).

if ( ^CONCEPT(#1,#3,#4) & IMPSENT(#2) )
    CONCEPT[test=+](#1,#3,#4).

if ( ^CONCEPT(#1,#3,#4) & IMPSUMMARY(#2) )
    CONCEPT[test=+](#1,#3,#4).

if ( ^CONCEPT(#1,#3,#4) & SUMMARY(#2) )
    CONCEPT[test=+](#1,#3,#4).

if ( ^CONCEPT[test:~](#1) )
   ~.

if ( ^CONCEPT[test:~](#1,#2) )
   ~.

if ( ^CONCEPT[test:~](#1,#2,#3) )
   ~.

