// *********Rules with keywords:************

///number entites names defined until now\

//54> entity#1, where ( #1[rule=94,geneprotrul:+] | #1[rulebis=94,geneprotrul:+] ).
//54> entity#1, where ( #1[rule=95,generul:+] | #1[rulebis=95,generul:+] ).
