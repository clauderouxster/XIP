// subclauses

// mit einleitender Konjunktion
| punct[comma:+], bg{(conj; adv), conj#1[subord:+]}, ?*[finite:~, punct:~], vfin{?*, #2[last]} |
	CONNECT(#2, #1).		//wenn punct in Klammern, werden Sätzte, die mit einer konj anfangen 2x etikettiert, wenn punct nicht in Klammern, werden Sätze nicht etikettiert, bei denen vor der Konjunktion kein Komma steht

| bg{conj#1[subord:+, start:+]}, ?*[finite:~, punct:~], vfin{?*,#2[last]} |
	CONNECT(#2, #1).

