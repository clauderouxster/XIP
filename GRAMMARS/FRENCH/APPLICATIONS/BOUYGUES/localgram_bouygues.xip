DiRègles :


10> noun[phone=+] -> | ?[phonename:+];noun[lemme:"Téléphone"];noun[lemme:"téléphone"] , (punct[left:~,right:~]) | 
                num+[rom:~], 
                punct*[lemme:"+"],
                punct*[form:fcpar],
                punct*[form:fslash],
                num*[rom:~],
                punct*[form:fopar,fin:~],
                punct*[form:fhyph,fin:~,debut:~],
                num*[rom:~]. 




10>noun[phone=+,mobile=+] -> |noun[lemme:"mobile"];noun[lemme:"Mobil"];noun[lemme:"cell"];noun[lemme:"Port"];noun[lemme:"GSM"];?[lemme:"M"];noun[lemme:"mob"]  , (punct) |                
                num+[mobile:~,rom:~], 
                punct*[lemme:"+"],
                 punct*[form:fcpar],
                 num*[rom:~],
                 punct*[form:fopar,fin:~],
                 punct*[form:fhyph,fin:~],
                 num*[rom:~]. 


10>noun[fax=+] -> |noun[faxname:+];?[lemme:"F"], (punct[form:f2pts]) | 
                 num+, 
                 punct*[lemme:"+"],
                 punct*[form:fcpar],
                 punct*[form:fslash],
                 num*[rom:~],
                 punct*[form:fopar,fin:~],
                 punct*[form:fhyph,fin:~,debut:~],
                 num*[rom:~]. 

10>noun[phone=+] -> num[internatcode,debut],num+, 
                punct*[lemme:"+"],
                 punct*[form:fcpar],
                 num*[rom:~],
                 punct*[form:fopar,fin:~],
                 punct*[form:fhyph,fin:~],
                 num*[rom:~]. 

10> noun[phone=+] -> punct[lemme:"+",debut],
                num+[rom:~], 
                punct*[form:fcpar],
                punct*[form:fslash],
                num*[rom:~],
                punct*[form:fopar,fin:~],
                punct*[form:fhyph,fin:~],
                num*[rom:~]. 


