<!ELEMENT articles (Article*)>


<!-- each file consists of several samples, which have a title taken from the Afp, A.P... and text, encoded as Body -->
<!ELEMENT Article (Title,Source,Date,Body)>
<!ATTLIST Article
          id ID #IMPLIED
          understood (yes|no) "yes">
          
<!ELEMENT Title (#PCDATA)>
<!ATTLIST Title 
           id CDATA #IMPLIED
           n CDATA #IMPLIED
          xmlns CDATA #FIXED>


<!-- The text encoded in Article contains text as well as one annotated name -->
<!ELEMENT Body (#PCDATA|annot)*>



<!-- the annotated name can be of two semantic types : org (organisation) and location are used -->
<!ELEMENT annot (location|org)*>
<!ATTLIST annot
          id ID #IMPLIED
         >



<!-- locations have readings literal or metonymic. Metonymies have a reading type. Notes are free text comments for clarification only -->

<!ELEMENT location (#PCDATA)>
<!ATTLIST location
	  id ID #IMPLIED
	  reading (literal|metonymic) #IMPLIED
          metotype (object-for-representation|object-for-name|place-for-people|place-for-event|place-for-product)    #IMPLIED
          notes CDATA #IMPLIED>


<!-- organisations have readings literal or metonymic. Metonymies have a reading type. Notes are free text comments for clarification only -->

<!ELEMENT org (#PCDATA)>
<!ATTLIST org
	  id ID #IMPLIED
	  reading (literal|metonymic) #IMPLIED
          metotype (object-for-representation|object-for-name|organisation-for-product|organisation-for-facility|organisation-for-members|organisation-for-index|organisation-for-event)    #IMPLIED
          notes CDATA #IMPLIED>



