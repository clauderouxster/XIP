//Paris c'est beau.  relation SUBJ(est, Paris)

 |SC{   NP[first :+]{?*,#1[last :+]}, ?*[PUNCT : +], NP{#2[lemme : ce]}, FV{?*, #3}  }| if(SUBJ(#3[lemme : être], #2))
SUBJ(#3_VERB, #1_SUBJ).


 |SC{   NP[first :+]{?*,#1[last :+]}, ?*[PUNCT : +], NP{#2[lemme : ce]}, FV{?*, #3}  }| 
  if( SUBJ(#3_VERB, #1_SUBJ) & ^OBJ(#3_VERB, #1_SUBJ)) ~ .


