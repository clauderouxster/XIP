!# islamic month names

	
< m o u h a r (r) [a|e] m >		IMONTH; !1
< s a f a r >				IMONTH; !2
< r a b i (a) MSEP >			IMONTH2; !3,4
< r a b i (a) %  I (I) >		IMONTH;
< (d) j o u m a d a MSEP >		IMONTH2;
< (d) j o u m a d a %  I (I) >		IMONTH;
< r a (d) j a b >			IMONTH;
< c h a [a|%'] b a n (e) >		IMONTH;
< r a m a d a n >			IMONTH;
< c h a w (w) a l >			IMONTH;
< d [h (o)|o] u MSEP >			IMONTH3;


LEXICON IMONTH2
< a l MSEP a w (w) a l >		IMONTH;
< ([a|e]) l MSEP o u l a >		IMONTH;
< [a|e] l MSEP t (h) a n i (a) >	IMONTH;
< a t (h) MSEP t (h) a n i (a) >	IMONTH;
< [a|e] t (h) a n i (a) >		IMONTH;
< o u t MSEP t a n i (a) >		IMONTH;

LEXICON IMONTH3 
<a l MSEP q [i|a] (%') d a >		IMONTH;
<a l MSEP h i (d) j [i|j] a >		IMONTH;
