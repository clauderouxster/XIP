Vocabulaires:

	redon = noun[masc=+,sg=+,p3=+].


/LIEU\
	CHLS = noun[lieu=+,org=+].
	chls = noun[lieu=+,org=+].
	SAU = noun[masc=+,sg=+,p3=+]. // service d'accueil des urgences
	IHL = noun[masc=+,fem=+,sg=+,p3=+,lieu=+,prof=+]. // interne des hospices de Lyon ?
	
/PERSON\
	CCA = noun[masc=+,fem=+,sg=+,p3=+,prof=+]. // chef clinique assistant
	ORL = noun[masc=+,fem=+,sg=+,p3=+,prof=+]. // oto-rhino-laryngologiste
	interne += noun[prof=+].

/ORG\
	SMU = noun[masc=+,sg=+,p3=+,org=+].

	
/EVENEMENT\
	course += ?[debutEvent=+].
	vélo : noun += [sport:=+].

// CH - Dec 2009
// pour distinguer les titres pouvant referer a du personnel soignant

[p,P]rof:		noun += [tit:+,tit_med:+] .
[p,P]rof.: 		noun += [tit_med:+] .
[p,P]r(.):		noun += [tit_med:+] .
[d,D]r(.):			noun += [tit_med:+] .
médecin:			noun += [tit_med:+] .
professeur:			noun += [tit_med:+] .
radiologiste:			noun += [tit_med:+] .
anesthésiste:			noun += [tit_med:+] .
interne:			noun += [tit_med:+] .
docteur:			noun += [tit_med:+] .






