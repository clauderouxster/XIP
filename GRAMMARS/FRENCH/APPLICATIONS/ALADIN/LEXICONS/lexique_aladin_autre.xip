Vocabulaires:

// unites de mesure rencontres dans textes medicaux et non reconnues par gram des entites

abdomino     	= prefix.

mmHg  		= noun[measure=+] .
(%d)(%d)%dmmHg	= noun[measure=+].
"Âµg"		= noun[measure=+] .
(%d)(%d)%dµg	= noun[measure=+] .


// adding features
PFC:		noun += [measure:+] .
(%d)(%d)%dPFC	= noun[measure=+] .

CGR:		noun += [measure:+] .
(%d)(%d)%dCGR	= noun[measure=+] .


// CR utilisé comme abbréviation de compte-rendu dans les CR
CR:		noun += [guessed=~,proper=~,masc=+,sg=+,abr=+] .
CRO:		noun += [guessed=~, proper=+, masc=+, sg=+, abr=+].

// erreur ortho
sénior:		noun += [guessed=~,sg=+,adj=+] .




