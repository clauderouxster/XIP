Translation:
//-- MAIN CATEGORIES ----------------------------------------------------------
Adj		= adj.			// adjective
Adv		= adv.			// adverb
Conj		= conj.			// conjunction
Cpd		= trunc.		// truncated word part
Det		= det.			// determiner
DetPron		= det[pron=+].		// determiner or pronoun
Itj		= itj.			// interjection
Noun		= noun[common=+].	// (common) noun
Num		= num.			// numeral
Poss		= poss.			// possessive 
Prep		= prep.			// preposition
Pron		= pron.			// pronoun
Prop		= noun[proper=+].	// proper noun
Ptcl		= ptcl.			// particle
Verb		= verb.			// verb

Punct		= punct.		// punctuation
Symbol		= symbol.		// special symbol (currency, character, ...)
Sgml		= symbol[sgml=+].	// SGML tag

Dig		= num[dig=+].		// digital number

// other lexical features

Act	= [act=+].		// active [verb]
Card	= [card=+].	// cardinal [number]
Comp	= [comp=+].	// coparative [adjective]
Coord	= [coord=+].	// coordinating [conjunction]
Cop	= [cop=+].		// copula
Def	= [def=+].		// definitive [noun, adjective, determiner, ...]
Gen 	= [gen=+].		// genitive case [noun]
Imp	= [imp=+].		// imperative [verb]
Indef	= [indef=+].	// indefinitive [noun, adjective, determiner, ...]
Inf 	= [inf=+].		// infinitive [verb]
Masc 	= [masc=+].	// masculine gender
Neut	= [neut=+].	// neuter gender
Nom	= [nom=+].		// nominative case
Obj	= [obj=+].		// objective case
Ord	= [ord=+].		// ordinal [number]
PartPast	= [ppast=+].	// past participle [adjectival use]
PartPres	= [ppres=+].	// present participle [verb]
Past	= [past=+].	// past tense
Pl	= [pl=+].		// plural
Pos	= [pos=+].		// positive (basic) degree [adjectives]
Pres 	= [pres=+].	// present tense [verb]
Prt1	= [prt1=+].	// part 1 (of double conjunction)
Prt2	= [prt1=+].	// part 2 (of double conjunction)
Recip	= [recip=+].	// reciprocal (pronoun)
Refl	= [refl=+].	// reflexive (pronoun)
Rel	= [rel=+].		// relative (pronoun)
SForm	= [sform=+].	// s-form
Sg	= [sg=+].		// singular
Subj	= [subj=+].	// subject case*
Subord	= [subord=+].	// subordinating (conjunction)
Sup	= [super=+].	// superlative degree
Supin	= [supin=+].	// supinum [verb]
Utr	= [utr=+].		// common or utrum (masculine or feminine) gender
Wh	= [wh=+].		// interrogative or relative [pronoun, determiner]

// punctuations
Apst		= [apos=+].		// apostrophe '
Colon		= [colon=+].		// colon :
Comma		= [comma=+].		// comma ,
Curr		= [curr=+].		// currency $ € £
Dash		= [dash=+].		// dash -
Hyphen		= [hyphen=+].		// hyphen -
Paren		= [paren=+].		// parenthesis ( ) [ ] { }
Quote		= [quote=+].		// quote " « »
Semicolon	= [semicol=+].		// semicolon ;
Sent 		= [sent =+].		// sentence final . ! ? 
Slash 		= [slash=+].		// slash, backslah / \  
Unit		= [unit=+].		// measure unit ° 

// numbers
Dec		= [dec=+].		// decimal number
Percent		= [percent=+].		// number with percent sign
Rom		= [roman=+]. 		// roman number

// other special symbols
Email	 	= [email=+].		// e-mail address
Final		= [final=+].		// final SGML symbol
Initial  	= [initial=+].		// initials (person names)
Tel 		= [phone=+].		// phone number
URL 		= [url=+].		// URL


// lexical source
guessed		= [guessed=+].
