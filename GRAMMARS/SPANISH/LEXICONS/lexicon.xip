Vocabulary:

// add here ...


ex += [prefix:+] .


más += [compar=+].
como += [modo=+].


cuyo += [rel=+].


tanto += [compar=+].
como += [compar=+].
que += [compar=+].

//don += [title=+].
//doña += [title=+].

//We add the feature "enum" ton conjonctions "y, e, o"
y +=[enum=+].
e +=[enum=+].
o +=[enum=+].



//we add the feature "coord" to the conjonctions "y e ni o"
y += [coord=+].
e += [coord=+].
ni += [coord=+].
o += [coord=+].
ó += [coord=+].

// rey must have the feature title:+
rey += [title=+].

//We add the feature Aux for auxiliar verbs.
haber +=[aux=+].

//Feature Cop for copulative verbs
estar +=[cop=+].
ser +=[cop=+].
parecer +=[cop=+].

//We add Num for "cientos"
ciento +=[num=+].

//We add the feature PrepCour to prepositions "a" and "de"
a += [prepcour=+].
de += [prepcour=+].

//We add the feature fopar for the opening brackets and fcpar for the final brackets
\( += [fopar=+].
\) += [fcpar=+].

//We add the feature dots to ":"
\: += [dots=+].
//We add the feature PP_a for those names, verbs or adjectives who need a preposotional complement starting by "a".
acceso +=[pp_a=+].

//We add the featur Pp_en for those verbs (or adjectives) who need a prepositional complement starting by "en".
basar += [pp_en=+].
permanecer += [pp_en=+].

//We add the feature "temp" for words like "día", "hora", "mes" etc.
día += [temp=+].
mes += [temp=+].
hora += [temp=+].
minuto += [temp=+].
segundo:noun += [temp=+].
semana += [temp=+].
año += [temp=+].
//poco += [temp=+].
//mucho += [temp=+].
lunes += [temp=+,day=+].
martes += [temp=+,day=+].
miércoles += [temp=+,day=+].
jueves += [temp=+,day=+].
viernes += [temp=+,day=+].
sábado += [temp=+,day=+].
domingo += [temp=+,day=+].
enero += [temp=+,month=+].
febrero += [temp=+,month=+].
marzo += [temp=+,month=+].
abril += [temp=+,month=+].
mayo += [temp=+,month=+].
junio += [temp=+,month=+].
julio += [temp=+,month=+].
agosto += [temp=+,month=+].
septiembre += [temp=+,month=+].
octubre += [temp=+,month=+].
noviembre += [temp=+,month=+].
diciembre += [temp=+,month=+].
vez += [temp=+].
ayer += [temp=+].
mañana += [temp=+].
pasadomañana += [temp=+].
cuando += [temp=+].
tiempo += [temp=+].

//We add the feature "exp_temp"
hacer +=[exp_temp=+].

//animated entities
gobierno +=[!anim=+].
servicio +=[!anim=+].
administrador +=[!anim=+].

//verbes de "parole"
decir +=[!parole=+].
comentar +=[!parole=+].
preguntar +=[!parole=+].
sentenciar +=[!parole=+].
indicar +=[!parole=+].
explicar +=[!parole=+].
declarar +=[!parole=+].
informar +=[!parole=+].
anunciar +=[!parole=+].
pedir +=[!parole=+].
opinar +=[!parole=+].
advertir +=[!parole=+].
definir +=[!parole=+].
señalar +=[!parole=+].
criticar +=[!parole=+].
añadir +=[!parole=+].

//we add verbal features such as Modal, Aspect (Aspectual).
//Modal:

//possibility
//puede estudiar
poder +=[modalpot=+].

//debe de estar estudiando
deber +=[modalpot=+].

//obligation
//debe estudiar
//tiene que estudiar
tener +=[modalpot=+].

//ha de estudiar, hay que estudiar
haber +=[modalpot=+].


//Aspectual:

//with "a" preposition + inf
//voy a 
ir +=[aspectpot=+].
 
//empiezo a 
empezar +=[aspectpot=+].
comenzar +=[aspectpot=+].

//torno a estudiar
tornar +=[aspectpot=+].

//vuelvo a estudiar
volver +=[aspectpot=+].

//acostumbro a estudiar
acostumbrar +=[aspectpot=+].

//alcanzar a
alcanzar +=[aspectpot=+].

//ponerse a, romper a, echarse a llorar
poner +=[aspectpot=+].
romper +=[aspectpot=+].
echar +=[aspectpot=+].

//llegar a 
llegar +=[aspectpot=+].

//pasar a
pasar +=[aspectpot=+].

//venir a
venir +=[aspectpot=+].

//With "de" preposition + inf

//estoy a punto de
//estar a punto +=[aspectpot=+]. 

//paro, ceso dejo de trabajar
parar +=[aspectpot=+].
cesar +=[aspectpot=+].
dejar +=[aspectpot=+].

//concluyo, acabo, termino de estudiar
concluir +=[aspectpot=+].
acabar +=[aspectpot=+].
terminar +=[aspectpot=+].

//With "en" preposition + inf
//dar en
dar +=[aspectpot=+].

//With no preposition + ger

//estoy estudiando
estar +=[aspectpot=+].

//sigo, continÃºo estudiando
seguir +=[aspectpot=+].
continuar +=[aspectpot=+].

//andar
andar +=[aspectpot=+].

//proseguir
proseguir +=[aspectpot=+].

//salir
salir +=[aspectpot=+].


//With no preposition + part

//tengo, llevo estudiados
tener +=[aspectpot=+].
llevar +=[aspectpot=+].
quedar +=[aspectpot=+].
resultar +=[aspectpot=+].

//With no preposition + inf

//suelo estudiar
soler +=[aspectpot=+].
osar +=[aspectpot=+].



//nouns that can have a prep+inf as modifier
//de
intención +=[de_inf=+].
sensación +=[de_inf=+].
impresión +=[de_inf=+].
placer +=[de_inf=+].
objetivo +=[de_inf=+].
