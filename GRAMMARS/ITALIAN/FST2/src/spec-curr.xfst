define CURRANY	CURRSYMB | CURRABBR | CURRCODE ;

read regex [
	CURRSYMB			%+Symbol:0 %+Curr:0
|	CURRABBR			%+Prop:0 %+Curr:0 %+Acron:0
|	CURRCODE			%+Prop:0 %+Curr:0 %+Acron:0

|	(SIGN) CURRANY AMOUNT 		%+Dig:0 %+Curr:0 	# -$100.00
|	(SIGN) AMOUNT (WS) CURRANY	%+Dig:0 %+Curr:0 	# -100.00$

|	(SIGN) CARDDOT CURRSYMB DIGIT+	%+Dig:0 %+Curr:0 	# 5€50 12.000€50
];




