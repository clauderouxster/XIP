DependencyRules:

if ( ^CONCEPT(#1) & TITLE(#2) )
   CONCEPT[impsent=+,vis=+](#1).

if ( ^CONCEPT(#1) & IMPSENT(#2) )
   CONCEPT[impsent=+,vis=+](#1).

if ( ^CONCEPT(#1) & IMPSUMMARY(#2) )
   CONCEPT[impsent=+,contribution=+,vis=+](#1).

if ( ^CONCEPT(#1) & SUMMARY(#2) )
   CONCEPT[impsent=+,contribution=+](#1).

if ( ^CONCEPT(#1,#3) & IMPSENT(#2) )
   CONCEPT[impsent=+,vis=+](#1,#3).

if ( ^CONCEPT(#1,#3) & IMPSUMMARY(#2) )
   CONCEPT[impsent=+,vis=+,contribution=+](#1,#3).

if ( ^CONCEPT(#1,#3) & SUMMARY(#2) )
   CONCEPT[impsent=+,contribution=+](#1,#3).

if ( ^CONCEPT(#1,#3,#4) & IMPSENT(#2) )
   CONCEPT[impsent=+,vis=+](#1,#3,#4).

if ( ^CONCEPT(#1,#3,#4) & IMPSUMMARY(#2) )
   CONCEPT[impsent=+,vis=+,contribution=+](#1,#3,#4).

if ( ^CONCEPT(#1,#3,#4) & SUMMARY(#2) )
   CONCEPT[impsent=+,contribution=+](#1,#3,#4).

if ( ^CONCEPT(#1,#3,#4,#5) & IMPSUMMARY(#2) )
   CONCEPT[impsent=+,vis=+,contribution=+](#1,#3,#4,#5).

if ( ^CONCEPT(#1,#3,#4,#5) & SUMMARY(#2) )
   CONCEPT[impsent=+,contribution=+](#1,#3,#4,#5).

