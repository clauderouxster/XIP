Features:
[
// pour les lieux
	loc-pr:{+},	// marque les dépendances COORDINATE et POTPOLEVENT qui concernent le lieu
	loc-pr2:{+},	// marque les dépendances COORDINATE et POTPOLEVENT qui concernent deux lieux
	tr_loc:{+},	// verbes dont l'objet direct peut être le lieu de l'action
	where:{+},	// marque les noms qui sont antécédents d'un where
	antec:{+},	// marque les where qui ont un antécédent 
	commonplace:{+},	// marque les noms communs qui renvoient à des lieux
	commoncity:{+},	// marque les noms communs qui désignent des villes
	commoncountry:{+},	// marque les noms communs qui désignent des pays
	building:{+},	// marque les noms propres qui sont des noms de bâtiments
	unit-loc:{+},	// marque les noms qui désignent des parties de lieu
	noplace:{+},	// marque les noms qui ne sont pas des noms de lieux (dans le contexte)
	mvt_to:{+},		// marque les verbes qui prennent un c. de lieu en 'to'
	supp:{carry_out, subj},	//signale quels verbes supports accompagnent quels noms d'événements
	preploc:{+},		// marque les prépositions qui peuvent régir des noms de lieux
	prepnoloc:{+},		// marque les prépositions qui ne peuvent pas régir des noms de lieux
	complex:{+},		// marque les adjectifs qui forment une expression avec le nom 'complex'
	loc_on:{+},		// marque les noms dont le complément introduit par 'on' peut être le lieu de l'événement
	firstprep:{+},		//une préposition en début de phrase

// autres
	phrase_have:{+},	// marque les noms qui forment une locution avec 'to have'
	adjmod:{+},		// les adjectifs modaux (possible, necessary)
	expr_inf:{+},		// caractérise la relation PREPD quand l'expression peut être suivie d'un infinitif
	deny:{+},		// les verbes qui ont la même construction que to deny (someone the right to do)
	sc_np_toinf1:{+},	// verbes dont l'objet est le sujet de l'infinitif qui suit (ask)
	grpr:{+},		// dépendances ajoutées à la grammaire par le fichier grammar_pr.xip

	cfactual:{+},		// verbes qui associés à un nom d'événement indiquent qu'il ne se produit pas
	chge:{fact-cfact, cfact-fact},	// verbes qui expriment un changement de factualité du nom auxquel ils sont associés
	noevnt:{+},		// noms qui ne sont pas des événements
	src:{+},
	annex:{+},		// marque les emboîtements de type ANNEX

	target:{+},
	actor:{+},
	information:{+},
	topic:{+},
	factual:{+},
	counter-factual:{+},
	possible:{+},

	other:{+},
		teest:{+}


]
