Vocabulary:

\] : punct += [square=+].
\[ : punct += [square=+].

// not plural nouns but singular verbs
express : ? += [nonounpl=+]. 
//say : ? += [nonounpl=+]. 
stress : ? += [nonounpl=+]. 
want : ? += [nonounpl=+].
welcome : ? += [nonounpl=+].

//these words are not likely to be nouns when preceded by an entity
claim : ? += [preentverb=+].
need : ? += [preentverb=+].
state : ? += [preentverb=+].


military : ? += [danger=+].


//modal adjectives
impossible : adj += [modaladj=+].
likely : adj += [modaladj=+].
necessary : nadj += [modaladj=+].
optional : adj += [modaladj=+].
possible : adj += [modaladj=+].
probable : adj += [modaladj=+].
unlikely : adj += [modaladj=+].
improbable : adj += [modaladj=+].

//modal adverbs
certainly : adv += [modaladv=+].
likely : adv += [modaladv=+].
maybe : adv += [modaladv=+].
possibly : adv += [modaladv=+].
probably : adv += [modaladv=+].
reportedly : adv += [modaladv=+].
surely : adv += [modaladv=+].

//nouns that express modality when used as predicates
option : noun +=[modalnoun=+].
possibility : noun +=[modalnoun=+].

//conjonctions
although : conj += [annex=+, logic=+].
because : conj += [annex=+, logic=+].
since : conj += [annex=+, logic=+, temp=+].
after : conj += [annex=+, temp=+].
if : conj += [annex=+].


