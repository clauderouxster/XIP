// This file contains features dedicated to extract entities like 'Author', 'Recipient', 'Copyee', 'Document date' and 'Other dates'



Features:
[features_GM: 
  [
	exp:{+},		// for the author of a document
	exp_pot:{+},		// for the site names right before an author
	recipient:{+},		// for the recipient of a document
	recipient_pot:{+},		// for the recipient of a document
	copyee:{+},		// for the copyees of a document
	doc_date:{+},		// for the date of a document
	non_doc_date:{+},		// when it's not possible to be a doc_date (even if it's the first date found in the document)
	other_date:{+},		// for other dates of a document
	first_date:{+}
	



 ]
]  

